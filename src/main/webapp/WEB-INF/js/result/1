/**
 * the unique global variable for Meituan JS Framework
 * @class M
 * @static
 */
var M = window.M || {};

if (typeof Date.now !== "function") {
    Date.now = function () {
        return (new Date()).getTime();
    };
}

/**
 * 模块组元信息
 * @property Groups
 * @static
 * @type {Object}
 */
M.Groups = M.Groups || {};

/**
 * 添加模块组元信息
 * @method addGroup
 * @static
 * @param {String} name 组名
 * @param {Object} detail 组元信息
 * @example
 *      M.addGroup("www", {
 *          "root":"fecore/",
 *          "combine":true,
 *          "modules":{
 *              "mt-addresschecker":{
 *                  "requires":["mt-base"],
 *                  "path":"mt/js/addresschecker-min.veda81a1f.js"
 *              }
 *          }
 *      });
 */
M.addGroup = function (name, detail) {
    if (M.DEV === 2) {
        detail.combine = false;
        detail.base = M.COMBO_BASE + detail.root;
        delete detail.root;
    }
    M.Groups[name] = detail;
};

M.Env = M.Env || {};
M.Env._beacon = M.Env._beacon || {};
M.config = M.config || {};

// 数据统计
(function () {
    // 日志发送机制配置
    M.config['mt-beacon'] = {
        domainHost: M.DOMAIN_HOST,
        cdnDomainHost: M.CDN_DOMAIN_HOST,
        data: function () {
            var r = document.referrer || M.HTTP_REFERER,
                o = {
                    time: Date.now() + M.DELTA_TIME || 0
                };
            if (r) {
                o.r = r;
            }

            if (M.PAGE_VIEW_DATA && M.PAGE_VIEW_DATA.page) {
                o.page = M.PAGE_VIEW_DATA.page;
            }

            return o;
        },
        blog: {
            data: { url: M.PAGE_VIEW }
        }
    };
    M.config['mt-log'] = {
        pageview: {
            data: function () {
                M.TimeTracker = M.TimeTracker || {};
                var extra = {
                    rt: M.TimeTracker.reqtime || 0,
                    st: M.TimeTracker.start || 0,
                    dt: M.TimeTracker.domReady || 0,
                    lt: M.TimeTracker.load || 0,
                    fst: M.TimeTracker.fst || 0,
                    sc: window.screen.width + 'x' + window.screen.height
                };
                if (M.PAGE_VIEW_DATA) {
                    mix(extra, M.PAGE_VIEW_DATA);
                }

                return extra;
            }
        },
        error: {
            disable: M.LOG_DISABLE
        }
    };

    /**
     * 收集js错误日志
     * @method log
     * @static
     * @param {String} [level=fatal] 错误等级，分为'fatal','error','warn','info'
     * @param {String} msg 错误信息
     * @param {Object} [extra] 其它内容
     */
    M.log = (function () {
        var lastLevel = '',
            lastMsg = '',
            count = 0,
            maxCount = 10,
            timer = null,
            time = 500;

        function log(level, msg, extra) {
            var info = {
                type: 'jsdebug',
                level: level || 'fatal',
                msg: msg
            };
            if (extra) {
                mix(info, extra);
            }
            if (M.PAGE_VIEW_DATA && M.PAGE_VIEW_DATA.page) {
                info.page = M.PAGE_VIEW_DATA.page;
            }
            if (!M.LOG_DISABLE) {
                beacon(info);
            }
        }

        // 将对象o转化为query string
        function toQueryString(o) {
            var prop, a = [];
            for (prop in o) {
                if (o.hasOwnProperty(prop)) {
                    a.push(encodeURIComponent(prop) + '=' + encodeURIComponent(o[prop]));
                }
            }
            return a.join('&');
        }

        // 向js日志收集服务器发送beacon请求
        // 占用key：time:时间, c:Cookie, r:Referer
        function beacon(o, type) {
            var ts = (new Date()).getTime(),
                b = M.Env._beacon[ts] = new Image();
            var protocol = window.location.protocol,
                uri = protocol === 'https:' ? protocol + '//felog.' + M.DOMAIN_HOST + '/_.gif'
                                            : protocol + '//fe.' + M.CDN_DOMAIN_HOST + '/_.gif';

            mix(o, {
                time: ts + (M.DELTA_TIME || 0)
            });
            if (M.HTTP_REFERER) {
                o.r = M.HTTP_REFERER;
            }

            b.src = uri + '?' + toQueryString(o);
        }

        return function (level, msg, extra) {
            if (level !== lastLevel || msg !== lastMsg) {
                window.setTimeout(function () {
                    log(level, msg, extra);
                }, time);
                lastLevel = level;
                lastMsg = msg;
                count = 0;
            } else {
                if (timer || count++ >= maxCount) return;
                timer = window.setTimeout(function () {
                    log(level, msg, extra);
                    timer = null;
                }, time);
            }
        };
    })();

    // 将s对象中r没有的属性复制给r
    function mix(r, s, overwrite) {
        if (!s) return;
        for (var sprop in s) {
            if (s.hasOwnProperty(sprop) && (overwrite || typeof r[sprop] === 'undefined')) {
                r[sprop] = s[sprop];
            }
        }
    }

    M.config['w-slider'] = {
        cssLazyClass: 'lazy-img'
    };
})();

(function () {
    var log = function (Y, level, msg, extra) {
        if (Y.mt && Y.mt.log && Y.mt.log.error) {
            Y.mt.log.error(level, msg, extra);
        } else {
            M.log(level, msg, extra);
            if (!Y.Env._logLoaded) {
                Y.use('mt-log');
                Y.Env._logLoaded = true;
            }
        }
    };
    var YUI = window.YUI,
        YUIConfig = window['YUI_config'],
        isDebug = M.DEBUG || (YUIConfig && YUIConfig.debug) || M.DEV,
        config,
        prop;
    if (YUI) {
        // yui config info
        if (M.DEV === 2) {
            config = {
                base: M.COMBO_BASE + 'yui/' + (M.YUI_VERSION || '3.3.0') + '/build/',
                maxURLLength: 1500,
                combine: false,
                groups: M.Groups,
                modules: M.Groups.site || {}
            };
        } else {
            config = {
                debug: false,
                root: 'yui/' + (M.YUI_VERSION || '3.3.0') + '/build/',
                maxURLLength: 1500,
                combine: true,
                comboBase: M.COMBO_BASE || "",
                comboSep: M.IS_NEW_COMBO ? ';' : '&',
                groups: M.Groups,
                modules: M.Groups.site || {}
            };
        }
        if (isDebug) {
            config.debug = true;
            config.filter = {
                searchExp: '-min(\\.v[0-9a-zA-Z]{8})?\\.js',
                replaceStr: '$1.js'
            };
        } else {
            config.throwFail = false;
            config.errorFn = function (msg, e, module) {
                // 把错误收集到一个数组里方面后续处理
                M._ERRORS =  M._ERRORS || [];
                M._ERRORS.push(e);
                var Y = this,
                    stackInfo = e.stack && e.stack.toString(),
                    pattern = /\(.*?\)/g,
                    o = { src: 'catch', module: module };
                if (stackInfo) {
                    o.stack = stackInfo.replace(pattern, "");
                }
                log(Y, 'fatal', e.message, o);
                return true;
            };
            config.loadErrorFn = function (Y, useCb, loaderResponse, args) {
                log(Y, 'fatal', loaderResponse.msg, { src: 'loadError', module: args[args.length - 1]});
            };
            // 监听错误信息, 覆盖之前的window.onerror方法
            window.onerror = function (msg, url, line) {
                M.log('fatal', msg, { src: 'window', url: url || window.location.href, line: line });
                return true;
            };
        }

        config['mt-log'] = M.config['mt-log'];
        config['mt-beacon'] = M.config['mt-beacon'];
        config['mt-scrollloader'] = M.config['mt-scrollloader'];
        config['w-slider'] = M.config['w-slider'];
        if (YUIConfig) {
            for (prop in config) {
                if (!YUIConfig.hasOwnProperty(prop) && config.hasOwnProperty(prop)) {
                    YUIConfig[prop] = config[prop];
                }
            }
        } else {
            window['YUI_config'] = config;
        }

        /**
         * YUI对象
         * @property YUI
         * @static
         * @type {Function}
         */
        M._YUI = YUI;
        /**
         * YUI instance cache
         * @property instances
         * @static
         * @type {Array}
         */
        M.instances = [];

        /**
         * page data stash, using for interaction with page which has not yui3 environment
         * @deprecated 请避免在全局空间下存储数据
         * @property appStashData
         * @static
         * @type {Object}
         */
        M.appStashData = {};
        /**
         * 定义模块，YUI.add的别名
         * @method add
         * @static
         * @param {String} name 模块名称
         * @param {Function} fn 定义函数
         * @param {String} version 版本号
         * @param {Object} detail 元信息
         */
        M.add = YUI.add;
        /**
         * 调用模块，YUI.use的别名
         * @method use
         * @static
         * @param {String} [name]* 模块名称
         * @param {Function} fn 回调函数
         */
        M.use = function () {
            var instance,
                g, m, group, module;

            // hack Loader bug
            // @link https://github.com/yui/yui3/issues/639
            if (YUI.version >= '3.8.0') {
                for (g in config.groups) {
                    if (config.groups.hasOwnProperty(g)) {
                        group = config.groups[g];
                        for (m in group.modules) {
                            if (group.modules.hasOwnProperty(m)) {
                                module = group.modules[m];
                                if (module.langCache) delete module.langCache;
                                if (module.skinCache) delete module.skinCache;
                            }
                        }
                    }
                }
            }

            // get the configured YUI instance
            instance = YUI();
            M.instances.push(instance);
            instance.use.apply(instance, arguments);
            return this;
        };
    }
}());
 

;/*jshint white:true*/
/*global M:true*/

/**
 * 二次开发基础模块
 * @module mt-base
 */
M.add('mt-base', function (Y) {
    var $N = Y.Node;
    var isStr = Y.Lang.isString;
    var isArray = Y.Lang.isArray;
    var isObject = Y.Lang.isObject;
    var isFunction = Y.Lang.isFunction;
    var trim = Y.Lang.trim;

    Y.namespace('mt.lang');
    /**
     * 语言层面的扩展
     * @class lang
     * @namespace mt
     * @static
     */
    Y.mt.lang = {
        /**
         * 时间格式化
         * [A JavaScript equivalent of PHP’s date](http://phpjs.org/functions/date:380)
         * [详细文档](http://php.net/manual/en/function.date.php)
         * @method date
         * @static
         * @param {String} format 时间格式，如Y-m-d H:i:s
         * @param {Date|String|Integer} [timestamp] 时间戳 注意：如果是整数，时间单位为秒
         * @example
         *      var time = Y.mt.lang.date('Y-m-d H:i');
         */
        date: function (format, timestamp) {
            var that = this,
                jsdate, f, formatChr = /\\?([a-z])/gi, formatChrCb,
                // Keep this here (works, but for code commented-out
                // below for file size reasons)
                //, tal= [],
                _pad = function (n, c) {
                    if ((n = n + "").length < c) {
                        return new Array((++c) - n.length).join("0") + n;
                    } else {
                        return n;
                    }
                },
                txtWords = ["Sun", "Mon", "Tues", "Wednes", "Thurs", "Fri", "Satur",
                "January", "February", "March", "April", "May", "June", "July",
                "August", "September", "October", "November", "December"],
                txtOrdin = {
                    1: "st",
                    2: "nd",
                    3: "rd",
                    21: "st",
                    22: "nd",
                    23: "rd",
                    31: "st"
                };
            formatChrCb = function (t, s) {
                return f[t] ? f[t]() : s;
            };
            f = {
            // Day
                // 返回月中的Date | Day of month w/leading 0; 01..31
                d: function () {
                    return _pad(f.j(), 2);
                },
                // Shorthand day name; Mon...Sun
                D: function () {
                    return f.l().slice(0, 3);
                },
                // Day of month; 1..31
                j: function () {
                    return jsdate.getDate();
                },
                // Full day name; Monday...Sunday
                l: function () {
                    return txtWords[f.w()] + 'day';
                },
                // ISO-8601 day of week; 1[Mon]..7[Sun]
                N: function () {
                    return f.w() || 7;
                },
                // Ordinal suffix for day of month; st, nd, rd, th
                S: function () {
                    return txtOrdin[f.j()] || 'th';
                },
                // Day of week; 0[Sun]..6[Sat]
                w: function () { //
                    return jsdate.getDay();
                },
                // Day of year; 0..365
                z: function () { //
                    var a = new Date(f.Y(), f.n() - 1, f.j()),
                        b = new Date(f.Y(), 0, 1);
                    return Math.round((a - b) / 864e5) + 1;
                },
                // ISO-8601 week number
                W: function () {
                    var a = new Date(f.Y(), f.n() - 1, f.j() - f.N() + 3),
                        b = new Date(a.getFullYear(), 0, 4);
                    return 1 + Math.round((a - b) / 864e5 / 7);
                },

            // Month
                // Full month name; January...December
                F: function () {
                    return txtWords[6 + f.n()];
                },
                // Month w/leading 0; 01...12
                m: function () {
                    return _pad(f.n(), 2);
                },
                // Shorthand month name; Jan...Dec
                M: function () {
                    return f.F().slice(0, 3);
                },
                // Month; 1...12
                n: function () {
                    return jsdate.getMonth() + 1;
                },
                // Days in month; 28...31
                t: function () {
                    return (new Date(f.Y(), f.n(), 0)).getDate();
                },

            // Year
                // Is leap year?; 0 or 1
                L: function () {
                    return (new Date(f.Y(), 1, 29).getMonth()) === 1 ? 1 : 0;
                },
                // ISO-8601 year
                o: function () {
                    var n = f.n(), W = f.W(), Y = f.Y();
                    return Y + (n === 12 && W < 9 ? -1 : n === 1 && W > 9);
                },
                // Full year; e.g. 1980...2010
                Y: function () {
                    return jsdate.getFullYear();
                },
                // Last two digits of year; 00...99
                y: function () {
                    return (f.Y() + "").slice(-2);
                },

            // Time
                // am or pm
                a: function () {
                    return jsdate.getHours() > 11 ? "pm" : "am";
                },
                // AM or PM
                A: function () {
                    return f.a().toUpperCase();
                },
                // Swatch Internet time; 000..999
                B: function () {
                    var H = jsdate.getUTCHours() * 36e2, // Hours
                        i = jsdate.getUTCMinutes() * 60, // Minutes
                        s = jsdate.getUTCSeconds(); // Seconds
                    return _pad(Math.floor((H + i + s + 36e2) / 86.4) % 1e3, 3);
                },
                // 12-Hours; 1..12
                g: function () {
                    return f.G() % 12 || 12;
                },
                // 24-Hours; 0..23
                G: function () {
                    return jsdate.getHours();
                },
                // 12-Hours w/leading 0; 01..12
                h: function () {
                    return _pad(f.g(), 2);
                },
                // 24-Hours w/leading 0; 00..23
                H: function () {
                    return _pad(f.G(), 2);
                },
                // Minutes w/leading 0; 00..59
                i: function () {
                    return _pad(jsdate.getMinutes(), 2);
                },
                // Seconds w/leading 0; 00..59
                s: function () {
                    return _pad(jsdate.getSeconds(), 2);
                },
                // Microseconds; 000000-999000
                u: function () {
                    return _pad(jsdate.getMilliseconds() * 1000, 6);
                },

            // Timezone
                // Timezone identifier; e.g. Atlantic/Azores, ...
                e: function () {
                    throw 'Not supported (see source code of date() for timezone on how to add support)';
                },
                // DST observed?; 0 or 1
                // Compares Jan 1 minus Jan 1 UTC to Jul 1 minus Jul 1 UTC.
                // If they are not equal, then DST is observed.
                I: function () {
                    var a = new Date(f.Y(), 0), // Jan 1
                        c = Date.UTC(f.Y(), 0), // Jan 1 UTC
                        b = new Date(f.Y(), 6), // Jul 1
                        d = Date.UTC(f.Y(), 6); // Jul 1 UTC
                    return 0 + ((a - c) !== (b - d));
                },
                // Difference to GMT in hour format; e.g. +0200
                O: function () {
                    var a = jsdate.getTimezoneOffset();
                    return (a > 0 ? "-" : "+") + _pad(Math.abs(a / 60 * 100), 4);
                },
                // Difference to GMT w/colon; e.g. +02:00
                P: function () {
                    var O = f.O();
                    return (O.substr(0, 3) + ":" + O.substr(3, 2));
                },
                // Timezone abbreviation; e.g. EST, MDT, ...
                T: function () {
                    return 'UTC';
                },
                // Timezone offset in seconds (-43200...50400)
                Z: function () {
                    return -jsdate.getTimezoneOffset() * 60;
                },

            // Full Date/Time
                // ISO-8601 date.
                c: function () {
                    return 'Y-m-d\\Th:i:sP'.replace(formatChr, formatChrCb);
                },
                // RFC 2822
                r: function () {
                    return 'D, d M Y H:i:s O'.replace(formatChr, formatChrCb);
                },
                // Seconds since UNIX epoch
                U: function () {
                    return parseInt(jsdate.getTime() / 1000, 10);
                }
            };
            /**
             * 格式化日期
             * @method date
             * @param {String} format 指定格式
             * @param {Number} [timestamp] 日期
             * @return {String|Number} 指定格式的日期
             */
            this.date = function (format, timestamp) {
                that = this;
                // 修改双重三元表达式为分支判断以通过jslint检查
                if (typeof timestamp === 'undefined') {
                    jsdate = new Date();
                } else {
                    jsdate = (timestamp instanceof Date) ? new Date(timestamp) : new Date(timestamp * 1000);
                }
                return format.replace(formatChr, formatChrCb);
            };
            return this.date(format, timestamp);
        },
        /**
         * 由字符串生成UNIX时间戳
         * [A JavaScript equivalent of PHP’s strtotime](http://phpjs.org/functions/strtotime:554)
         * [详细文档](http://php.net/manual/en/function.strtotime.php)
         * @method strtotime
         * @static
         * @param {String} str 时间字符串
         * @param {Integer} now 计算相对时间时使用的时间戳，默认为当前时间，注意：时间单位为秒
         * @return {Number} 时间戳，单位s
         */
        strtotime: function (str, now) {
            /*jshint maxcomplexity:30*/
            var i, match, s, strTmp = '', parse = '';

            strTmp = trim(str + '');
            strTmp = strTmp.replace(/\s{2,}|^\s|\s$/g, ' '); // unecessary spaces
            strTmp = strTmp.replace(/[\t\r\n]/g, ''); // unecessary chars

            if (strTmp === 'now') {
                return (new Date()).getTime() / 1000; // Return seconds, not milli-seconds
            } else if (!isNaN(parse = Date.parse(strTmp))) {
                return (parse / 1000);
            } else if (now) {
                now = new Date(now * 1000); // Accept PHP-style seconds
            } else {
                now = new Date();
            }

            strTmp = strTmp.toLowerCase();

            var __is =
            {
                day:
                {
                    'sun': 0,
                    'mon': 1,
                    'tue': 2,
                    'wed': 3,
                    'thu': 4,
                    'fri': 5,
                    'sat': 6
                },
                mon:
                {
                    'jan': 0,
                    'feb': 1,
                    'mar': 2,
                    'apr': 3,
                    'may': 4,
                    'jun': 5,
                    'jul': 6,
                    'aug': 7,
                    'sep': 8,
                    'oct': 9,
                    'nov': 10,
                    'dec': 11
                }
            };

            var process = function (m) {
                var ago = (m[2] && m[2] === 'ago');
                var num = (num = m[0] === 'last' ? -1 : 1) * (ago ? -1 : 1);

                switch (m[0]) {
                case 'last':
                case 'next':
                    switch (m[1].substring(0, 3)) {
                    case 'yea':
                        now.setFullYear(now.getFullYear() + num);
                        break;
                    case 'mon':
                        now.setMonth(now.getMonth() + num);
                        break;
                    case 'wee':
                        now.setDate(now.getDate() + (num * 7));
                        break;
                    case 'day':
                        now.setDate(now.getDate() + num);
                        break;
                    case 'hou':
                        now.setHours(now.getHours() + num);
                        break;
                    case 'min':
                        now.setMinutes(now.getMinutes() + num);
                        break;
                    case 'sec':
                        now.setSeconds(now.getSeconds() + num);
                        break;
                    default:
                        var day;
                        if (typeof (day = __is.day[m[1].substring(0, 3)]) !== 'undefined') {
                            var diff = day - now.getDay();
                            if (diff === 0) {
                                diff = 7 * num;
                            } else if (diff > 0) {
                                if (m[0] === 'last') diff -= 7;
                            } else {
                                if (m[0] === 'next') diff += 7;
                            }
                            now.setDate(now.getDate() + diff);
                        }
                        break;
                    }
                    break;

                default:
                    if (/\d+/.test(m[0])) {
                        num *= parseInt(m[0], 10);

                        switch (m[1].substring(0, 3)) {
                        case 'yea':
                            now.setFullYear(now.getFullYear() + num);
                            break;
                        case 'mon':
                            now.setMonth(now.getMonth() + num);
                            break;
                        case 'wee':
                            now.setDate(now.getDate() + (num * 7));
                            break;
                        case 'day':
                            now.setDate(now.getDate() + num);
                            break;
                        case 'hou':
                            now.setHours(now.getHours() + num);
                            break;
                        case 'min':
                            now.setMinutes(now.getMinutes() + num);
                            break;
                        case 'sec':
                            now.setSeconds(now.getSeconds() + num);
                            break;
                        default:
                            now.setSeconds(now.getSeconds() + num);
                            break;
                        }
                    } else {
                        return false;
                    }
                    break;
                }
                return true;
            };

            // match = strTmp.match(/^(\d{2,4}-\d{2}-\d{2})(?:\s(\d{1,2}:\d{2}(:\d{2})?)?(?:\.(\d+))?)?$/);
            // 修复无前缀0的错误，例如2010-9-1可以识别为2010-09-01
            match = strTmp.match(/^(\d{2,4}-\d{1,2}-\d{1,2})(?:\s(\d{1,2}:\d{1,2}(:\d{1,2})?)?(?:\.(\d+))?)?$/);

            if (match !== null) {
                if (!match[2]) {
                    match[2] = '00:00:00';
                } else if (!match[3]) {
                    match[2] += ':00';
                }

                s = match[1].split(/-/g);

                for (i in __is.mon) {
                    if (__is.mon[i] === s[1] - 1) {
                        s[1] = i;
                    }
                }
                s[0] = parseInt(s[0], 10);

                s[0] = (s[0] >= 0 && s[0] <= 69) ? '20' + (s[0] < 10 ? '0' + s[0] : s[0] + '') : (s[0] >= 70 && s[0] <= 99) ? '19' + s[0] : s[0] + '';
                return parseInt(Y.mt.lang.strtotime(s[2] + ' ' + s[1] + ' ' + s[0] + ' ' + match[2]) + (match[4] ? match[4] / 1000 : ''), 10);
            }

            var regex = '([+-]?\\d+\\s' +
                '(years?|months?|weeks?|days?|hours?|min|minutes?|sec|seconds?' +
                '|sun\\.?|sunday|mon\\.?|monday|tue\\.?|tuesday|wed\\.?|wednesday' +
                '|thu\\.?|thursday|fri\\.?|friday|sat\\.?|saturday)' +
                '|(last|next)\\s' +
                '(years?|months?|weeks?|days?|hours?|min|minutes?|sec|seconds?' +
                '|sun\\.?|sunday|mon\\.?|monday|tue\\.?|tuesday|wed\\.?|wednesday' +
                '|thu\\.?|thursday|fri\\.?|friday|sat\\.?|saturday))' +
                '(\\sago)?';

            match = strTmp.match(new RegExp(regex, 'gi')); // Brett: seems should be case insensitive per docs, so added 'i'
            if (match === null) {
                return false;
            }

            for (i = 0; i < match.length; i++) {
                if (!process(match[i].split(' '))) {
                    return false;
                }
            }

            return (now.getTime() / 1000);
        },
        /**
         * 获取日期对应周几，返回周一
         * @method getReadableWeek
         * @param {Date|String|Integer} 时间戳
         * @return {String} '周一','周日'
         */
        getReadableWeek: function (timestamp) {
            var weekDay = Y.mt.lang.date('w', timestamp),
                weekPattern = ['日', '一', '二', '三', '四', '五', '六'],
                slug = weekPattern[weekDay];
            return '周' + slug;
        },
        /**
         * 计算字符串长度，中文算两字符
         * @method getLength
         * @static
         * @param {String} str 字符串
         * @return {Number} 字符串长度
         */
        getLength: function (str) {
            if (!str || !isStr(str)) return 0;
            for (var i = 0, count = 0, len = str.length ; i < len; i++) {
                count = str.charCodeAt(i) > 255 ? count + 2 : count + 1;
            }
            return count;
        },
        /**
         * 截断字符串，中文算两字符
         * @method truncateStr
         * @static
         * @param {String} str 字符串
         * @param {Integer} len 长度
         * @param {String} [suffix=''] 截断后缀
         * @return {String} 截断后的字符串
         */
        truncateStr: function (str, len, suffix) {
            var i,
                realLen = 0,
                needSuffix = true,
                charCode,
                result = str;

            suffix = suffix || '';


            // 计算需要显示的实际长度
            for (i = 0; realLen <= len; i++) {
                charCode = str.charCodeAt(i);
                // 字符串长度小于限制长度 则不需要后缀
                if (isNaN(charCode)) {
                    needSuffix = false;
                    break;
                } else if (charCode > 255) {
                    // ASCII 大于255 的为中文字符,中文算两个字符
                    realLen += 2;
                } else {
                    realLen += 1;
                }
            }

            // 如果最后一次相加超出了len则 i-1
            if (realLen > len) {
                i--;
            }

            if (needSuffix) {
                result = str.substring(0, i) + suffix;
            }
            return result;
        },
        /**
         * 字符半角转全角
         * 全角空格为12288，半角空格为32
         * 其他字符全角(65281-65374)减去半角(33-126)等于65248
         * @method toFull
         * @static
         * @param {String} str 字符串
         * @return {String} 转换为全角的字符串
         */
        toFull: function (str) {
            var ret = '';
            for (var i = 0, len = str.length; i < len; i++) {
                var charCode = str.charCodeAt(i);
                if (charCode === 32) {
                    ret += String.fromCharCode(12288);
                } else if (charCode < 127) {
                    ret += String.fromCharCode(charCode + 65248);
                } else {
                    ret += str[i];
                }
            }
            return ret;
        },
        /**
         * 字符全角转半角
         * 全角空格为12288，半角空格为32
         * 其他字符全角(65281-65374)减去半角(33-126)等于65248
         * @method toHalf
         * @static
         * @param {String} str 字符串
         * @return {String} 转换为半角的字符串
         */
        toHalf: function (str) {
            var ret = '';
            for (var i = 0, len = str.length; i < len; i++) {
                var charCode = str.charCodeAt(i);
                if (charCode === 12288) {
                    ret += String.fromCharCode(32);
                } else if (charCode > 65280 && charCode < 65375) {
                    ret += String.fromCharCode(charCode - 65248);
                } else {
                    ret += String.fromCharCode(charCode);
                }
            }
            return ret;
        },
        /**
         * 从字符串转换成数组，相同键值将覆盖
         * @method parseStr
         * @static
         * @param {String} str 要转换的字符窜 token=abcdef&action=msg.del
         * @param {Boolean} [isCover=true] 如有相同键是否后者覆盖前者, 默认覆盖
         * @param {String} [joinKey='&'] join时候的字符串
         * @param {String} multipleJoin  如果有多个key相同，将其按照给定的key进行join
         * @return {Object} 转换得到的对象
         */
        parseStr: function (str, isCover, joinKey, multipleJoin, decode) {
            if (!str || !isStr(str)) return null;

            if (typeof isCover === 'undefined') isCover = true;
            if (typeof decode === 'undefined') decode = true;
            joinKey = joinKey || '&';

            var arr = str.split(joinKey),
                ret = {},
                i, len,
                pair, k, v;
            for (i = 0, len = arr.length; i < len; i++) {
                pair = arr[i].split('=');
                //pair[1]中含有"="的情况
                if (pair.length > 2) {
                    pair[1] = pair.slice(1).join('=');
                }
                k = decode ? decodeURIComponent(pair[0]) : pair[0];
                v = decode ? decodeURIComponent(pair[1]) : pair[1];
                if (isCover) {
                    ret[k] = v;
                } else {
                    if (ret.hasOwnProperty(k)) {
                        if (!isArray(ret[k])) ret[k] = [ret[k]];
                        ret[k].push(v);
                        if (multipleJoin) ret[k] = ret[k].join(multipleJoin);
                    } else {
                        ret[k] = v;
                    }
                }
            }
            return ret;
        },
        /**
         * 填补数字的空白
         * @method pad
         * @static
         * @param {Number} num 需要填补空白的数字
         * @param {Number} count 需要凑齐的位数
         * @param {String} [letter='0'] 填补的字母
         */
        pad: function (num, count, letter) {
            letter = letter || '0';
            var len = num.toString().length;
            while (len < count) {
                num = letter + num;
                len++;
            }
            return num;
        },
        /**
         * 解析服务器返回的UTC时间
         * @method parseDate
         * @static
         * @param {String} strTime 格式如Wed Jun 13 10:11:18 +0000 2007
         * @return {Number} 时间戳
         */
        parseDate: function (strTime) {
            var tmp = strTime.split(' ');
            var stdFormat = tmp[1] + " " + tmp[2] + ", " + tmp[5] + " " + tmp[3];
            return Date.parse(stdFormat) - (new Date()).getTimezoneOffset() * 60 * 1000;
        },
        /**
         * 获取服务器相对时间
         * @method getRelativeTime
         * @static
         * @param {String|Date} strTime 格式如Wed Jun 13 10:11:18 +0000 2007 或者Date对象
         * @return {String} 服务器相对时间
         */
        getRelativeTime: function (strTime) {
            var time = isStr(strTime) ? this.parseDate(strTime).valueOf() : strTime;
            var now = new Date();
            var seconds = parseInt((now.getTime() - time) / 1000, 10);
            if (seconds < 1) {
                return '1 秒前';
            } else if (seconds < 60) {
                return seconds + ' 秒前';
            } else if (seconds < 60 * 60) {
                return parseInt(seconds / 60, 10) + ' 分钟前';
            } else if (seconds < 60 * 60 * 24) {
                return '约 ' + parseInt(seconds / 3600, 10) + ' 小时前';
            } else {
                return this.getFullTime(strTime);
            }
        },
        /**
         * 获取服务器完整时间
         * @method getFullTime
         * @static
         * @param {String|Date} strTime 格式如Wed Jun 13 10:11:18 +0000 2007 或者Date对象
         * @return {String} 服务器完整时间，如2008-11-03
         */
        getFullTime: function (strTime) {
            var d = new Date(isStr(strTime) ? this.parseDate(strTime) : strTime);
            var year = d.getFullYear();
            var month = this.pad(d.getMonth() + 1, 2);
            var date = this.pad(d.getDate(), 2);
            var hour = this.pad(d.getHours(), 2);
            var minute = this.pad(d.getMinutes(), 2);
            return year + '-' + month + '-' + date + ' ' + hour + ':' + minute;
        },
        /**
         * 检查needle是否在数组中
         * [A JavaScript equivalent of PHP’s inArray](http://phpjs.org/functions/in_array:432)
         * @method inArray
         * @static
         * @param {Mixed} needle 需要查找的项
         * @param {Array} haystack 数组
         * @param {Boolean} argStrict 是否是严格匹配
         * @return {Boolean} 是否在数组中
         */
        inArray: function (needle, haystack, argStrict) {
            /*jshint eqeqeq:false*/
            var key = '', strict = !!argStrict;
            if (strict) {
                for (key in haystack) {
                    if (haystack[key] === needle) {
                        return true;
                    }
                }
            } else {
                for (key in haystack) {
                    if (haystack[key] == needle) {
                        return true;
                    }
                }
            }
            return false;
        },
        /**
         * 字符串第一个字符大写
         * @method ucfirst
         * @static
         * @param {String} str 字符串
         * @return {String} 结果字符串
         */
        ucfirst: function (str) {
            str += '';
            return str.charAt(0).toUpperCase() + str.substr(1);
        },
        /**
         * 将array-like对象转换为real array
         * @method toArray
         * @static
         * @param {Object} o array-like object
         * @return {Array} 数组
         */
        toArray: function (o) {
            var arr = null;
            try {
                // 非ie浏览器
                arr = Array.prototype.slice.call(o, 0);
            } catch (ex) {
                arr = [];
                for (var i = 0, len = o.length; i < len; ++i) {
                    arr.push(o[i]);
                }
            }
            return arr;
        },
        /**
         * 获得[min, max]区间的值
         * [参考文档](http://code.google.com/closure/library/docs/introduction.html#names)
         * @method clamp
         * @static
         * @param {Number} value 参考值
         * @param {Number} min 最小值
         * @param {Number} max 最大值
         * @return {Number} 结果值
         * @example
         *      Y.mt.lang.clamp(5, 2, 3); // output 3
         *      Y.mt.lang.clamp(5, 2, 9); // output 5
         */
        clamp: function (value, min, max) {
            return Math.min(max, Math.max(min, value));
        },
        /**
         * 对象是否存在
         * @method isExist
         * @static
         * @param {Object} o 待检查对象
         * @return {Boolean} 对象是否存在
         */
        isExist: function (o) {
            return o !== null && typeof o !== 'undefined';
        },
        /**
         * 判断是否为数值
         * @method isNumeric
         * @static
         * @param {*} o 待检查对象
         * @return {Boolean}
         */
        isNumeric: function (o) {
            return this.isExist(o) && (/\d/).test(o) && !isNaN(o);
        },
        /*
         * 替换html实体字符
         */
        htmlEscape: function (text) {
            return text.replace(/[<>"&]/g/*" to fool the syntax highlight*/, function (match) {
                switch (match) {
                case '<':
                    return "&lt;";
                case '>':
                    return "&gt;";
                case '&':
                    return "&amp;";
                case '"':
                    return "&quot;";
                }
            });
        }
    };

    Y.namespace('mt.util');
    /**
     * 工具函数
     * @class util
     * @namespace mt
     * @static
     */
    Y.mt.util = {
        /**
         * 获取指定对象上属性值，支持多级属性名称解析。
         * @method getProperty
         * @param {Object} o 属性宿主
         * @param {String} prop 属性名称
         * @param {Boolean} [notDeep=false] 为false时将开启多级属性解析。
         * @return {Mixed} 返回的属性值。undefined表示属性不存在
         */
        getProperty: function (o, prop, notDeep) {
            var keys,
                k;
            if (!o || !prop) {
                return undefined;
            }
            if (notDeep || prop.indexOf('.') === -1) {
                return o[prop];
            }
            keys = prop.split('.');
            while (keys.length) {
                k = keys.shift();
                if (Y.Lang.isUndefined(o[k]) || Y.Lang.isNull(o[k])) {
                    return o[k];
                } else {
                    o = o[k];
                }
            }
            return o;
        },
        /**
         * 从指定对象上获取多个属性值
         * @method getProperties
         * @param {Object} o 属性宿主
         * @param {Array} props 指定的属性名称
         * @return {Object} 返回的属性集合。注意：所有指定属性在hasOwnProperty时都会为true，但若该属性不存在于宿主上，其值为undefined
         *
         */
        getProperties: function (o, props) {
            var ret = {},
                _ = Y.mt.util;
            if (!o) {
                return null;
            }
            Y.Array.each(props, function (p) {
                var v = _.getProperty(o, p);
                if (!Y.Lang.isUndefined(v)) {
                    ret[p] = v;
                }
            });
            return ret;
        },
        /**
         * 检查浏览器是否支持placeholder
         * @method isSupportPlaceholder
         * @static
         * @return {Boolean} 是否支持placeholder
         */
        isSupportPlaceholder: function () {
            var elInput = document.createElement('input');
            return 'placeholder' in elInput;
        },
        /**
         * 解析Ajax返回对象
         * @method getEvalRes
         * @static
         * @param {Object} o
         * @param {String} customMsg 自定义错误字段的信息
         * @return {Object} JSON对象
         */
        getEvalRes: function (o, customMsg) {
            if (!o || !o.responseText) return o;
            var res = this.decodeJSON(o.responseText);
            return res || {'status': 0, 'msg': customMsg || '网络有问题，请稍后重试。'};
        },
        /**
         * 解析JSON字符串成对象
         * @method decodeJSON
         * @static
         * @param {String} str JSON字符串
         * @return {Object} JSON对象
         */
        decodeJSON: function (str) {
            /*jshint evil:true*/
            try {
                return eval('(' + str + ')');
            } catch (ex) {
                return null;
            }
        },
        /**
         * 解析节点属性保存的JSON
         * @method decodeAttr
         * @static
         * @param {Selector|HTMLElement|Node} nd
         * @param {String} attr 除去'data-'前缀的属性名。保存json数据的属性需以data-为前缀
         * @return {Object} JSON对象
         */
        decodeAttr: function (nd, attr) {
            var str = Y.mt.util.data(nd, attr);
            if (!str) return null;
            return Y.mt.util.decodeJSON(str);
        },
        /**
         * 通过ctrl enter来操作
         * @method ctrlEnter
         * @static
         * @param {Selector|HTMLElement|Node} nd
         * @param {Function} callback ctrlEnter后需要执行的函数
         */
        ctrlEnter: function (nd, callback) {
            nd = Y.one(nd);
            if (!nd) return;
            nd.on('key', function (e) {
                e.halt();
                callback();
            }, 'down:13+ctrl');
        },
        /**
         * 转换postData
         * @method toPostData
         * @static
         * @param {Object} o 要转换的对象 如{token:'abcdef', action:'msg.del'}
         * @param {Boolean} [isEncode=true] 是否需要encode
         * @return {String} POST的数据字符串
         */
        toPostData: function (o, isEncode) {
            if (!Y.Lang.isObject(o)) return o;
            if (typeof isEncode === 'undefined') isEncode = true;

            var arr = [],
                util = this;
            Y.Object.each(o, function (value, key) {
                var param;
                if (isArray(value)) {
                    Y.Array.each(value, function (item) {
                        param = util.joinKeyValue(key, item, isEncode);
                        arr.push(param);
                    });
                } else {
                    param = util.joinKeyValue(key, value, isEncode);
                    arr.push(param);
                }
            });
            return arr.join('&');
        },
        /**
         * 根据key,value生成键值对
         * @method joinKeyValue
         * @static
         * @param {String} key 键
         * @param {String} value 值
         * @param {Boolean} [isEncode] 是否编码
         * @return {String} key-value对组成的字符串
         */
        joinKeyValue: function (key, value, isEncode) {
            if (isEncode) {
                return this.encodeText(key) + "=" + this.encodeText(value);
            } else {
                return key + "=" + value;
            }
        },
        /**
         * 对字符编码
         * @method encodeText
         * @static
         * @param {String} text
         * @return {String} 编码后的字符串
         */
        encodeText: function (text) {
            return text ? encodeURIComponent(text) : text;
        },
        /**
         * 预加载图片
         * @method loadImage
         * @static
         * @param {String} src 图片地址
         */
        loadImage: function (src) {
            var img = new Image();
            img.src = src;
        },
        /**
         * 获取或设置节点属性(获取节点属性data-key的value，建议用Node.getData('key')代替)
         * @method data
         * @static
         * @param {Selector|HTMLElement|Node} nd 节点
         * @param {String} key 键
         * @param {String} value 值
         * @return {String|Boolean} 节点属性|是否设置成功
         */
        data: function (nd, key, value) {
            nd = Y.one(nd);
            if (!nd || !isStr(key)) return false;
            var attr = 'data-' + key;
            if (value === undefined) {
                return nd.getAttribute(attr);
            } else {
                if (!isStr(value)) {
                    value = Y.JSON.stringify(value);
                }
                nd.setAttribute(attr, value.replace(/'/g/*'*/, '&#039;'));
                return true;
            }
        },
        /**
         * 获取或设置表单项
         * @method field
         * @static
         * @param {Selector|HTMLElement|Node} ndForm form节点
         * @param {String} name 表单项名称
         * @param {String} value 值
         * @return {Node|Boolean} 表单项值|是否设置成功
         */
        field: function (ndForm, name, value) {
            ndForm = Y.one(ndForm);
            if (!ndForm || ndForm.get('tagName').toLowerCase() !== 'form') return false;
            var elForm = $N.getDOMNode(ndForm),
                ndField;
            if (value === undefined) {
                return Y.one(elForm.elements[name]);
            } else {
                // TODO 支持radio等同名元素数组
                ndField = Y.one(elForm.elements[name]);
                if (!ndField) return false;
                ndField.set('value', value);
                return true;
            }
        },
        /**
         * 切换显示隐藏区块
         * @method toggle
         * @static
         * @param {HTMLElement|Selector|Node} nd 区块元素
         * @param {Boolean} [flag] true为显示/false为隐藏
         */
        toggle: function (nd, flag) {
            nd = Y.one(nd);
            if (!nd) return;
            var show = flag === undefined ? nd.getStyle('display') === 'none' : flag;
            nd.setStyle('display', show ? 'block' : 'none'); // NOTE 此处不能将'block'换为''
        },
        /**
         * 添加onload监听器
         * @method addLoadListener
         * @static
         * @param {Function} handler 响应函数
         */
        addLoadListener: function (handler) {
            var oldListener = window.onload;
            if (!isFunction(oldListener)) {
                window.onload = handler;
            } else {
                window.onload = function () {
                    oldListener();
                    handler();
                };
            }
        },
        /**
         * 添加onbeforeunload监听器
         * @method addUnloadListener
         * @static
         * @param {Function} handler 响应函数
         */
        addUnloadListener: function (handler) {
            var oldListener = window.onbeforeunload;
            if (!isFunction(oldListener)) {
                window.onbeforeunload = handler;
            } else {
                window.onbeforeunload = function () {
                    var oldMsg, msg;
                    oldMsg = oldListener();
                    msg = handler();
                    return msg || oldMsg;
                };
            }
        },
        /**
         * 控制事件回调函数响应频率，例如scroll、resize
         * @method throttle
         * @static
         * @param {Function} handler
         * @param {Number} [time] 单位为ms
         * @return {Function} 添加频率控制的函数
         */
        throttle: function (handler, time) {
            var timer = null;
            time = time || 0;

            return function () {
                if (timer) return;
                timer = window.setTimeout(function () {
                    handler();
                    timer = null;
                }, time);
            };
        },
        /**
         * 传入一个函数func，和一个数字ms，返回一个f.
         * f函数调用时，将在ms毫秒后执行func.
         * 如果在ms内调用再次调用了f，则又将当前时刻为基准，
         * 在ms后执行func.
         *
         * @param {Function} func
         * @param {Number} ms
         * @returns {Function}
         */
        delay: function (func, ms) {
            var handler = null;
            return function () {
                if (handler) {
                    clearTimeout(handler);
                    handler = null;
                }
                var args = [].slice.call(arguments);
                handler = setTimeout(function () {
                    func.apply(null, args);
                }, ms);
            };
        },
        /**
         * 移除对象中的事件句柄
         * @method detachHandle
         * @static
         * @param {Object} o 保存事件句柄对象
         */
        detachHandle: function (o) {
            if (!o) return;
            var p;
            for (p in o) {
                if (o.hasOwnProperty(p) && o[p]) {
                    if (isFunction(o[p].detach)) {
                        o[p].detach();
                    } else {
                        Y.mt.util.detachHandle(o[p]);
                    }
                }
            }
        },
        /**
         * TODO 改进
         * javascript object to string
         * @method objToString
         * @static
         * @param {Object} o 对象
         * @param {String} level 递归
         * @return {String} JSON字符串
         */
        objToString: function (o, level) {
            if (level && level === "deep") return Y.JSON.stringify(o);

            var strArr = [],
                key = '',
                value = '',
                keys = Y.Object.keys(o);

            strArr.push('{');
            for (var i = 0; i < keys.length; i++) {
                key = keys[i];
                value = o[key];

                strArr.push(key + ':');
                // boolean、对象、string和number
                if (value) {
                    value = isObject(value) ? null : value.toString();
                }
                strArr.push("'" + value + "'");
                if (i !== keys.length - 1) {
                    strArr.push(',');
                }
            }

            strArr.push('}');
            return strArr.join('');
        },
        /**
         * 获取对象的keys 而非function的key
         * @deprecated 请使用Y.Object.keys
         * @method getKeys
         * @static
         * @param {Object} o 对象
         * @return {Array} key数组
         */
        getKeys: function (o) {
            var res = [];
            for (var p in o) {
                if (typeof o === "object" && typeof o !== "function" && o.hasOwnProperty(p)) {
                    res.push(p);
                }
            }
            return res;
        },
        /**
         * 重排。TIP：能够通过改善CSS解决reflow bug的，不要使用此方式
         * [force reflow in ie7](http://stackoverflow.com/questions/1702399/how-can-i-force-reflow-after-dhtml-change-in-ie7)
         * @method reflow
         * @static
         * @param {Selector|HTMLEelement|Node} node
         */
        reflow: function (node) {
            node = Y.one(node);
            var el = node ? node.getDOMNode() : document.body;
            el.className = el.className;
        },
        /**
         * Cookie
         * @class Cookie
         * @namespace mt.util
         * @static
         */
        Cookie: {
            /**
             * 获取过期时间
             * @method getExpiresDate
             * @static
             * @param {Number} days 天数
             * @param {Number} hours 小时数
             * @param {Number} minutes 分钟数
             * @return {String} 过期时间字符串
             */
            getExpiresDate: function (days, hours, minutes) {
                var ExpiresDate = new Date();
                //hours should be minutes
                if (typeof days === "number" && typeof hours === "number" && typeof minutes === "number") {
                    ExpiresDate.setDate(ExpiresDate.getDate() + parseInt(days, 10));
                    ExpiresDate.setHours(ExpiresDate.getHours() + parseInt(hours, 10));
                    ExpiresDate.setMinutes(ExpiresDate.getMinutes() + parseInt(minutes, 10));
                    return ExpiresDate.toGMTString();
                }
            },
            /**
             * 获取offset起的一对键值
             * @method _getValue
             * @private
             * @static
             * @param {Number} offset
             * @return {String} 一对键值组成的字符串
             */
            _getValue: function (offset) {
                var cookie = document.cookie,
                    endstr = cookie.indexOf(";", offset);
                if (endstr === -1) {
                    endstr = cookie.length;
                }
                return decodeURIComponent(cookie.substring(offset, endstr));
            },
            /**
             * 获取cookie值
             * @method get
             * @static
             * @param {String} name
             * @return {String} cookie值
             */
            get: function (name) {
                var cookie = document.cookie,
                    arg = name + "=",
                    alen = arg.length,
                    clen = cookie.length,
                    i = 0;
                while (i < clen) {
                    var j = i + alen;
                    if (cookie.substring(i, j) === arg) {
                        return this._getValue(j);
                    }
                    i = cookie.indexOf(" ", i) + 1;
                    if (i === 0) break;
                }
                return "";
            },
            /**
             * 设置cookie值
             * @method set
             * @static
             * @param {String} name
             * @param {String|Number} value
             * @param {String} expires
             * @param {String} path
             * @param {String} domain
             * @param {String} secure
             */
            set: function (name, value, expires, path, domain, secure) {
                document.cookie = name + "=" + encodeURIComponent(value) +
                    ((expires) ? "; expires=" + expires : "") +
                    ((path) ? "; path=" + path : "") +
                    ((domain) ? "; domain=" + domain : "") +
                    ((secure) ? "; secure" : "");
            },
            /**
             * 删除cookie值
             * @method remove
             * @static
             * @param {String} name
             * @param {String} path
             * @param {String} domain
             */
            remove: function (name, path, domain) {
                if (this.get(name)) {
                    document.cookie = name + "=" +
                        ((path) ? "; path=" + path : "") +
                        ((domain) ? "; domain=" + domain : "") +
                        "; expires=Thu, 01-Jan-70 00:00:01 GMT";
                }
            },
            /**
             * 清除mt cookie值
             * TODO 此函数有一些问题，清除cookie需要指定path和domain，但是path和domain无法从document.cookie中取得
             * @method clear
             * @static
             */
            clear: function () {
                var cookies = document.cookie.split(';');
                for (var i = 0; i < cookies.length; i++) {
                    this.remove(trim(cookies[i].split('=')[0]));
                    this.remove(trim(cookies[i].split('=')[0]));
                }
            }
        }
    };

    Y.namespace('mt.Tracking');
    /**
     * 页面追踪
     * @class Tracking
     * @namespace mt
     * @static
     */
    Y.mt.Tracking = {
        /**
         * 初始化事件自动跟踪
         * 注意：如果是监听链接，注意不要使用stopEvent来停止默认事件，否则ga无法完成统计。
         *       可以使用preventDefault或者javascript:void(0)。
         * 1.自定义跟踪GA事件，标签需添加gaevent属性, 如<dt gaevent="Help|Open|/help/Newbie/PayToday">...</dt>
         * 2.跟踪外链区分来源，a标签需添加galabel属性，如<a galabel="default" herf="http://kaixin001.com/...">...</a>
         * @method init
         * @static
         */
        init: function () {
            var GA_EVENT = 'gaevent', // 自定义trackEvent属性
                GA_LABEL = 'galabel', // 通用trackEvent外链属性
                _gaq = window._gaq,
                ndDoc = Y.one(document.body);

            ndDoc.delegate('click', function () {
                var node = this;

                Y.mt.Tracking.trackEventByNode(node);
                trackGACustomEvent(node);

                if (node.test('a')) {
                    trackOutLink(node);
                }
            }, '[gaevent], [data-mtevent], a');

            /**
             * 处理自定义GA事件
             * @method trackGACustomEvent
             * @param { Node } nd DOM节点
             */
            function trackGACustomEvent(nd) {
                var attr = nd.getAttribute(GA_EVENT);
                if (!attr) return;
                var attrs = attr.split('|'),
                    gaCategory = attrs[0],
                    gaEvent = attrs[1],
                    gaLabel = attrs[2],
                    gaValue = attrs[3];

                if (!gaLabel && nd.get('href')) {
                    gaLabel = nd.get('href').replace('http://', '');
                }
                if (typeof _gaq !== 'undefined' && _gaq) {
                    _gaq.push(['_trackEvent', gaCategory, gaEvent, gaLabel, gaValue]);
                }
            }

            /**
             * 处理外链
             * @method trackOutLink
             * @param { Node } nd Node节点
             */
            function trackOutLink(nd) {
                var link = nd.get('href'),
                    reg = /^(http:\/\/|mailto:)/,
                    index,
                    label;

                if (link && reg.test(link)) {
                    link = link.replace('http://', '');
                    index = link.indexOf('?');
                    label = nd.getAttribute(GA_LABEL);
                    if (index > 0) {
                        link = link.substring(0, index);
                    }

                    if (/\.(meituan|mt|sankuai)\.com/.test(link)) {
                        // 跳过美团内部链接
                        return;
                    }

                    if (label) {
                        link += '#label=' + label;
                    }

                    if (_gaq) {
                        _gaq.push(['_trackEvent', 'OutLink', 'Click', link]);
                    }
                }
            }
        },
        /**
         * 追踪pv
         * @method trackPageview
         * @static
         * @param {String} url
         * @param {Object} extra
         */
        trackPageview: function () {
            M.trackPageview.apply(Y, arguments);
        },
        /**
         * 追踪事件
         * @method trackEvent
         * @static
         * @param {String} category
         * @param {String} action
         * @param {String} label
         * @param {Object} extra
         */
        trackEvent: function () {
            M.trackEvent.apply(Y, arguments);
        },
        /**
         * 追踪节点事件
         * @method trackEventByNode
         * @static
         * @param {Node} nd
         */
        trackEventByNode: function (nd) {
            var MT_EVENT = 'mtevent',
                data = Y.mt.util.decodeAttr(nd, MT_EVENT),
                category, action, label;
            if (!data) return;
            category = data.ca;
            action = data.ac;
            label = data.la;
            delete data.ca;
            delete data.ac;
            delete data.la;

            this.trackEvent(category, action, label, data);
        }
    };

    /**
     * 宏
     * @class macro
     * @namespace mt
     * @static
     */
    Y.mt.macro = {
        /**
         * key map
         * @property key
         * @static
         * @type {Object}
         */
        key: {
            BACKSPACE   : 8,
            TAB         : 9,
            ENTER       : 13,
            PAGEUP      : 33,
            PAGEDOWN    : 34,
            ESC         : 27,
            LEFT        : 37,
            UP          : 38,
            RIGHT       : 39,
            DOWN        : 40
        }
    };

    /**
     * 使用Y.mt.require而不是使用namespace.
     * 因为module名已经限定了模块的名字，没有必要另起namespace的炉灶。
     * 如果模块没有加载, Y.mt.require将抛出友好的错误提示。
     *
     * @param module
     * @returns {*}
     */
    Y.mt.require = function (module) {
        var exports = Y.Env._exported;
        if (!Object.prototype.hasOwnProperty.call(exports, module)) {
            throw new Error(
                    'Please `Y.use` the module [' + module + '] before `Y.mt.require` it.' +
                    'Otherwise you must forgot add `es:true` when defined the module.'
            );
        }
        return exports[module];
    };

    Y.namespace('mt.widget');
    /**
     * 详见: http://wiki.sankuai.com/pages/viewpage.action?pageId=18382870
     * @class widget
     * @namespace mt
     * @description 
     * @static
     */
    Y.mix(Y.mt.widget, {
        /**
         * 预加载
         * @method preload
         * @static
         */
        preload: function (container) {
            //有关widget的初始化记录
            M.Widget = {
                preload: {
                    modules: [],
                    instances: {}
                },
                lazyload: {
                    modules: [],
                    instances: {}
                },
                map: this.getWidgetMap(),
                instances: {}
            };

            var widgetMap = M.Widget.map,
                preload = M.Widget.preload,
                lazyload = M.Widget.lazyload,
                decode = Y.mt.util.decodeJSON;

            var nodeList;
            var widgetSelector = '[data-widget], [data-uix]';
            if (container) {
                var ndContainer = Y.one(container);
                if (ndContainer) {
                    nodeList = ndContainer.all(widgetSelector);
                    if (ndContainer.hasAttribute('data-widget') || ndContainer.hasAttribute('data-uix')) {
                        nodeList.push(ndContainer);
                    }
                }
            } else {
                nodeList = Y.all(widgetSelector);
            }

            if (!nodeList || !nodeList.size()) {
                return;
            }

            nodeList.each(function (ndWidget) {
                var widgetName = ndWidget.getAttribute('data-widget') || ndWidget.getAttribute('data-uix'),
                    widgetLowerName = widgetName.toLowerCase(),
                    indexOf = Y.Array.indexOf;

                // 将属性写在DomNode上，防止不同实例的YUI生成不同实例的ndWidget
                // 不写在attribute上的原因是，节点可能会被复制。
                if (ndWidget.getDOMNode().hasWidgetInited) return;
                ndWidget.getDOMNode().hasWidgetInited = true;

                if (widgetName && (widgetMap[widgetLowerName] || widgetMap[widgetName])) {
                    var params = decode(ndWidget.getAttribute('data-params')) || ndWidget.getData('data-params') || {},
                        isUIX = ndWidget.hasAttribute('data-uix'),
                        isLazyload = !!params.lazyload;

                    if (isLazyload) {
                        if (!lazyload.instances[widgetName]) {
                            lazyload.instances[widgetName] = [];
                        }
                        lazyload.instances[widgetName].push({node: ndWidget, params: params, isUIX: isUIX});
                        if (indexOf(lazyload.modules, widgetMap[widgetLowerName]) === -1) {
                            lazyload.modules.push(widgetMap[widgetLowerName]);
                        }
                    } else {
                        if (!preload.instances[widgetName]) {
                            preload.instances[widgetName] = [];
                        }
                        preload.instances[widgetName].push({node: ndWidget, params: params, isUIX: isUIX});
                        if (indexOf(preload.modules, widgetMap[widgetLowerName]) === -1) {
                            preload.modules.push(widgetMap[widgetLowerName]);
                        }
                    }
                }
            });

        },
        /**
         * 初始化
         * @method init
         * @static
         * @event Widget.init.complete
         */
        init: function (container) {
            this.preload(container);

            var _this = this,
                ucfirst = Y.mt.lang.ucfirst,
                preload = M.Widget.preload,
                lazyload = M.Widget.lazyload;

            if (preload.modules.length && !Y.Object.isEmpty(preload.instances)) {
                Y.use(preload.modules, function (Y) {
                    Y.Object.each(preload.instances, callback);
                    Y.Global.fire("Widget.init.complete", M.Widget);
                });
            }

            if (lazyload.modules.length && !Y.Object.isEmpty(lazyload.instances)) {
                if (document.readyState === 'complete') {
                    Y.use(lazyload.modules, function (Y) {
                        Y.Object.each(lazyload.instances, callback);
                    });
                } else {
                    var onload = window.onload;
                    window.onload = function () {
                        if (onload) {
                            onload();
                        }
                        Y.use(lazyload.modules, function (Y) {
                            Y.Object.each(lazyload.instances, callback);
                        });
                    };
                }
            }

            function callback(instances, key) {
                var widgetName = ucfirst(key);
                Y.Array.each(instances, function (instance) {
                    var widget = instance.isUIX ? Y.mt.uix[widgetName] : Y.mt.widget[widgetName];
                    if (widget) {
                        _this.widgetInit(widget, instance.node);
                    }
                });
            }
        },
        /**
         * 实例化widget
         * @method widgetInit
         * @static
         * @param {Class} Widget 控件构造器
         * @param {Node} nd 节点
         */
        widgetInit: function (Widget, node) {
            var decode = Y.mt.util.decodeJSON,
                isNewWidget = Widget.superclass && Widget.superclass.constructor && Widget.superclass.constructor.NAME,
                params = decode(node.getAttribute('data-params')) || node.getData('data-params') || {},
                isUIX = node.hasAttribute('data-uix'),
                widgetInstance;

            if (isUIX) {
                widgetInstance = new Widget(node, params);
            } else if (isNewWidget) {
                //yui3类的Widget初始化
                params.node = node;
                widgetInstance = new Widget(params);
                widgetInstance.render();
            } else {
                //一般的Widget初始化
                widgetInstance = new Widget(node, params);
            }
            this.saveWidget(node, widgetInstance);
        },
        /**
         * 保存widget
         * @method saveWidget
         * @static
         * @param {Selector|HTMLElement|Node} nd
         * @param {Object} widget
         */
        saveWidget: function (nd, widget) {
            nd = Y.one(nd);
            if (!nd) return;
            if (!nd.get('id')) nd.set('id', Y.guid());
            M.Widget.instances[nd.get('id')] = widget;
        },
        /**
         * 取回widget
         * @method fetchWidget
         * @static
         * @param {Selector|HTMLElement|Node} nd
         * @return {Object} widget实例
         */
        fetchWidget: function (nd) {
            var ndId;
            nd = Y.one(nd);
            if (nd) ndId = nd.get('id');

            return ndId ? M.Widget.instances[ndId] : null;
        },
        /**
         * 可被初始化的widget序列
         * @method getWidgetMap
         * @static
         * @return {Object} widget map
         */
        getWidgetMap: function () {
            return {
                buttontopost: "w-base",
                cascade: "w-select-cascade",
                livesearch: "w-livesearch",
                flashcopy: "w-flashcopy",
                commondialog: "w-base",
                headertip: "w-base",
                autocompletelist: "w-autocomplete",
                commontable: "w-table",
                sorttable: "w-table",
                ajaxtable: "w-table",
                paginator: "w-paginator",
                datepicker: "w-date",
                colorpicker: "w-colorpicker",
                timepicker: "w-date",
                validate: "w-validate",
                rating: "w-rating",
                dialog: "w-dialog",
                linkdialog: "w-dialog",
                bubbletip: "w-bubbletip",
                slider: "w-slider",
                verticaltab: "w-tab",
                tab: "w-tab",
                tabindex: "w-tab",
                picgroup: "w-image",
                picphotodialog: "w-image-dialog",
                realtimeeditor: "w-realtime-editor",
                hovermark: "w-hovermark",
                guide: "w-guide",
                imagecopper: 'w-image-copper',
                collapse: "uix-collapse",
                tooltip: "uix-tooltip",
                dropdown: "uix-dropdown",
                sticky: "uix-sticky",
                tabview: "uix-tabview",
                scrollspy: 'uix-scrollspy',
                smoothscroll: "uix-smoothscroll",
                mapview: "w-mapview"
            };
        }
    });
},
// version
'1.0.0',
// dependency
{
    requires: [
        'node',
        'event',
        'json'
    ]
});
 

;/*jshint white:true,unused:true*/
/**
 * 系统中的一些和UI相关的通用工具、展示类
 * @module mt-uiutil
 */
M.add('mt-uiutil', function (Y) {

    var $D = Y.DOM;

    /**
     * @class UIUtil
     * @namespace mt
     * @static
     */
    Y.mt.UIUtil = {
        /**
         * toggle 某范围内的一组节点
         * @method toggleNode
         * @param {Selector|Node|HTMLElement} container 一组toggle节点的父级容器
         * @param {Selector} trigger 触发点击事件的seletor
         * @param {Selector} handle 响应点击事件toggle的selector
         * @param {String} activeClass 触发器激活时添加的CSS类
         */
        toggleNode: function (container, trigger, handle, activeClass) {
            var ndContainer = Y.one(container);
            if (!ndContainer) return;

            var nlTrigger = ndContainer.all(trigger),
                nlHandle = ndContainer.all(handle),
                CLASS_HOVER = 'hover',
                CLASS_ACTIVE = activeClass ? activeClass : '';

            ndContainer.delegate('click', function () {
                var ndBox = nlHandle.item(nlTrigger.indexOf(this)),
                    display = ndBox.getStyle('display');

                if (display === 'none') {
                    ndBox.setStyle('display', 'block');
                    this.addClass(CLASS_ACTIVE);
                } else {
                    ndBox.setStyle('display', 'none');
                    this.removeClass(CLASS_ACTIVE);
                }
            }, trigger);
            ndContainer.delegate('mouseover', function () {
                this.addClass(CLASS_HOVER);
            }, trigger);
            ndContainer.delegate('mouseout', function () {
                this.removeClass(CLASS_HOVER);
            }, trigger);
        },
        /**
         * 获得节点的各种尺寸信息，尤其是对于隐藏节点来说比较方便
         * @method getDimensions
         * @param {Selector|Node|HTMLElement} node 具体节点
         * @return {Object}
         * {width: 10, height: 10, offsetWidth: 10, offsetHeight: 10, paddingBottom.....}
         */
        getDimensions: function (node) {
            node = Y.one(node);
            var el = node.getDOMNode(),
                ndParent = node.ancestor(),
                dimensions = {}, allWidth = 0, allHeight = 0, old = {}, oldParentPosition, name,
                cssShow = { position: 'absolute', visibility: 'hidden', display: 'block' };

            Y.Array.each([
                'marginTop', 'marginBottom', 'marginLeft', 'marginRight',
                "paddingLeft", "paddingRight", "paddingBottom", "paddingTop",
                'borderLeftWidth', 'borderRightWidth', 'borderBottomWidth', 'borderTopWidth'
            ], function (val) {
                dimensions[val] = parseInt($D.getStyle(el, val), 10);
            });
            //正常显示的状态
            if (el.offsetHeight !== 0) {
                Y.mix(dimensions, {
                    offsetHeight: el.offsetHeight,
                    offsetWidth: el.offsetWidth,
                    width: parseInt($D.getStyle(el, "width"), 10),
                    height: parseInt($D.getStyle(el, "height"), 10)
                });
            } else {
                // 节点隐藏
                for (name in cssShow) {
                    if (cssShow.hasOwnProperty(name)) {
                        old[name] = el.style[name];
                        el.style[name] = cssShow[name];
                    }
                }
                oldParentPosition = ndParent.getStyle('position');
                if (oldParentPosition !== 'relative' && oldParentPosition !== 'absolute') {
                    ndParent.setStyle('position', 'relative');
                }
                Y.mix(dimensions, {
                    offsetHeight: el.offsetHeight,
                    offsetWidth: el.offsetWidth
                });
                ndParent.setStyle('position', oldParentPosition);
                for (name in cssShow) {
                    if (cssShow.hasOwnProperty(name)) {
                        el.style[name] = old[name];
                    }
                }
                //计算节点的宽度和高度
                Y.Array.each(['borderLeftWidth', 'borderRightWidth', 'paddingLeft', 'paddingRight'], function (val) {
                    allWidth += dimensions[val];
                });
                Y.Array.each(['borderTopWidth', 'borderBottomWidth', 'paddingTop', 'paddingBottom'], function (val) {
                    allHeight += dimensions[val];
                });

                dimensions.width = dimensions.offsetWidth - allWidth;
                dimensions.height = dimensions.offsetHeight - allHeight;
            }

            return dimensions;
        },
        /**
         * 图片预加载
         * @method loadImg
         * @return {Function} 返回函数接受的参数如下:
         * <dl>
         *     <dt>url {String} 图片的url</dt>
         *     <dd>ready {Function} onready时的处理</dd>
         *     <dd>load {Function} onload时的处理</dd>
         *     <dd>error {Function} onerror时的处理</dd>
         * </dl>
         */
        loadImg: function () {
            var list = [], intervalId = null,

            // 用来执行队列
            tick = function () {
                var i = 0;
                for (i; i < list.length; i++) {
                    if (list[i].end) {
                        i--;
                        list.splice(i, 1);
                    } else {
                        list[i]();
                    }
                }
                if (list.length === 0) {
                    stop();
                }
            },
            // 停止所有定时器队列
            stop = function () {
                window.clearInterval(intervalId);
                intervalId = null;
            };
            return function (url, ready, load, error) {
                var width, height, newWidth, newHeight,
                    img = new Image();

                img.src = url;

                // 如果图片被缓存，则直接返回缓存数据
                if (img.complete) {
                    if (ready) {
                        ready.call(img);
                    }
                    if (load) {
                        load.call(img);
                    }
                    return img;
                }

                width = img.width;
                height = img.height;
                onready();

                // 加入队列中定期执行
                if (!onready.end) {
                    list.push(onready);
                    // 无论何时只允许出现一个定时器，减少浏览器性能损耗
                    if (intervalId === null) intervalId = window.setInterval(tick, 40);
                }

                // 加载错误后的事件
                img.onerror = function () {
                    if (error) {
                        error.call(img);
                    }
                    onready.end = true;
                    img = img.onload = img.onerror = null;
                };
                // 完全加载完毕的事件
                img.onload = function () {
                    // onload在定时器时间差范围内可能比onready快
                    // 这里进行检查并保证onready优先执行
                    if (!onready.end) {
                        onready();
                    }

                    if (load)  {
                        load.call(img);
                    }

                    // IE gif动画会循环执行onload，置空onload即可
                    img = img.onload = img.onerror = null;
                };
                // 图片尺寸就绪
                function onready() {
                    newWidth = img.width;
                    newHeight = img.height;
                    // 如果图片已经在其他地方加载可使用面积检测
                    if (newWidth !== width || newHeight !== height || newWidth * newHeight > 1024) {
                        if (ready) {
                            ready.call(img);
                        }
                        onready.end = true;
                    }
                }
                return img;
            };
        },

        /**
         * @method isSupportAnimation
         * @description 监控浏览器是否支持动画的高级属性
         * @return supportResult {Object} {'transition': Boolean, 'transform': Boolean, 'animation': Boolean}
         */
        isSupportAnimation: function () {
            var prefixes = [' ', 'webkit', 'Webkit', 'Moz', 'O', 'ms', 'Khtml'],
                ndDetect = document.createElement('div'),
                supportResult = {
                    transform: false,
                    animation: false,
                    transition: false
                };

            for (var p in supportResult) {
                if (supportResult.hasOwnProperty(p)) {
                    for (var i = 0, len = prefixes.length; i < len; i++) {
                        var prop = prefixes[i] === ' ' ? p : prefixes[i] + Y.mt.lang.ucfirst(p);
                        if (typeof ndDetect.style[prop] === 'string') {
                            supportResult[p] = true;
                            break;
                        }
                    }
                }
            }
            return supportResult;
        },

        /**
         * @method bindTranEndEvent
         * @description 绑定跨浏览器过渡结束的事件
         * @param ndTarget {String || Node} 事件绑定节点
         * @param callback {Function} 回调函数
         */
        bindTranEndEvent: function (ndTarget, callback) {
            var endEvent = ['transitionend', 'webkitTransitionEnd', 'oTransitionEnd', 'MSTransitionEnd'];

            if (this.isSupportAnimation().transition) {
                Y.one(ndTarget).on(endEvent, function () {
                    callback(ndTarget);
                });
            } else {
                callback();
            }
        },

        /**
         * @method bindAnimEndEvent
         * @description 绑定跨浏览器动画结束事件
         * @param ndTarget {String || Node} 事件绑定节点
         * @param callback {Function} 回调函数
         */
        bindAnimEndEvent: function (ndTarget, callback) {
            var endEvent = ['animationend', 'webkitAnimationEnd', 'MSAnimationEnd', 'oanimationend'];

            if (this.isSupportAnimation().animation) {
                // 使用原生的绑定事件，是因为YUI3不能监听animationend事件
                for (var i = 0, len = endEvent.length; i < len; i++) {
                    Y.one(ndTarget).getDOMNode().addEventListener(endEvent[i], function (e) {
                        callback(e, ndTarget);
                    });
                }
            } else {
                callback(ndTarget);
            }
        }
    };

}, '1.0.0', { requires: ['mt-base', 'node', 'dom'] });
 

;/*jshint white:true*/
/*global M:true*/
/**
 * 封装一些常用的方法到对象M.window上。
 * @module mt-window
 */
M.add('mt-window', function (Y) {
});

M.window = M.window || (function (w) {
    var d = w.document;

    var isObjectEmpty = function (o) {
        if (Object.keys) {
            return Object.keys(o).length === 0;
        }
        var ret = true;
        for (var k in o) {
            if (o.hasOwnProperty(k)) {
                ret = false;
                break;
            }
        }
        return ret;
    };

    var unbind = function (type, cb, elem) {
        elem = elem || w;
        if (elem.removeEventListener) {
            // 据YUI说Firefox下会报错
            try {
                elem.removeEventListener(type, cb);
            } catch (ex) {}
        } else if (elem.detachEvent) {
            elem.detachEvent('on' + type, cb);
        }
    };
    // 事件绑定返回值，用于解绑
    var Handler = function (type, cb, elem) {
        this.type = type;
        this.cb = cb;
        this.elem = elem || w;
    };
    Handler.prototype.detach = function () {
        unbind(this.type, this.cb, this.elem);
        delete this.cb;
        delete this.elem;
    };
    var bind = function (type, cb, elem) {
        elem = elem || w;
        if (elem.addEventListener) {
            elem.addEventListener(type, cb);
        } else if (elem.attachEvent) {
            elem.attachEvent('on' + type, function (e) {
                cb.call(elem, e);
            });
        }
        return new Handler(type, cb, elem);
    };

    var now = Date.now || function () { return (new Date()).getTime(); };

    // requestAnimationFrame polyfill
    // 参考自https://github.com/darius/requestAnimationFrame/blob/master/requestAnimationFrame.js
    (function () {
        var vendors = ['webkit', 'moz'],
            i, vp, lastTime;
        for (i = 0; i < vendors.length && !w.requestAnimationFrame; ++i) {
            vp = vendors[i];
            w.requestAnimationFrame = w[vp + 'RequestAnimationFrame'];
            w.cancelAnimationFrame = w[vp + 'CancelAnimationFrame'] || w[vp + 'CancelRequestAnimationFrame'];
        }
        // iOS6 is buggy
        if (w.navigator && (/iP(ad|hone|od).*OS 6/.test(w.navigator.userAgent) || !w.requestAnimationFrame || !w.cancelAnimationFrame)) {
            lastTime = 0;
            w.requestAnimationFrame = function (callback) {
                var t = now(),
                    nextTime = Math.max(lastTime + 16, t);
                return w.setTimeout(function () {
                    callback(nextTime);
                    lastTime = nextTime;
                }, nextTime - t);
            };
            w.cancelAnimationFrame = function (id) {
                w.clearTimeout(id);
            };
        }
    })();

    var FrameManager = (function () {
        var frameStamp = 1,
            cbq = {},
            isRunning = false;
        var executor = function () {
            var queue = [],
                fs;
            ++frameStamp;
            for (fs in cbq) {
                if (cbq.hasOwnProperty(fs) && cbq[fs].length && fs <= frameStamp) {
                    queue = queue.concat(cbq[fs]);
                    delete cbq[fs];
                }
            }
            if (queue.length) {
                callInFrames(queue);
            }
            if (isObjectEmpty(cbq)) {
            // 如果rAF中的队列为空，则停止执行
                isRunning = false;
                frameStamp = 1;
            } else {
            // 如果rAF中的队列不为空，则下一帧继续
                w.requestAnimationFrame(executor);
            }
        };
        return {
            setFrameout: function (cb, delay) {
                delay = delay || 0;
                var fs = frameStamp + delay,
                    queue = cbq[fs] || [];
                queue.push(cb);
                cbq[fs] = queue;
                if (!isRunning) {
                    isRunning = true;
                    w.requestAnimationFrame(executor);
                }
                return {
                    fs: fs,
                    cb: cb
                };
            },
            clearFrameout: function (h) {
                if (!h || !h.fs || !h.cb) return;
                if (cbq[h.fs]) {
                    for (var i = cbq[h.fs].length - 1; i >= 0; i--) {
                        if (cbq[h.fs][i] === h.cb) {
                            cbq[h.fs].splice(i, 1);
                            break;
                        }
                    }
                }
                delete h.cb;
                delete h.fs;
            }
        };
    })();

    // 控制回调持续执行时间
    var callInFrames = (function () {
        var cbq = [],
            isRunning = false,
            execute = function () {
                var cb,
                    stopTime = now() + 20;  // 每帧限制函数执行时间为20ms，未执行完的函数放到下帧继续
                isRunning = true;

                while (cbq.length && now() <= stopTime) {
                    cb = cbq.shift();
                    cb();
                }
                if (cbq.length) {
                    w.requestAnimationFrame(execute);
                } else {
                    isRunning = false;
                }
            };
        return function (cbOrArray) {
            cbq = cbq.concat(cbOrArray);
            if (!isRunning) {
                w.requestAnimationFrame(execute);
            }
        };
    })();

    var throttle = function (cb, interval) {
        var pending = false;
        interval = interval || 0;
        return function (event) {
            if (pending) return;
            var currentTarget = this;
            pending = true;
            FrameManager.setFrameout(function () {
                cb.call(currentTarget, event);
                pending = false;
            }, interval);
        };
    };
    var scroll = function (cb, interval) {
        return bind('scroll', throttle(cb, interval));
    };
    var resize = function (cb, interval) {
        return bind('resize', throttle(cb, interval));
    };
    var getScrollDownCB = function (cb) {
        var pos = 0;
        return function (event) {
            var posTo = winInfo.pageYOffset();
            if (pos < posTo) {
                cb.call(this, event);
            }
            pos = posTo;
        };
    };
    var scrollDown = function (cb, interval) {
        return bind('scroll', getScrollDownCB(throttle(cb, interval)));
    };

    // 提供最新的视口宽高，滚动位置
    var winInfo = (function () {
        function getWinSize() {
            var cache = {};
            if (typeof w.innerWidth !== 'undefined') {
            // This works for all browsers except IE version 8 and before
                cache.innerWidth = w.innerWidth;
                cache.innerHeight = w.innerHeight;
            } else if (d.compatMode === 'CSS1Compat') {
            // For IE in Standards mode
                cache.innerWidth = d.documentElement.clientWidth;
                cache.innerHeight = d.documentElement.clientHeight;
            } else {
            // For browsers in Quirks mode
                cache.innerWidth = d.body.clientWidth;
                cache.innerHeight = d.body.clientHeight;
            }

            return {
                w: cache.innerWidth,
                h: cache.innerHeight
            };
        }
        function getPageOffsets() {
            var cache = {};
            if (typeof w.pageXOffset !== 'undefined') {
            // This works for all browsers except IE version 8 and before
                cache.pageXOffset = w.pageXOffset;
                cache.pageYOffset = w.pageYOffset;
            } else if (d.compatMode === 'CSS1Compat') {
            // For IE in Standards mode
                cache.pageXOffset = d.documentElement.scrollLeft;
                cache.pageYOffset = d.documentElement.scrollTop;
            } else {
            // For browsers in Quirks mode
                cache.pageXOffset = d.body.scrollLeft;
                cache.pageYOffset = d.body.scrollTop;
            }

            return {
                x: cache.pageXOffset,
                y: cache.pageYOffset
            };
        }
        return {
            pageXOffset: function () {
                return getPageOffsets().x;
            },
            pageYOffset: function () {
                return getPageOffsets().y;
            },
            innerWidth: function () {
                return getWinSize().w;
            },
            innerHeight: function () {
                return getWinSize().h;
            },
            docWidth: function () {
                var root = d.documentElement;
                if (d.compatMode !== 'CSS1Compat') {
                    root = d.body;
                }
                return root.scrollWidth;
            },
            docHeight: function () {
                var root = d.documentElement;
                if (d.compatMode !== 'CSS1Compat') {
                    root = d.body;
                }
                return root.scrollHeight;
            }
        };
    })();

    return {
        /**
         * 返回当前时刻
         * @method now
         * @static
         * @return {Number}
         */
        now: now,
        /**
         * 监听window的scroll事件，回调函数统一处理
         * @method scroll
         * @static
         * @param {Function} cb
         * @param {Number} [interval=0] 每两次执行间隔在指定帧以上
         * @return {Object} 拥有detach方法
         */
        scroll: scroll,
        scrollDown: scrollDown,
        /**
         * 监听window的resize事件，回调函数统一处理
         * @method resize
         * @static
         * @param {Function} cb
         * @param {Number} [interval=0] 每两次执行间隔在指定帧以上
         * @return {Object} 拥有detach方法
         */
        resize: resize,
        /**
         * 获取window相关的信息，如pageXOffset, pageYOffset, innerHeight, innerWidth
         * @method get
         * @static
         * @param {String} attr
         * @return {Number}
         */
        get: function (attr) {
            if (attr) {
                return winInfo[attr]();
            } else {
                var ret = {};
                for (attr in winInfo) {
                    if (winInfo.hasOwnProperty(attr)) {
                        ret[attr] = winInfo[attr]();
                    }
                }
                return ret;
            }
        },
        math: {
            /**
             * 计算left + right，保留指定精度。会先对操作数做精度处理。消除原生浮点数操作带来的误差
             * @method subtract
             * @param {Number} left
             * @param {Number} right
             * @param {Number} precision
             * @return {String}
             */
            add: function (left, right, precision) {
                var l = Math.round(this.shift(left, precision)),
                    r = Math.round(this.shift(right, precision));
                return this.toFixed(this.shift(l + r, -precision), precision);
            },
            /**
             * 计算left - right，保留指定精度。会先对操作数做精度处理。消除原生浮点数操作带来的误差
             * @method subtract
             * @param {Number} left 被减数
             * @param {Number} right 减数
             * @param {Number} precision
             * @return {String}
             */
            subtract: function (left, right, precision) {
                var l = Math.round(this.shift(left, precision)),
                    r = Math.round(this.shift(right, precision));
                return this.toFixed(this.shift(l - r, -precision), precision);
            },
            /**
             * 计算number * Math.pow(10, factor)。只移动小数点，以确保不引入由浮点数运算带来的误差
             * @method shift
             * @param {Number} number
             * @param {Number} factor 小于0表示左移小数点（缩小），大于0表示右移小数点（放大）
             * @return {Number}
             */
            shift: function (number, factor) {
                if (number !== number) return NaN;
                number = +number;
                if (!number || !factor) return number;

                // shift out，缩小
                var neg  = number < 0,
                    sign = neg ? '-' : '+';
                number = neg ? -number : number;

                number += '.';
                var arr = number.split('.'),
                    integer = arr[0],
                    fraction = arr[1],
                    i = factor;

                // shift in，放大
                if (factor > 0) {
                    // 确保尾数够0
                    while (i--) {
                        fraction += '0';
                    }
                    return +(sign + integer + fraction.slice(0, factor) + '.' + fraction.slice(factor));
                }

                i = factor;
                // 确保够0
                while (i++) {
                    integer = '0' + integer;
                }
                return +(sign + integer.slice(0, factor) + '.' + integer.slice(factor) + fraction);
            },
            /**
             * 对给定数字取给定精度。消除原生toFixed的bug, 如2.385.toFixed(2) => 2.38
             * @method toFixed
             * @param {Number} number
             * @param {Number} precision 小于0表示整数取精度（this.toFixed(2135.245, -2) => 2100），大于0表示浮点数取精度（this.toFixed(2135.245, 2) => 2135.25）
             * @return {String}
             */
            toFixed: function (number, precision) {
                number = parseFloat(number);
                if (number !== number) return NaN;
                precision = parseInt(precision, 10) || 0;
                if (precision !== precision) return NaN;

                if (!precision) return Math.round(number) + '';

                number = Math.round(this.shift(number, precision));
                var paddingZero = '',
                    i = precision;
                if (precision < 0) {
                    while (i++) {
                        paddingZero += '0';
                    }
                    return number ? number + paddingZero : '0';
                }

                var neg = number < 0,
                    sign = neg ? '-' : '';
                number = neg ? -number : number;
                number += '';
                while (number.length <= precision) {
                    number = '0' + number;
                }
                return sign + number.slice(0, -precision) + '.' + number.slice(-precision);
            }
        },
        setFrameout: FrameManager.setFrameout,
        clearFrameout: FrameManager.clearFrameout
    };
})(window);



;/*jshint white:true,unused:true*/
/**
 * @module mt-event
 */
M.add('mt-event', function (Y) {
    var NS = Y.namespace('mt.Event');
    // execute functions as soon as possible
    function Asap() {
        this.pending = true;
        this.queue = [];
    }
    Asap.prototype.push = function (fn) {
        if (this.pending) {
            this.queue.push(fn);
        } else {
            window.setTimeout(fn, 0);
        }
    };
    Asap.prototype.flush = function () {
        this.pending = false;
        while (this.queue.length) {
            window.setTimeout(this.queue.shift(), 0);
        }
    };
    NS.Asap = Asap;

    /**
     * bind function, specifying context, extra arguments
     * @param {Function|String} f function object OR method name
     * @param {Object} c value of this when executing f. If f is a method name, the regarding function object is supposed to be a property value of c
     * @param {...} args extra arguments passed to f
     * @return {Function} new function
     *
     */
    NS.rbind = function (f, c) {
        var xargs = arguments.length > 2 ? Y.Array(arguments, 2, true) : null;
        return function () {
            var fn = typeof f === 'string' ? c[f] : f;
            var args = xargs ? Y.Array(arguments, 0, true).concat(xargs) : arguments;
            if (M.DEBUG) {
                // Y.error处理后，无法在浏览器中定位错误，故不要try catch
                return fn.apply(c || this, args);
            }

            var ret;
            try {
                ret = fn.apply(c || this, args);
            } catch(ex) {
                ret = ex;
                Y.error('rbind function call error', ex, 'mt-event');
            }
            return ret;
        };
    };

    NS.load = (function () {
        var queue = new Asap();
        if (window.onload) {
            queue.push(NS.rbind(window.onload));
        }
        // 一些情况下，在执行到这里的时候已经过了 onload
        if (document.readyState === "complete") {
            queue.flush();
        }
        window.onload = function () {
            queue.flush();
        };
        return function () {
            queue.push(NS.rbind.apply(NS, arguments));
        };
    })();

    NS.domready = (function() {
        var queue = new Asap();
        var monitor = function () {
            queue.flush();
        };
        NS.load(monitor);
        Y.on('domready', monitor);
        return function () {
            queue.push(NS.rbind.apply(NS, arguments));
        };
    }());

}, '1.0.0', {
    requires: ['event']
});


;/*jshint white:true,unused:true*/
/**
 * 监听节点群的hover事件，支持transition的效果
 * @module mt-event-hover
 */
M.add('mt-event-hover', function (Y) {
    var isFunction = Y.Lang.isFunction;
    var isArray = Y.Lang.isArray;
    var isString = Y.Lang.isString;
    var Promise = Y.Promise;

    function HoverHandle(ndTarget, mouseenter, mouseleave, transition) {
        // 当前处于over状态的target数。enter时加1，leave时减1
        this.state = 0;
        // 每个target对应的hover handle
        this.handles = {};
        // 所有mouseenter回调
        this.enterCallbacks = [];
        // 所有mouseleave回调
        this.leaveCallbacks = [];

        this.setTransition(transition);
        this.add(ndTarget, mouseenter, mouseleave);
    }

    HoverHandle.prototype.add = function (ndContainer, mouseenter, mouseleave, selector) {
        return this.addTarget(ndContainer, selector).addCallback(mouseenter, mouseleave);
    };

    HoverHandle.prototype.addTarget = function (ndTarget, selector) {
        var id = Y.stamp(ndTarget);
        if (ndTarget && !this.handles[id]) {
            this.handles[id] = this._addTarget(ndTarget, selector);
        }
        return this;
    };

    HoverHandle.prototype.removeTarget = function (ndTarget) {
        var id = isString(ndTarget) ? ndTarget : Y.stamp(ndTarget);
        if (this.handles[id]) {
            this.handles[id].detach();
            delete this.handles[id];
            return true;
        }
        return false;
    };

    HoverHandle.prototype.addCallback = function (mouseenter, mouseleave) {
        return this._addCallback(this.enterCallbacks, mouseenter)._addCallback(this.leaveCallbacks, mouseleave);
    };

    HoverHandle.prototype.removeCallback = function (mouseenter, mouseleave) {
        return this._removeCallback(this.enterCallbacks, mouseenter)._removeCallback(this.leaveCallbacks, mouseleave);
    };

    HoverHandle.prototype.getTransition = function (type) {
        var delay = this.transition[type];
        if (isFunction(delay)) {
            return new Promise(delay);
        }
        return new Promise(function (resolve) {
            Y.later(+delay || 0, null, resolve);
        });
    };

    HoverHandle.prototype.setTransition = function (config) {
        this.transition = Y.merge(this.transition, config);
        return this;
    };

    HoverHandle.prototype.detach = function () {
        Y.Object.each(this.handles, function (h) {
            h.detach();
        });
        this.handles = {};
        this.enterCallbacks = [];
        this.leaveCallbacks = [];
        return this;
    };

    HoverHandle.prototype._addTarget = function (ndTarget, selector) {
        var self = this;
        var mouseenter = function () {
            self.state += 1;

            // 不为1，说明有多个元素在鼠标hover下
            if (self.inProgress || self.state !== 1) {
                return;
            }
            self.inProgress = true;

            var context = this;
            var args = Y.Array(arguments);
            self.getTransition('mouseenter').then(function () {
                self.inProgress = false;
                if (self.state) {
                    self._runCallbacks(self.enterCallbacks, context, args);
                }
            });
        };
        var mouseleave = function () {
            self.state -= 1;
            // 不为0，说明self.state > 0，即还有元素在鼠标hover下
            if (self.inProgress || self.state !== 0) {
                // 理论上，期望是先有mouseenter然后才能有mouseleave
                // 但页面加载时，如果鼠标先落在了target上，初始化后，再移开时就会出现一次mouseleave
                // 考虑到实际应用中都是mouseenter修改某个状态，再由mouseleave恢复，所以忽略这一次的mouseleave
                self.state = self.state < 0 ? 0 : self.state;
                return;
            }

            self.inProgress = true;

            var context = this;
            var args = Y.Array(arguments);
            self.getTransition('mouseleave').then(function () {
                self.inProgress = false;
                if (!self.state) {
                    self._runCallbacks(self.leaveCallbacks, context, args);
                }
            });
        };
        if (selector) {
            return ndTarget.delegate('hover', mouseenter, mouseleave, selector);
        } else {
            return ndTarget.on('hover', mouseenter, mouseleave);
        }
    };

    HoverHandle.prototype._runCallbacks = function (callbacks, context, args) {
        try {
            Y.Array.each(callbacks, function (cb) {
                cb.apply(context, args);
            });
        } catch(ex) {
            if (M.DEV) {
                throw ex;
            }
        }
    };

    HoverHandle.prototype._addCallback = function (callbacks, callback) {
        if (isFunction(callback) && Y.Array.indexOf(callbacks, callback) === -1) {
            callbacks.push(callback);
        }
        return this;
    };

    HoverHandle.prototype._removeCallback = function (callbacks, callback) {
        var index = Y.Array.indexOf(callbacks, callback);
        if (index > -1) {
            callbacks.splice(index, 1);
        }
        return this;
    };

    var NS = Y.namespace('mt.Event.Hover');
    /**
     * 监听节点群的hover事件，支持transition的效果。如果只是祖先与后代的关系，请优先指定selector以使用delegate
     * @method listen
     * @param {Node|NodeList|Array} targets 节点群。
     * @param {Function} mouseenter mouseenter触发的transition结束时，若鼠标还在节点群上，则执行该回调
     * @param {Function} mouseleave mouseleave触发的transition结束时，若鼠标不在节点群上，则执行该回调
     * @param {Object} transition mouseenter或mouseleave时，在回调执行前做一系列操作（譬如延时）。
     * @param {Number|Function} [transition.mouseenter] 如果为Number，则表示延时；如果为Function，则将基于该函数创建Promise，控制回调执行
     * @param {Number|Function} [transition.mouseleave] 如果为Number，则表示延时；如果为Function，则将基于该函数创建Promise，控制回调执行
     * @param {String} selector 如果不为空，则targets必须是Node，使用delegate的方式监听mouseenter与mouseleave
     *
     */
    NS.listen = function (targets, mouseenter, mouseleave, transition, selector) {
        var handle = new HoverHandle(null, mouseenter, mouseleave, transition);
        if (targets) {
            if (targets._node) {
                handle.addTarget(targets, selector);
            } else if (targets._nodes || isArray(targets)) {
                Y.each(targets, handle.addTarget, handle);
            }
        }
        return handle;
    };

    NS.once = function () {
        var h = NS.listen.apply(NS, arguments);
        h.addCallback(function () {
            h.detach();
        });
        return h;
    };

}, '1.0.0', {
    requires: ['promise', 'event']
});


;/*jshint white:true, unused:true*/

/**
 * 组件模块
 * @module mt-component
 */
M.add('mt-component', function (Y) {

        var Event = Y.mt.Event;
        var isFunction = Y.Lang.isFunction,
            each = Y.Array.each,
            oeach = Y.Object.each,
            keys = Y.Object.keys,
            noop = function () {
            },
            $Util = Y.mt.util,
            ucfirst = Y.mt.lang.ucfirst,
            YUIConfig = window['YUI_config'],
            Promise = Y.Promise,

            Resource,
            Component,
            ComponentHub,

            APP_MODULES = M.Groups && M.Groups[M.APP] ? M.Groups[M.APP].modules : {};

        // 组件资源管理
        Y.mt.ComponentResource = Resource = {
            cache: {},
            buffer: [],
            timer: null,
            TYPE_CSS: 'css',
            TYPE_SCRIPT: 'js',
            DELAY: 50,
            /**
             * 获取 Combo URL 地址
             * @method getComboURL
             * @param {Array} pathList
             * @param {String}
             */
            getComboURL: function (pathList) {
                var urls = [],
                    base;
                if (M.DEV !== 2) {
                    return YUIConfig.comboBase + pathList.join(YUIConfig.comboSep);
                }
                base = M.Groups && M.Groups[M.APP] ? M.Groups[M.APP].base : YUIConfig.base;
                each(pathList, function (path) {
                    // NOTE ugly remove duplicate app segment
                    if (path.indexOf('\/' + M.APP) === 0 || path.indexOf(M.APP) === 0) {
                        path = path.substring(M.APP.length + (path.indexOf('\/') === 0 ? 1 : 0));
                    }
                    urls.push(base + path);
                });
                return urls;
            },
            /**
             * 加载 CSS
             * @method _requestCSS
             * @param {Array} cssList
             * @param {Function} resolve
             */
            _requestCSS: function (cssList, resolve) {
                Y.Get.css(this.getComboURL(cssList), function (err) {
                    if (err) {
                        var e = new Error('ComponentResource request css error: ' + err[0].error);
                        if (M._ERRORS) {
                            M._ERRORS.push(e);
                        }
                        throw e;
                    }
                    resolve();
                });
            },
            /**
             * 加载脚本
             * @method _requestScript
             * @param {Array} modules
             * @param {Function} resolve
             */
            _requestScript: function (modules, resolve) {
                Y.use(modules, function () {
                    resolve();
                });
            },
            /**
             * 获取资源。使用延时支持资源自动合并
             * @method _getResource
             * @param {String} res
             * @param {String} type
             */
            _getResource: function (res, type) {
                var promise = new Promise(function (resolve) {
                    Resource.buffer.push({
                        res: res,
                        type: type,
                        resolve: resolve
                    });
                });

                if (this.timer) {
                    window.clearTimeout(this.timer);
                }
                this.timer = window.setTimeout(function () {
                    Resource.flush();
                }, this.DELAY);

                return promise;
            },
            /**
             * 加载 CSS
             * @method getCSS
             * @param {String} path
             * @return {Promise}
             */
            getCSS: function (path) {
                return this._getResource(path, this.TYPE_CSS);
            },
            /**
             * 加载脚本
             * @method getScript
             * @param {String} module
             * @return {Promise}
             */
            getScript: function (module) {
                return this._getResource(module, this.TYPE_SCRIPT);
            },
            /**
             * 批量加载资源
             * @method flush
             */
            flush: function () {
                var cache = this.cache,
                    cssList = [],
                    scriptList = [],
                    promiseCSS,
                    promiseScript,
                    resolveCSS,
                    resolveScript;

                promiseCSS = new Promise(function (resolve) {
                    resolveCSS = resolve;
                });
                promiseScript = new Promise(function (resolve) {
                    resolveScript = resolve;
                });

                each(this.buffer, function (atom) {
                    var res = atom.res,
                        type = atom.type;

                    if (type === Resource.TYPE_CSS) {
                        cssList.push(res);
                    } else {
                        scriptList.push(res);
                    }
                    if (!cache[res]) {
                        cache[res] = type === Resource.TYPE_CSS ? promiseCSS : promiseScript;
                    }
                    cache[res].then(atom.resolve);
                });
                this.buffer = [];

                if (cssList.length) {
                    this._requestCSS(cssList, resolveCSS);
                } else {
                    resolveCSS();
                }
                if (scriptList.length) {
                    this._requestScript(scriptList, resolveScript);
                } else {
                    resolveScript();
                }
            }
        };

        /**
         * 组件类
         * @class Component
         * @namespace mt
         */
        Y.mt.Component = Component = function (node, name) {
            this.node = node;
            node.component = this;
            this.name = name;
            this.stamp = Y.stamp(node);
            this.paused = false;
            this.destroyed = false;

            this.config = null;
            this.state = -1;

            this.publishEvent();
        };
        Component.LIFE_CYCLE = ['init', 'fetch', 'render', 'ready', 'update', 'destroy'];
        Component.LIFE_CYCLE_BOUNDARY = 4;
        // NOTE 默认组件配置，和 www 仓库 classes/Component.php 中配置同步
        Component.DEFAULT_CONFIG = {
            lazyRender: true,
            priority: 0,
            afterLoad: true
        };
        Component.EVENT_PREFIX = 'component:';
        Component.CLASS_BOOTING = 'mt-component--booting';
        Component.CLASS_BOOTED = 'mt-component--booted';
        Y.mix(Component.prototype, {
            /**
             * 声明生命周期事件
             * @method publishEvent
             */
            publishEvent: function () {
                var instance = this;
                each(Component.LIFE_CYCLE, function (phase) {
                    instance.publish(Component.EVENT_PREFIX + phase, {
                        emitFacade: true,
                        fireOnce: phase !== 'update',
                        defaultFn: function (e) {
                            this.setState(phase);
                            (this['_def' + ucfirst(phase) + 'Fn'] || noop).call(this, e);
                        }
                    });
                });
                // NOTE 只有同一 sandbox 内的 Node 节点才能接收到该事件
                // 而这一点，潜在依赖了 Y.Node 构造器中从 Cache 中获取 Node 实例的机制
                instance.addTarget(instance.node);
                // HACK 触发 Node augment EventTarget 懒执行
                instance.node.on(Component.EVENT_PREFIX + Component.LIFE_CYCLE[0], noop);
            },
            /**
             * 进入生命周期
             * @method start
             */
            start: function () {
                var node = this.node;
                if (node.hasClass(Component.CLASS_BOOTING) ||
                    node.hasClass(Component.CLASS_BOOTED)) return;

                node.addClass(Component.CLASS_BOOTING);
                this.nextSync();
            },
            /**
             * 初始化默认行为
             * @method _defInitFn
             * @private
             */
            _defInitFn: function () {
                var node = this.node,
                    decodeAttr = $Util.decodeAttr;

                this.config = Y.merge(Component.DEFAULT_CONFIG, decodeAttr(node, 'component-config'));
                this.params = decodeAttr(node, 'component-params');

                this.next();
            },
            /**
             * 加载资源默认行为
             * @method _defFetchFn
             * @private
             */
            _defFetchFn: function () {
                var instance = this,
                    resource = {},
                    next = function () {
                        instance.addListener();
                        instance.next();
                    };

                if (this.config.css) {
                    resource.css = this.config.css;
                }
                if (APP_MODULES[this.name]) {
                    resource.js = this.name;
                }

                if (Y.Object.isEmpty(resource)) {
                    next();
                } else {
                    ComponentHub.fetchResource(resource).then(next);
                }
            },
            /**
             * 渲染默认行为
             * @method _defRenderFn
             * @private
             */
            _defRenderFn: function () {
                var node = this.node,
                    config = this.config;

                if (isLazyRender(node)) {
                    node.setHTML(node.one('textarea[rel="lazyrender"]').get('value'));
                    // recursively boot internal components
                    ComponentHub.boot(node);
                } else if (config.lazyDisplay) {
                    node.show();
                }

                var component = this;

                check();

                function isLazyRender(node) {
                    var ndLazy = node.one('textarea[rel="lazyrender"]');
                    return ndLazy && ndLazy.ancestor() === node;
                }

                // 等待父组件ready, 确保父组件的scope已经初始化成功
                // 否则子组件scope上的状态将无法保证
                // 见测试用例：nested component with scope
                function check() {
                    var parent = component.getParent();
                    if (!parent || parent.isReady) {
                        component.next();
                    } else {
                        setTimeout(check, 10);
                    }
                }
            },
            /**
             * 可交互默认行为
             * @method _defReadyFn
             * @private
             */
            _defReadyFn: function () {
                Y.mt.bind.init(this.node);
                this.isReady = true;
                this.node.replaceClass(Component.CLASS_BOOTING, Component.CLASS_BOOTED);
            },
            /**
             * 数据更新默认行为
             * @method _defUpdateFn
             * @private
             */
            _defUpdateFn: function () {
                // TODO data binding
            },
            /**
             * 销毁默认行为
             * @method _defDestroyFn
             * @private
             */
            _defDestroyFn: function () {
                this.node.remove(true);
                this.destroyed = true;
            },
            /**
             * 设置状态
             * @method setState
             * @param {String} phase
             */
            setState: function (phase) {
                this.state = Y.Array.indexOf(Component.LIFE_CYCLE, phase);
            },
            /**
             * 进入下一声明周期阶段
             * @method next
             */
            next: function () {
                Y.soon(Y.bind(this.nextSync, this));
            },
            /**
             * next 的同步方法
             * @method nextSync
             */
            nextSync: function () {
                var nextState = this.state + 1,
                    phase = Component.LIFE_CYCLE[nextState];
                if (this.paused || this.destroyed) return;

                if (nextState < Component.LIFE_CYCLE_BOUNDARY) {
                    this.fire(Component.EVENT_PREFIX + phase, {
                        component: this
                    });
                }
            },
            /**
             * 添加生命周期事件监听器
             * @method addListener
             */
            addListener: function () {
                var node = this.node,
                    listeners = ComponentHub.listeners[this.name];
                if (!listeners) return;
                oeach(listeners, function (fn, phase) {
                    node.on(Component.EVENT_PREFIX + phase, fn);
                });
            },
            /**
             * 暂停生命周期
             * @method pause
             */
            pause: function () {
                this.paused = true;
            },
            /**
             * 恢复生命周期
             * @method resume
             */
            resume: function () {
                this.paused = false;
                this.next();
            },
            getParent: function () {
                var parent = this.node.ancestor('[data-component]');

                return parent ? (parent._$component || {isReady: false}) : null;
            }
        });
        Y.augment(Component, Y.EventTarget);

        // Node-Component Event Methods
        Y.Node.addMethod('fireComponent', function (el, event, data) {
            var node = this;
            if (event.indexOf(Component.EVENT_PREFIX) !== 0) {
                event = Component.EVENT_PREFIX + event;
            }
            if (node.component) {
                node.component.fire(event, data);
            } else {
                node.after(Component.EVENT_PREFIX + 'ready', function () {
                    node.component.fire(event, data);
                });
            }
        });
        Y.Node.addMethod('onComponent', function (el, event, fn) {
            subComponent(this, 'on', event, fn);
        });
        Y.Node.addMethod('afterComponent', function (el, event, fn) {
            subComponent(this, 'after', event, fn);
        });
        function subComponent(node, method, event, fn) {
            if (event.indexOf(Component.EVENT_PREFIX) !== 0) {
                event = Component.EVENT_PREFIX + event;
            }
            if (node.component) {
                node.component[method](event, Y.bind(fn, node));
            } else {
                node.on(Component.EVENT_PREFIX + 'init', function () {
                    node.component[method](event, Y.bind(fn, node));
                });
            }
        }


        /**
         * 组件管理器
         * @class ComponentHub
         * @namespace mt
         * @static
         */
        Y.mt.ComponentHub = ComponentHub = {
            /**
             * 组件容器
             * @property components
             */
            components: {},
            /**
             * 组件生命周期监听器容器
             * @property listeners
             */
            listeners: {},

            id: 0,
            /**
             * 监听组件节点生命周期方法
             * @method attach
             * @static
             * @param {String} name
             * @param {Function|Object} o
             */
            attach: function (name, o) {
                if (isFunction(o)) {
                    o = {
                        ready: o
                    };
                }
                this.listeners[name] = o;
            },
            /**
             * 将一个节点启动为一个组件，并注册其ready函数.
             * 默认仅处理有data-component属性的节点，为防止出现未知异常，
             * 要求开发指定节点，而不是替之搜寻。
             *
             * @param node
             * @param readyFn
             */
            bootNode: function (node, readyFn, force) {
                node = Y.one(node);
                if (!force && !node.hasAttribute('data-component')) {
                    throw new Error(
                        'You are attempting to manually boot a node without data-component attribute.'
                    );
                }

                var name = 'manual-boot-component-' + (++this.id);
                node.setAttribute('data-component', name);
                this.attach(name, readyFn);

                this.boot(node.ancestor());
            },
            /**
             * 自动启动所有组件
             * @method boot
             * @static
             * @param {Node} ndContainer
             */
            boot: function (ndContainer) {
                var nlComponent = (ndContainer || Y).all('[data-component]'),
                    components = ComponentHub.components,
                    immediateSet = {},
                    afterLoadSet = {};

                if (ndContainer && ndContainer.getAttribute('data-component')) {
                    nlComponent.push(ndContainer);
                }
                nlComponent.each(function (nd) {
                    if (nd._$component) return; // 如果已经booting/booted，返回
                    if (!nd.getAttribute('data-component')) return; //如果没有名字，表明这是一个manual boot的component, 返回

                    var component;
                    component = new Component(nd, nd.getAttribute('data-component'));
                    components[component.stamp] = component;
                    nd._$component = component;

                    component.after(Component.EVENT_PREFIX + 'init', function () {
                        this.pause();
                        add(this, this.config.afterLoad ? afterLoadSet : immediateSet);
                    });

                    component.start();
                });
                bootPhase(immediateSet);
                Event.load(function () {
                    bootPhase(afterLoadSet);
                });

                function add(component, set) {
                    var priority = component.config.priority;
                    if (!set[priority]) {
                        set[priority] = [];
                    }
                    set[priority].push(component);

                }

                function bootPhase(phase) {
                    each(sortKeys(phase), function (priority) {
                        var components = phase[priority];
                        Y.soon(function () {
                            each(components, function (component) {
                                component.resume();
                            });
                        });
                    });
                }

                function sortKeys(o) {
                    return keys(o).sort(function (x, y) {
                        return y - x;
                    });
                }
            },
            /**
             * 获取资源
             * @method fetchResource
             * @static
             * @param {Object} resource
             * @return {Promise}
             */
            fetchResource: function (resource) {
                var promises = [];
                if (resource.css) {
                    promises.push(Resource.getCSS(resource.css));
                }
                if (resource.js) {
                    promises.push(Resource.getScript(resource.js));
                }
                return Promise.all(promises);
            },
            /**
             * 根据节点获取组件实例
             * @method getComponentByNode
             * @static
             * @param {Node} node
             * @return {Component}
             */
            getComponentByNode: function (node) {
                return ComponentHub.components[Y.stamp(node)];
            }
        };
    },
// version
    '1.0.0',
// dependency

    {
        requires: ['mt-base', 'mt-event', 'node', 'promise', 'mt-bind']
    });


;/*jshint white:true,unused:true, curly:false, plusplus:false*/
/*global M:true*/

/**
 * 监听元素进出视口事件
 * @module mt-viewporttracker
 */
M.add('mt-viewporttracker', function (Y) {
    var W = M.window;

    var ENTER_VIEW = 'enter_view',
        DELAY = 300;

    function Sport() {
        Sport.superclass.constructor.apply(this, arguments);
    }
    Sport.NAME = 'trackedsport';
    Sport.ATTRS = {
        node: {
            value: null,
            setter: function (val) {
                return Y.one(val);
            },
            writeOnce: true
        },
        margin: {
            value: [0, 0, 0, 0],
            getter: function (margin) {
                margin = [].concat(margin);
                margin[0] = margin[0] ? margin[0] : 0;
                margin[1] = margin[1] === undefined ? margin[0] : margin[1];
                margin[2] = margin[2] === undefined ? margin[0] : margin[2];
                margin[3] = margin[3] === undefined ? margin[1] : margin[3];
                return margin;
            }
        },
        region: {
            value: null,
            getter: function (region) {
                var margin = this.get('margin'),
                    preTs = this._ts || 0,
                    ts = W.now();
                region = region || this._region;
                if (!region) {
                    region = this._refreshRegion();
                } else {
                    if (DELAY + preTs < ts) {
                        this._ts = ts;
                        region = this._refreshRegion();
                    }
                }
                region.left -= margin[0];
                region.top -= margin[1];
                region.right += margin[2];
                region.bottom += margin[3];
                return region;
            }
        },
        target: {
            value: null
        }
    };
    Y.extend(Sport, Y.Base, {
        initializer: function () {
            this._refreshRegion();
            this.publish(ENTER_VIEW);
        },
        _refreshRegion: function () {
            var node = this.get('node');
            if (node) {
                this._region = node.get('region');
                return this._region;
            } else {
                return this.get('target');
            }
        },
        check: function () {
            var sport = this;
            if (sport._enterTimer) return;

            sport._enterTimer = W.setFrameout(function () {
                if (sport.inTarget()) {
                    sport.fire(ENTER_VIEW);
                }
                sport._enterTimer = null;
            }, 20);
        },
        inTarget: function () {
            var r1 = this.get('region'),
                r2 = this.get('target');
            return r1.top < r2.bottom && r1.bottom > r2.top && r1.left < r2.right && r1.right > r2.left;
        }
    });

    var sports = [],
        viewport = null,
        isTracking = false;

    function startTracking() {
        var width = W.get('innerWidth'),
            height = W.get('innerHeight'),
            pageX = W.get('pageXOffset'),
            pageY = W.get('pageYOffset');
        viewport = {
            left: pageX,
            right: pageX + width,
            top: pageY,
            bottom: pageY + height,
            width: width,
            height: height
        };
        W.resize(function () {
            viewport.width = W.get('innerWidth');
            viewport.height = W.get('innerHeight');
            viewport.right = viewport.left + viewport.width;
            viewport.bottom = viewport.top + viewport.height;
            check();
        });
        W.scroll(function () {
            viewport.left = W.get('pageXOffset');
            viewport.top = W.get('pageYOffset');
            viewport.right = viewport.left + viewport.width;
            viewport.bottom = viewport.top + viewport.height;
            check();
        });
        isTracking = true;

        function check() {
            for (var i = sports.length - 1; i >= 0; i--) {
                sports[i].check();
            }
        }
    }

    Y.namespace('mt.ViewportTracker');
    /**
     * 元素进出视口事件监听
     * @class ViewportTracker
     * @namespace mt
     * @static
     */
    Y.mt.ViewportTracker = {
        onceenter: function (nd, viewportenter, margin) {
            var ViewportTracker = this;
            if (!isTracking) {
                startTracking();
            }
            var sport = new Sport({
                node: nd,
                margin: margin,
                target: viewport
            });
            if (sport.inTarget()) {
                viewportenter.call(sport);
            } else {
                sports.push(sport);
                sport.on(ENTER_VIEW, function () {
                    viewportenter.call(this);
                    ViewportTracker.remove(sport);
                });
            }
            return sport;
        },
        remove: function (sport) {
            for (var i = sports.length - 1; i >= 0; i--) {
                if (sport === sports[i]) {
                    sports.splice(i, 1);
                    return;
                }
            }
        }
    };
},
// version
'1.0.0',
// dependency
{
    requires: [
        'mt-window',
        'base-base',
        'node'
    ]
});

;/*jshint white:true,unused:true, curly:false, plusplus:false*/
/*global M:true*/

/**
 * BigRender
 * @module mt-bigrender
 */
M.add('mt-bigrender', function (Y) {
    var ViewportTracker = Y.mt.ViewportTracker,

        DATA_PREFIX = 'data-br-',
        CLS_TEXTAREA = 'J-bigrender-data',
        CLS_RENDERED_STAMP = 'br-rendered';

    /**
     * 按需加载文档内容
     * @class BigRender
     * @namespace mt
     * @static
     */
    var BigRender = {
        /**
         * 监听元素进入视口的事件：加载textarea中的内容
         * @method init
         * @param {Selector|Node} ndContainer 需要bigrender的texarea或包含这样的textarea的容器
         * @param {Object} [callbacks]
         * @param {Function} [callbacks.beforerender] 插入html之前执行的回调
         * @param {Function} [callbacks.afterrender] 插入html之后执行的回调
         *
         */
        init: function (ndContainer, callbacks) {
            ndContainer = Y.one(ndContainer);
            if (!ndContainer) return;

            if (ndContainer.hasClass(CLS_TEXTAREA)) {
                handleTextarea(ndContainer);
            } else {
                ndContainer.all('.' + CLS_TEXTAREA).each(handleTextarea);
            }
            function handleTextarea(ndTextarea) {
                // 监听（进入视口的）目标如果未指定，则默认为textarea的父节点
                ViewportTracker.onceenter(BigRender._getTarget(ndTextarea) || ndTextarea.ancestor(), function () {
                    BigRender.load(ndTextarea, callbacks);
                // offset将作为margin加在目标上，是否进入视口以margin box与viewport是否相交判断依据
                }, +BigRender.getData(ndTextarea, 'offset') || 0);
            }
        },
        /**
         * 直接加载textarea中的内容
         * @param {Selector|Node} ndContainer 需要bigrender的texarea或包含这样的textarea的容器
         * @param {Object} [callbacks]
         * @param {Function} [callbacks.beforerender] 插入html之前执行的回调
         * @param {Function} [callbacks.afterrender] 插入html之后执行的回调
         */
        loadImmediately: function (ndContainer, callbacks) {
            if (ndContainer.hasClass(CLS_TEXTAREA)) {
                // 指定textarea
                BigRender.load(ndContainer, callbacks);
            } else {
                // 指定容器
                ndContainer.all('.' + CLS_TEXTAREA).each(function (ndContainer) {
                    BigRender.load(ndContainer, callbacks);
                });
            }
        },
        getTarget: function (ndTextarea) {
            return BigRender._getTarget(ndTextarea) || ndTextarea.ancestor();
        },
        _getTarget: function (ndTextarea) {
            var target = BigRender.getData(ndTextarea, 'target'),
                filter;
            if (!target) return null;

            // filter: previous|ul
            if (target.indexOf('|') !== -1) {
                filter = target.split('|');
                target = ndTextarea[filter[0]](filter[1]);
            } else {
            // selector: #id
                target = Y.one(target);
            }
            return target;
        },
        getData: function (ndTextarea, name) {
            return ndTextarea.hasAttribute(DATA_PREFIX + name) ? ndTextarea.getAttribute(DATA_PREFIX + name) : null;
        },
        /**
         * 从textarea中取出内容，插入相应位置，并执行其中的js
         * @param {Node} ndTextarea
         * @param {Object} [callbacks]
         * @param {Function} [callbacks.beforerender] 插入html之前执行的回调
         * @param {Function} [callbacks.afterrender] 插入html之后执行的回调
         */
        load: function (ndTextarea, callbacks) {
            callbacks = callbacks || {};
            // 确保只执行一次。IE8下观测到有执行两遍的情况，故添加此处判断
            if (!ndTextarea || ndTextarea.hasClass(CLS_RENDERED_STAMP)) return;
            ndTextarea.addClass(CLS_RENDERED_STAMP);

            /*jshint evil:true*/

            // 内容放入div中再处理
            var content = ndTextarea.get('value'),
                ndContainer = BigRender._getTarget(ndTextarea),
                hasTarget = !!ndContainer,
                beforerender = BigRender.getData(ndTextarea, 'beforerender'),
                afterrender = BigRender.getData(ndTextarea, 'afterrender');

            if (!ndContainer) {
                ndContainer = ndTextarea.ancestor();
            }
            render();
            ndTextarea.removeClass(CLS_TEXTAREA);

            function render() {
                var callbackArgs = { 'target': ndContainer, 'content': content };
                if (beforerender) {
                    beforerender = new Function('e', beforerender);
                    if (false === beforerender.call(ndTextarea, callbackArgs)) return;
                }
                if (typeof callbacks.beforerender === 'function') {
                    if (false === callbacks.beforerender.call(ndTextarea, callbackArgs)) return;
                }

                if (hasTarget) {
                    ndContainer.appendChild(callbackArgs.content);
                } else {
                    ndContainer.insertBefore(callbackArgs.content, ndTextarea);
                }

                if (afterrender) {
                    afterrender = new Function('e', afterrender);
                    afterrender.call(ndTextarea, callbackArgs);
                }
                if (typeof callbacks.afterrender === 'function') {
                    callbacks.afterrender.call(ndTextarea, callbackArgs);
                }
            }
        }
    };
    Y.namespace('mt.BigRender');
    Y.mt.BigRender = BigRender;
},
// version
'1.0.0',
// dependency
{
    requires: [
        'mt-viewporttracker'
    ]
});

;/*jshint white:true,unused:true, curly:false, plusplus:false*/
/*global M:true*/

/**
 * 图片加载。自动懒加载，或手动懒加载
 * @module mt-imageloader
 */
M.add('mt-imageloader', function (Y) {
    var ViewportTracker = Y.mt.ViewportTracker,
        DATA_PREFIX = 'data-lazy-',
        DATA_SRC = 'data-src';

    var ImageLazyLoader = {
        /**
         * 图片lazyload
         * @method init
         * @param {Object} [config]
         * @param {Node|Selector} [config.ndContainer='body'] 需要开始lazy loading的区域
         * @param {String} [config.stampClass] 需要针对拥有哪种class的图片进行lazy loading
         * @param {Number} [config.offset] 离容器多远时加载图片
         * @param {Object} [config.on]
         * @param {Function} [config.on.request] 修改src前执行的回调，可用于修改即将设置的src。接收旧的src，返回新的src
         * @param {Function} [config.on.load] 图片的onload回调
         */
        init: function (config) {
            config = config || {};
            var stampClass = config.stampClass,
                ndContainer = config.ndContainer || 'body';
            Y.all(ndContainer).each(function (ndContainer) {
                if (ndContainer.hasAttribute(DATA_SRC)) {
                    // 指定图片节点
                    bind(ndContainer);
                } else {
                    // 指定容器
                    ndContainer.all('.' + stampClass).each(bind);
                }
            });
            function bind(ndImg) {
                ndImg.removeClass(stampClass);
                ViewportTracker.onceenter(ndImg, function () {
                    ImageLazyLoader._load(ndImg, config.on);
                }, config.offset);
            }
        },
        /**
         * 替换src，下载图片
         * @method getData
         * @param {Node} ndImg
         * @param {String} name 属性名称
         */
        getData: function (ndImg, name) {
            return ndImg.hasAttribute(DATA_PREFIX + name) ? ndImg.getAttribute(DATA_PREFIX + name) : null;
        },
        /**
         * 手动load图片
         * @param {Node|Selector} ndContainer
         * @param {Object} callbacks
         */
        loadImmediately: function (ndContainer, callbacks) {
            Y.all(ndContainer).each(function (ndContainer) {
                if (ndContainer.hasAttribute(DATA_SRC)) {
                    // 指定图片节点
                    ImageLazyLoader._load(ndContainer, callbacks);
                } else {
                    // 指定容器
                    ndContainer.all('[' + DATA_SRC + ']').each(function (ndImg) {
                        ImageLazyLoader._load(ndImg, callbacks);
                    });
                }
            });
        },
        /**
         * 替换src，下载图片
         * @method _load
         * @param {Node} ndImg
         * @param {Object} [callbacks]
         * @param {Function} [callbacks.request] 修改src前执行的回调，可用于修改即将设置的src。接收旧的src，返回新的src
         * @param {Function} [callbacks.load] 图片的onload回调
         */
        _load: function (ndImg, callbacks) {
            callbacks = callbacks || {};
            var src = ndImg.getAttribute(DATA_SRC),
                onload = callbacks.load,
                onrequest = callbacks.request,
                callbackArgs = { 'src': src };
            if (!src || ndImg.get('tagName').toLowerCase() !== 'img') return;

            // 确保不重复加载，对loadImmediately的形式尤其重要
            ndImg.removeAttribute(DATA_SRC);

            if (typeof onload === 'function') {
                ndImg.on('load', onload);
            }

            // 如有需求，可在添加支持节点上写onload属性的功能

            if (typeof onrequest === 'function') {
                if (false === onrequest.call(ndImg, callbackArgs)) return;
            }
            ndImg.setAttribute('src', callbackArgs.src);
        }
    };

    Y.namespace('mt.ImageLoader');
    Y.mt.ImageLoader = ImageLazyLoader;
},
// version
'1.0.0',
// dependency
{
    requires: [
        'mt-viewporttracker'
    ]
});

;/*jshint white:true, unused:true, curly:false, sub:true, plusplus:false, evil:true*/
/*global M*/
/**
 * ajax请求合并
 *
 * @module mt-ajax
 */
M.add('mt-ajax', function (Y) {
    var YL = Y.Lang;
    var now = YL.now;

    var MU = Y.mt.util;
    var getProp = MU.getProperty;
    var getProps = MU.getProperties;
    var Event = Y.mt.Event;

    function isHit(probability) {
        return probability && (!online || now() % 100 < (probability + 'e2'));
    }

    var jsonParse = function (str) {
        if (!str) return null;
        var ret;
        try {
            ret = Y.JSON.parse(str);
        } catch (ex) {
            return null;
        }
        return ret;
    };

    var online = !!getProp(M, 'Env.data-conf.online');
    var MTError = {
        send: function (errMsg) {
            if (!this._booted) {
                this._booted = true;
                M._ERRORS = M._ERRORS || [];
                var ravenUrl = getProp(M, 'Env.data-conf.ravenUrl');
                if (isHit(0.1) && !window.Raven && M.createAsyncScript && ravenUrl) {
                    Event.load(function () {
                        M.createAsyncScript(ravenUrl);
                    });
                }
            }
            if (errMsg) {
                M._ERRORS.push(errMsg);
            }
        },
        /**
         * 捕获错误
         * @param {Function} fn 可能出问题的函数
         * @param {Function} errorFn 出错时执行的回调：errorFn(ex)。可以在里面使用MTError.send来发送错误日志
         * @return {Function} 被修改的函数。
         *
         */
        _wrapFunction: function (fn, errorFn) {
            if (!online || fn._mtErrorWrapped) {
                return fn;
            }
            var ret = function () {
                var value;
                try {
                    value = fn.apply(this, arguments);
                } catch (ex) {
                    if (YL.isFunction(errorFn)) {
                        errorFn(ex);
                    }
                }
                return value;
            };
            ret._mtErrorWrapped = true;
            return ret;
        },
        wrap: function (fn, errorFn) {
            var E = this;
            if (YL.isFunction(fn)) {
                return E._wrapFunction(fn, errorFn);
            }
            if (fn && YL.isObject(fn, true)) {
                Y.Object.each(fn, function (v, k) {
                    if (YL.isFunction(v)) {
                        fn[k] = E._wrapFunction(v, errorFn);
                    }
                });
            }
        }
    };

    function TimeLine(startTimeStamp) {
        this.measures = {};
        this.timeStamps = {};
        this.mark(TimeLine.MARK_TIMELINE_START, startTimeStamp);
    }

    TimeLine.MARK_TIMELINE_START = '_mark_timeline_start_';

    TimeLine.now = now;

    TimeLine.isValidMeasure = function (value) {
        return value >= 0;
    };

    TimeLine.isValidTimeStamp = function (value) {
        return value > 0;
    };

    TimeLine.prototype.mark = function (markName, timeStamp) {
        this.timeStamps[markName] = TimeLine.isValidTimeStamp(timeStamp) ? timeStamp : TimeLine.now();
    };

    TimeLine.prototype.measure = function (measureName, startMark, endMark) {
        startMark = startMark || TimeLine.MARK_TIMELINE_START;
        this.setMeasure(measureName, (this.timeStamps[endMark] || TimeLine.now()) - this.timeStamps[startMark]);
    };

    TimeLine.prototype.setMeasure = function (measureName, duration) {
        if (TimeLine.isValidMeasure(duration)) {
            this.measures[measureName] = duration;
        } else {
            delete this.measures[measureName];
        }
    };

    TimeLine.prototype.getMeasure = function (measureName) {
        return this.measures[measureName];
    };

    TimeLine.prototype.getMeasures = function () {
        return Y.merge(this.measures);
    };

    function Transaction(conf) {
        var trn = this;

        trn.id = Transaction._id++;
        // state分别为EVENTS中对应index上的事件，即当前时刻该事件已发生。譬如trn.state=2表示complete事件已发生
        trn.state = 0;
        // success时设置。其值为response中的status字段，若response中无status字段，则是xhr中的status字段。
        trn.status = 0;
        trn._init(conf);
    }
    Y.augment(Transaction, Y.EventTarget, false, null, { prefix: 'mt-trn:' });
    Transaction.EVENTS = ['start', 'complete', 'success', 'failure', 'end'];
    Transaction._id = 0;
    Transaction._map = {};
    Transaction.foreachEvent = function (f) {
        Y.Array.each(Transaction.EVENTS, function (evt, state) {
            f(evt, state + 1);
        });
    };
    Transaction.create = function (conf) {
        var tag = conf.tag;
        var map = Transaction._map;
        var trn = tag && map[tag] || null;
        if (trn) {
            trn.listen(conf.on, conf['arguments']);
        } else {
            trn = new Transaction(conf);
            if (tag) {
                map[tag] = trn;
            }
        }
        return trn;
    };

    /**
     * 准备错误日志
     * @method getErrorMessage
     *
     */
    Transaction.getErrorMessage = function (cfg, extra) {
        var errMsg = '';
        cfg = getProps(cfg, ['uri', 'method', 'data', 'tag']);
        Y.Object.each(Y.merge(extra, cfg), function (v, k) {
            if (v || v === 0) {
                if (typeof v === 'object') {
                    v = Y.JSON.stringify(v);
                }
                errMsg += k + ':' + v + '\n';
            }
        });
        return errMsg;
    };

    Transaction.recordTiming = function (trn) {
        var tag = trn.tag;
        if (!tag || !window.mta) return;

        var start = getProp(window, 'performance.timing.navigationStart') || getProp(M, 'TimeTracker.start');
        if (!TimeLine.isValidTimeStamp(start)) {
            return;
        }

        var timeline = new TimeLine(start);
        timeline.mark('init');
        Transaction.foreachEvent(function (evt) {
            trn.once(evt, function () {
                timeline.mark(evt);
            });
        });

        if (!isHit(trn.pp)) return;

        var timings = {};
        trn.once('end', function () {
            timeline.measure('blocking', null, 'init');
            timeline.measure('send', 'init', 'start');
            timeline.measure('response', 'start', 'complete');
            timeline.measure('render', 'complete', 'end');
            timeline.measure('duration', 'start', 'end');
            timeline.measure('request', 'init', 'end');
            timeline.setMeasure('backend', getProp(trn, 'response.performance.backend'));
            timeline.setMeasure('latency', timeline.getMeasure('response') - timeline.getMeasure('backend'));
            timings[tag] = timeline.getMeasures();
            window.mta('timing', 'api', timings);
        });
    };

    Transaction.prototype._init = function (conf) {
        var trn = this;

        // 注册start, complete, success/failure, end事件
        Transaction.foreachEvent(function (evt, state) {
            trn.publish(evt, {
                emitFacade: true,
                fireOnce: true,
                defaultFn: function () {
                    trn.state = state;
                }
            });
        });
        // TODO:zoubin02:delete id argument of callbacks
        trn.once('start', function (e) {
            // start时存储Y.io对应的id
            this.yid = e.args[0];
        });
        // 在complete时解析response
        trn.once('complete', function (e) {
            this.setResponse(e.args[1]);
        });
        /*
        if (conf.failureRecord) {
            trn.once('failure', function () {
                var xhr = trn.xhr;
                if (!xhr) return;
                MTError.send(Transaction.getErrorMessage(trn.cfg, {
                    status: xhr.status,
                    statusText: xhr.statusText,
                    source: xhr.constructor.name
                }));
            });
        }
        */

        trn.tag = conf.tag;
        trn.cfg = getProps(conf, ['uri', 'method', 'data', 'tag']);
        trn.cfg.data = trn.cfg.data || {};
        trn.pp = conf.performancingProbability || 0;
        trn.listen(conf.on, conf['arguments']);
        Transaction.recordTiming(trn);
    };

    Transaction.prototype.addData = function (key, value) {
        this.cfg.data[key] = value;
    };

    Transaction.prototype.send = function () {
        var trn = this,
            cfg = trn.cfg,
            uri = cfg && cfg.uri,
            cbs = {};
        if (trn._done || !uri) {
            return;
        }
        trn._done = true;
        Transaction.foreachEvent(function (evt) {
            cbs[evt] = function () {
                // Y.io中的回调发生时，触发相应的自定义事件
                trn.fire(evt, { args: arguments });
            };
        });
        return Y.io(uri, {
            method: cfg.method || 'GET',
            data: cfg.data,
            on: cbs
        });
    };

    Transaction.prototype.setResponse = function (xhr) {
        if (!xhr) return null;

        var trn = this;
        if (!trn.xhr) {
            trn.xhr = xhr;
        }
        if (!trn.response) {
            trn.response = jsonParse(xhr.responseText) || {};
            if (YL.isUndefined(trn.response.status)) {
                trn.status = xhr.status;
            } else {
                trn.status = +trn.response.status || 0;
            }
        }
        return trn.response;
    };

    /**
     * 绑定事件，只支持start, complete, success/failure, end事件
     * @method listen
     * @param {Object} objEvents 回调集合对象。{ success: function () {} }
     * @param {Object} extra 各回调中可访问的数据
     */
    Transaction.prototype.listen = function (objEvents, extra) {
        if (!objEvents) {
            return;
        }
        var trn = this;
        objEvents = getProps(objEvents, Transaction.EVENTS);
        MTError.wrap(objEvents, function (ex) {
            MTError.send(ex);
        });
        // 事件有可能已经触发，需要保证start, complete, success/failure, end的执行顺序
        Transaction.foreachEvent(function (evt) {
            if (!YL.isFunction(objEvents[evt])) return;
            trn.onceAfter(evt, function () {
                var trn = this,
                    args = [trn.yid];
                if (evt === 'success' && (trn.status === 204 || trn.status === 404)) {
                    return;
                }
                // start与end时无response
                if (evt !== 'start' || evt !== 'end') {
                    args.push(trn.response);
                }
                // Y.io的第三个参数，所有回调公用的一些数据
                args.push(extra);
                M.window.setFrameout(function () {
                    objEvents[evt].apply(trn, args);
                });
            });
        });
        return trn;
    };

    function Request(cfg) {
        this.cfg = cfg;
        this.trn = Transaction.create(cfg);
        this._init();
    }
    Request.DATA_ATTR_PREFIX = 'data-comboajax-';

    Request.create = function (conf) {
        conf = Request.getValidConfig(conf);
        if (!conf) {
            return null;
        }
        return new Request(conf);
    };
    /**
     * 获取DOM节点属性
     * @method getData
     * @param {Node} node
     * @param {String} name 属性名称
     */
    Request.getData = function (node, name) {
        return node && node.getAttribute(Request.DATA_ATTR_PREFIX + name) || null;
    };
    /**
     * 设置DOM节点属性
     * @method setData
     * @param {Node} node
     * @param {String} name 属性名称
     * @param {String} value 属性值
     */
    Request.setData = function (node, name, value) {
        if (node && value) {
            node.setAttribute(Request.DATA_ATTR_PREFIX + name, value);
        }
    };
    /**
     * 生成统一格式的配置对象
     * @method getValidConfig
     * @param {Node|String|Object} conf
     * @return {Object}
     */
    Request.getValidConfig = function (conf) {
        var ret = {},
            name,
            value,
            node;

        if (!conf) {
            return null;
        }
        if (YL.isString(conf)) {
            return {
                uri: conf
            };
        }
        if (conf._node) {
            node = conf;
            // 命名空间，代表一个配置对象
            name = 'config';
            value = Request.getData(node, name);
            if (value) {
                ret = getProp(Y.mt, value);
            }
            name = 'on';
            value = Request.getData(node, name);
            if (value) {
                ret[name] = getProp(Y.mt, value);
            }
            Y.Array.each(['data', 'arguments'], function (name) {
                var value = Request.getData(node, name);
                if (value) {
                    ret[name] = jsonParse(value);
                }
            });
            Y.Array.each(['uri', 'method', 'tag'], function (name) {
                var value = Request.getData(node, name);
                if (value) {
                    ret[name] = value;
                }
            });
            ret['arguments'] = ret['arguments'] || {};
            ret['arguments'].node = node;
        } else if (typeof conf === 'object') {
            ret = Y.Object(conf);
        }

        // init函数可用于修改配置
        // 如果是修改数据，勿使用异步操作，否则发送时可能数据还没有准备好
        if (YL.isFunction(ret.init)) {
            ret.init = MTError.wrap(ret.init, function (ex) {
                MTError.send(ex);
                ret = {};
            });
            ret.init(node);
        }
        return ret.uri ? ret : null;
    };
    Request.prototype._init = function () {
        var self = this;
        var node = getProp(self, 'cfg.arguments.node');
        if (!node) {
            return;
        }
        var trn = self.trn;
        Transaction.foreachEvent(function (evt, state) {
            trn.once(evt, function () {
                Request.setData(node, 'state', state);
            });
        });
        trn.listen({
            success: function () {
                var onsuccess = Request.getData(node, 'onsuccess');
                if (onsuccess) {
                    onsuccess = new Function('$response', '$request', onsuccess);
                    onsuccess.call(node, trn.response, self);
                }
            }
        });
    };
    Request.prototype.listen = function (modules, cbNS, afterload) {
        var trn = this.trn,
            args = this.cfg['arguments'];
        if (afterload) {
            Event.load(exec);
        } else {
            exec();
        }
        function exec() {
            Y.use(modules, function (Y) {
                var callbacks = getProp(Y.mt, cbNS);
                if (!callbacks) return;
                trn.listen(callbacks, args);
            });
        }
    };
    Request.prototype.send = function () {
        this.trn.send();
    };

    function Ajax(tag, combo) {
        // 所有请求
        this.q = [];
        // 所有依赖
        this.requires = [];
        // 是否强制合并
        this.combo = combo;
        // 合并请求的标记
        this.tag = tag;
        // 所有请求的原始配置
        this.confs = [];
    }
    Ajax.combo = function (reqs, tag) {
        var creq = new Request({
            // 合并请求采集性能
            tag: tag,
            performancingProbability: 0.1,
            failureRecord: true,
            uri: '/multiact/default/' + (location.pathname || ""),
            method: 'POST'
        });
        var ctrn = creq.trn;
        var dirty = {};
        Y.Array.each(reqs, function (req) {
            var trn = req.trn;
            // reqs可能重复包含同样的数据请求
            if (dirty[trn.id]) return;
            dirty[trn.id] = true;

            // 让各事件冒泡
            ctrn.addTarget(trn);
            // 组装数据
            ctrn.addData(trn.id, Y.JSON.stringify(Ajax.getComboDataItem(trn.cfg)));
            // 响应子请求
            ctrn.once('complete', function () {
                var res = getProp(ctrn, 'response.' + trn.id);
                if (res) {
                    trn.setResponse({ responseText: res });
                }
            });
        });
        creq.send();
    };
    /**
     * 获取一个子请求的数据
     * @method getComboDataItem
     * @param {Object} conf
     * @private
     * @return {Object}
     */
    Ajax.getComboDataItem = function (conf) {
        var method = conf.method;
        var data = conf.data;
        var uri = conf.uri;
        var uriComponents = uri.split('/');
        var comboData = {};
        // uri以'/'开头，接下来是class/action
        if (uriComponents.length > 3) {
            // Class/Action
            comboData._a = (uriComponents.splice(0, 3)).join('/');
            // Action arguments
            comboData._p = uriComponents.join('/');
        } else {
            comboData._a = uri;
        }
        if (method && method.toLowerCase() === 'post') {
            comboData._m = 'post';
        }
        if (data) {
            comboData = Y.merge(data, comboData);
        }
        return comboData;
    };
    Ajax.getRequires = function (config) {
        var requires;
        if (config && config._node) {
            requires = Request.getData(config, 'requires');
            requires = requires && requires.replace(/\s+/g, '').split(',');
        } else if (config && typeof config === 'object') {
            requires = config.requires;
        }
        return requires;
    };
    /**
     * 添加一个需要combo的子请求
     * @method add
     * @param {URI|Node|Object} config 配置。
     */
    Ajax.prototype.add = function (config) {
        if (!config) return this;
        var requires = Ajax.getRequires(config);
        if (requires && requires.length) {
            this.requires = this.requires.concat(requires);
        }
        this.confs.push(config);
        return this;
    };
    Ajax.prototype.send = function () {
        var ajax = this;
        Y.use(ajax.requires, function () {
            var l;

            // 准备子请求
            Y.Array.each(ajax.confs, function (conf) {
                var req = Request.create(conf);
                if (req) {
                    ajax.q.push(req);
                }
            });

            l = ajax.q.length;
            if (!l) {
                return;
            }

            if (ajax.combo || l > 1) {
                ajax.combo = true;
                Ajax.combo(ajax.q, ajax.tag);
            } else {
                ajax.q[0].send();
            }
        });
    };

    Y.namespace('mt.Ajax');
    Y.mt.Ajax = {
        _T: Transaction,
        _R: Request,
        /**
         * 扫描元素，收集异步请求并发送
         * @method init
         * @param {Selector|Node} ndContainer
         *
         */
        init: function (ndContainer) {
            ndContainer = Y.one(ndContainer);
            if (!ndContainer) return;
            this.create(ndContainer.all('[' + Request.DATA_ATTR_PREFIX + 'state]'), 'multiact-init', true)
                .add(ndContainer)
                .send();
        },
        create: function (configs, tag, combo) {
            var ajax = new Ajax(tag, combo);
            if (Y.Lang.isArray(configs) || configs && configs._nodes) {
                Y.each(configs, ajax.add, ajax);
            } else {
                ajax.add(configs);
            }
            return ajax;
        },
        /**
         * 将指定的子请求合并发走。如果只有一个请求，则不走合并逻辑，直接发走
         * @method io
         * @param {uri|Node|Object|NodeList|Array} config
         * @param {String} tag 可用于采集合并请求的性能数据
         *
         */
        io: function (config, tag) {
            Y.mt.Ajax.create(config, tag).send();
        }
    };
},
// version
'1.0.0',

// dependency
{
    requires: [
        'io-base',
        'mt-window',
        'mt-event',
        'mt-base'
    ]
});

;/*jshint white:true,unused:true*/
/**
 * 简单的下拉菜单
 * @module uix-dropdown
 */
M.add('uix-dropdown', function (Y) {
    /**
     * 简单的下拉菜单,目前只支持hover触发
     * @param {Node} ndContainer 弹层容器
     * @param {Object} config 配置选项。
     * @example
        <div data-uix="dropdown" data-params='{"delay": 200}' class="dropdown dropdown--open">
            <a class="J-dropdown-trigger">指定trigger</a>
            <ul class="dropdown-menu">
            </ul>
        </div>

        <div data-uix="dropdown" data-params='{"delay": 200}' class="dropdown dropdown--open">
            <a>这里是将整个容器当作trigger</a>
            <ul class="dropdown-menu">
            </ul>
        </div>
     */
    function Dropdown(ndContainer, config) {
        var selector = '.J-uix-dropdown-trigger';
        var containerClass = 'dropdown--open';
        var transition = {
            mouseenter: 100,
            mouseleave: 100
        };

        if (config) {
            if (config.classname) {
                containerClass = config.classname;
            }

            if (config.delay) {
                transition = {
                    mouseenter: config.delay,
                    mouseleave: config.delay
                };
            }
        }

        Y.mt.Event.Hover.listen(ndContainer, function () {
            ndContainer.addClass(containerClass);
        }, function () {
            ndContainer.removeClass(containerClass);
        }, transition, ndContainer.one(selector) && selector);
    }

    Y.namespace('mt.uix');
    Y.mt.uix.Dropdown = Dropdown;

}, '1.0.0', { requires: ['mt-event-hover', 'mt-base']});


;/*jshint white:true,unused:true*/
/**
 * Node节点的placeholder插件
 * @module p-node-placeholder
 */
M.add('p-node-placeholder', function (Y) {
    var isIE =  Y.UA.ie,
        trim = Y.Lang.trim;

    var $Util = Y.mt.util,
        ATTR_INPUT_TYPE = "data-placeholder-type",
        changeType = function (el, type) {
            try {
                // IE6/7 not work
                el.type = type;
                return true;
            } catch (e) {
                return false;
            }
        };

    /**
     * Node节点的placeholder插件
     * @class Placeholder
     * @namespace mt.plugin
     * @constructor
     * @extends Plugin.Base
     * @param config {Object}
     */
    function Placeholder() {
        this.handler = {
            submit: null
        };
        Placeholder.superclass.constructor.apply(this, arguments);
    }
    /**
     * @property NAME
     * @type String
     */
    Placeholder.NAME = 'placeholderPlugin';
    /**
     * @property NS
     * @type String
     */
    Placeholder.NS = 'placeholder';
    Placeholder.ATTRS = {
        /**
         * 配置信息
         * @attribute placeholder
         * @type Object
         */
        placeholder: {
            valueFn: '_getPlaceholderValue',
            readOnly: true
        }
    };
    Placeholder.PLACEHOLDER_CLASS = 'placeholder';

    Y.extend(Placeholder, Y.Plugin.Base, {
        /**
         * 初始化
         * @method initializer
         * @private
         */
        initializer: function () {
            var plugin = this,
                host = plugin.get('host'),
                ndForm;

            if (host) {
                ndForm = host.ancestor('form');
            }
            if (!(host instanceof Y.Node) || $Util.isSupportPlaceholder()) return;

            plugin.onHostEvent('focus', plugin.hide);
            plugin.onHostEvent('blur', function () {
                plugin.show(false);
            });
            if (ndForm) {
                // NOTE Submit失败后所有placeholder都被清除，没有恢复，需要手动调用show
                plugin.handler.submit = ndForm.on('submit', function () {
                    plugin.hide();
                });
            }
            if (isIE) {
                // 刷新页面时，IE会在页面load时设置表单记忆值
                $Util.addLoadListener(function () {
                    if (host.get('value')) {
                        host.removeClass(Placeholder.PLACEHOLDER_CLASS);
                    }
                    plugin.show(true);
                });
            }

            plugin.show(true);
        },
        /**
         * 销毁时执行
         * @method destructor
         * @private
         */
        destructor: function () {
            this.hide();
            if (this.handler.submit) {
                this.handler.submit.detach();
            }
        },
        /**
         * 显示placeholder
         * @method show
         * @param {Boolean} loading
         */
        show: function (loading) {
            var host = this.get('host'),
                el = host.getDOMNode(),
                placeholder = this.get('placeholder'),
                type;

            if (host.get('value') === '' || (loading && this._isValueEqualPlaceholder())) {
                // password polyfill
                type = host.getAttribute(ATTR_INPUT_TYPE);
                if (type) {
                    el.type = 'text';
                } else if (host.get('type') === 'password') {
                    if (!changeType(el, 'text')) return;
                    host.setAttribute(ATTR_INPUT_TYPE, 'password');
                }
                host.set('value', placeholder);
                host.addClass(Placeholder.PLACEHOLDER_CLASS);
            }
        },
        /**
         * 隐藏placeholder
         * @method hide
         */
        hide: function () {
            var host = this.get('host'),
                type;
            if (this._isValueEqualPlaceholder()) {
                // password polyfill
                type = host.getAttribute(ATTR_INPUT_TYPE);
                if (type) {
                    host.set('type', type);
                }
                this._removePlaceholder();
                host.removeClass(Placeholder.PLACEHOLDER_CLASS);
            }
        },
        /**
         * 判断host的值是否等于placeholder
         * @method _isValueEqualPlaceholder
         * @private
         * @return {Boolean}
         */
        _isValueEqualPlaceholder: function () {
            return this.get('host').get('value') === this.get('placeholder');
        },
        /**
         * 删除host的值中可能包含的placeholder
         * @method _removePlaceholder
         * @private
         */
        _removePlaceholder: function () {
            var host = this.get('host'),
                placeholder = this.get('placeholder'),
                value = trim(host.get('value')),
                placeholderLength = placeholder.length,
                inputLength = value.length - placeholderLength,
                i,
                str;

            if (placeholder === value) {
                host.set('value', '');
                return;
            }

            // 处理浏览器未及时清理placeholder的情况
            if (value.indexOf(placeholder) === 0) {
                host.set('value', value.substring(placeholderLength));
                return;
            }
            if (value.lastIndexOf(placeholder) === inputLength) {
                host.set('value', value.substring(0, inputLength));
                return;
            }
            for (i = 1; i < (placeholderLength - 1); ++i) {
                str = value.substring(0, i) + value.substring(i + inputLength);
                if (str === placeholder) {
                    host.set('value', value.substring(i, i + inputLength));
                    return;
                }
            }
        },
        /**
         * 获取host的placeholder值
         * @method _getPlaceholderValue
         * @private
         * @return {String}
         */
        _getPlaceholderValue: function () {
            return this.get('host').getAttribute('placeholder');
        }
    });

    Y.namespace('mt.plugin').Placeholder = Placeholder;
},
//version
'1.0.0',
//dependency
{
    requires: ['mt-base', 'plugin']
});
 

;/*jshint white:true*/
/**
 * 节点动画插件模块
 * @module p-node-effect
 */
M.add('p-node-effect', function (Y) {
    var $N = Y.Node;
    var isStr = Y.Lang.isString;
    var isNumber = Y.Lang.isNumber;
    var isArray = Y.Lang.isArray;
    var isFunction = Y.Lang.isFunction;
    var $UIUtil = Y.mt.UIUtil;

    // 速度别名映射
    var SPEED_MAP = {
            'slow':     600,
            'middle':   400,
            'fast':     200
        },
        // 颜色范围
        COLOR_RANGE = {
            yellow: {
                from:   '#FFF478',
                to:     '#FFFFFF'
            }
        },
        BACKGROUND_COLOR = 'backgroundColor',
        POSITION = 'position',
        EFFECT_START = 'Effect.start',
        EFFECT_END = 'Effect.end';

    /**
     * 动态效果插件类
     * @class Effect
     * @constructor
     * @namespace mt.plugin
     * @extends Plugin.Base
     * @param {Object} config 配置
     */
    function Effect(config) {
        this._running = 0;
        Effect.superclass.constructor.apply(this, arguments);
    }
    /**
     * 类名，用来作为事件前缀
     * @property NAME
     * @static
     * @type {String}
     * @default 'MTMap'
     */
    Effect.NAME = 'effectPlugin';
    /**
     * 命名空间，供宿主调用
     * @property NS
     * @static
     * @type {String}
     * @default 'effect'
     */
    Effect.NS = 'effect';
    /**
     * 属性配置
     * @property ATTRS
     * @static
     * @type {Object}
     * @default {}
     */
    Effect.ATTRS = {};

    Y.extend(Effect, Y.Plugin.Base, {
        // PROTOTYPE METHODS
        /**
         * 初始化
         * @method initializer
         */
        initializer: function () {
            var plugin = this,
                ndHost = plugin.get('host');
            if (!(ndHost instanceof Y.Node)) return;

            ndHost.publish(EFFECT_START, {
                emitFacade: true,
                defaultFn: function () {
                    plugin._running++;
                }
            });
            ndHost.publish(EFFECT_END, {
                emitFacade: true,
                defaultFn: function () {
                    plugin._running--;
                }
            });
        },
        /**
         * 析构
         * @method destructor
         */
        destructor: function () {
        },
        /**
         * 简单淡入
         * @method fadeInSimple
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         * @param {String|Array} [color] 颜色渐变
         * @example  nd.effect.fadeInSimple('middle', null, 'yellow');<br />
         *           nd.effect.fadeInSimple(300, onEnd, ['#FFF', '#000']);<br />
         *           nd.effect.fadeInSimple();
         */
        fadeInSimple: function (speed, callback, color) {
            var plugin = this,
                ndHost = plugin.get('host'),
                hostAnim;

            speed = plugin._getSpeed(speed);

            ndHost.setStyle('opacity', 0).show();
            hostAnim = plugin._fade(ndHost, 1, speed, callback, color, {
                easing: Y.Easing.easeIn
            });

            plugin._run(hostAnim);
        },
        /**
         * 淡入
         * 首先扩展空间，然后淡入显示节点内容
         * @method fadeIn
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         * @param {String|Array} [color] 颜色渐变，example: 'yellow', ['#FFF', '#000']
         */
        fadeIn: function (speed, callback, color) {
            var plugin = this,
                ndHost = plugin.get('host'),
                dimensions = $UIUtil.getDimensions(ndHost),
                ndLayout = plugin._cloneLayout(ndHost, 'div'),
                cloneAnim,
                hostAnim;

            speed = plugin._getSpeed(speed);
            ndLayout.setStyles({
                'height': 0,
                'display': 'block'
            });
            ndHost.replace(ndLayout);

            hostAnim = plugin._fade(ndHost, 1, speed, callback, color, {
                easing: Y.Easing.easeIn
            });
            cloneAnim = new Y.Anim({
                node: ndLayout,
                to: { height: dimensions.marginTop + dimensions.offsetHeight + dimensions.marginBottom },
                duration: speed * 0.6,
                easing: Y.Easing.easeIn
            });
            cloneAnim.on('end', function () {
                plugin._setPosition(ndHost);
                ndHost.setStyle('opacity', 0).show();
                ndLayout.replace(ndHost);
            });

            plugin._run(cloneAnim, hostAnim);
        },
        /**
         * 简单淡出
         * @method fadeOutSimple
         * @param {Boolean} [isDestroy] 是否销毁节点
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         * @param {String|Array} [color] 颜色渐变，example: 'yellow', ['#FFF', '#000']
         */
        fadeOutSimple: function (isDestroy, speed, callback, color) {
            var plugin = this,
                ndHost = plugin.get('host'),
                hostAnim,
                cb = function () {
                    ndHost.hide();
                    if (isFunction(callback)) {
                        callback.call(ndHost);
                    }
                    if (isDestroy) {
                        ndHost.remove(true);
                    }
                };

            speed = plugin._getSpeed(speed);

            hostAnim = plugin._fade(ndHost, 0, speed, cb, color, {
                easing: Y.Easing.easeOut
            });
            ndHost.show();

            plugin._run(hostAnim);
        },
        /**
         * 淡出
         * 首先淡出节点显示，然后收缩空间
         * @method fadeOut
         * @param {Boolean} [isDestroy] 是否销毁节点
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         * @param {String|Array} [color] 颜色渐变，example: 'yellow', ['#FFF', '#000']
         */
        fadeOut: function (isDestroy, speed, callback, color) {
            var plugin = this,
                ndHost = plugin.get('host'),
                oldOpacity = ndHost.getStyle('opacity'),
                dimensions = $UIUtil.getDimensions(ndHost),
                ndLayout = plugin._cloneLayout(ndHost, 'div'),
                cloneAnim,
                hostAnim;

            speed = plugin._getSpeed(speed);
            ndLayout.setStyles({
                'height': dimensions.marginTop + dimensions.offsetHeight + dimensions.marginBottom,
                'display': 'block'
            });
            plugin._setPosition(ndHost);

            hostAnim = plugin._fade(ndHost, 0, speed, null, color, {
                easing: Y.Easing.easeOut
            });
            hostAnim.on('end', function () {
                ndHost.replace(ndLayout);
            });
            cloneAnim = new Y.Anim({
                node: ndLayout,
                to: { height: 0 },
                duration: speed * 0.6,
                easing: Y.Easing.easeOut
            });
            cloneAnim.on('end', function () {
                ndHost.setStyles({
                    'display': 'none',
                    'opacity': oldOpacity
                });
                ndLayout.replace(ndHost);
                if (isFunction(callback)) {
                    callback.call(ndHost);
                }
                if (isDestroy) {
                    ndHost.destroy(true);
                }
            });

            plugin._run(hostAnim, cloneAnim);
        },
        /**
         * 黄色淡出
         * @method yellowFadeOut
         * @param {Boolean} [isDestroy] 是否销毁节点
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         */
        yellowFadeOut: function (isDestroy, speed, callback) {
            this.fadeOut(isDestroy, speed, callback, 'yellow');
        },
        /**
         * 黄色淡入
         * @method yellowFadeIn
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         */
        yellowFadeIn: function (speed, callback) {
            this.fadeIn(speed, callback, 'yellow');
        },
        /**
         * 淡入淡出效果公共方法
         * @method _fade
         * @private
         * @param {Node} nd 节点
         * @param {Number} to 要达到的opacity值
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         * @param {String|Array} [color] 颜色渐变，example: 'yellow', ['#FFF', '#000']
         * @param {Object} [config]
         * @return {Object} 动画对象
         */
        _fade: function (nd, to, speed, callback, color, config) {
            var colorRange,
                anim,
                f,
                t,
                animConfig = {
                    node: nd,
                    from: {opacity: 0},
                    to: { opacity: to },
                    duration: speed,
                    easing: Y.Easing.easeNone
                };

            if (color) {
                if (color in COLOR_RANGE) {
                    colorRange = COLOR_RANGE[color];
                    f = colorRange.from;
                    t = colorRange.to;
                } else if (isArray(color) && color.length === 2) {
                    f = color[0];
                    t = color[1];
                }
                animConfig.from[BACKGROUND_COLOR] = f;
                animConfig.to[BACKGROUND_COLOR] = t;
            }
            Y.mix(animConfig, config || {}, true);
            anim = new Y.Anim(animConfig);

            anim.on('end', function () {
                if (isFunction(callback)) {
                    callback.call(nd);
                }
            });

            return anim;
        },
        /**
         * 滑入
         * @method slideDown
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         */
        slideDown: function (speed, callback) {
            var plugin = this,
                ndHost = plugin.get('host'),
                dimensions = $UIUtil.getDimensions(ndHost),
                layoutAnim;

            ndHost.setStyles({
                height: 0,
                paddingTop: 0,
                paddingBottom: 0,
                marginTop: 0,
                marginBottom: 0
            });
            ndHost.setStyle('display', 'block');

            speed = plugin._getSpeed(speed);
            layoutAnim = new Y.Anim({
                node: ndHost,
                to: {
                    height: dimensions.offsetHeight,
                    paddingTop: dimensions.paddingTop,
                    paddingBottom: dimensions.paddingBottom,
                    marginTop: dimensions.marginTop,
                    marginBottom: dimensions.marginBottom
                },
                duration: speed,
                easing: Y.Easing.easeOut
            });
            layoutAnim.on('end', function () {
                if (isFunction(callback)) {
                    callback.call(ndHost);
                }
            });

            plugin._run(layoutAnim);
        },
        /**
         * 滑出
         * @method slideUp
         * @param {Boolean} [isDestroy] 是否销毁节点
         * @param {String|Number} [speed] 速度，单位ms
         * @param {Function} [callback] 动画完成时回调函数
         */
        slideUp: function (isDestroy, speed, callback) {
            var plugin = this,
                ndHost = plugin.get('host'),
                dimensions = $UIUtil.getDimensions(ndHost),
                ndLayout = plugin._cloneLayout(ndHost, 'div'),
                layoutAnim;

            ndHost.replace(ndLayout);
            ndLayout.setStyles({
                height: dimensions.marginTop + dimensions.offsetHeight + dimensions.marginBottom,
                position: 'relative'
            }).appendChild(ndHost);

            speed = plugin._getSpeed(speed);
            layoutAnim = new Y.Anim({
                node: ndLayout,
                to: { height: 0 },
                duration: speed,
                easing: Y.Easing.easeIn
            });
            layoutAnim.on('end', function () {
                ndHost.hide().unwrap();
                if (isFunction(callback)) {
                    callback.call(ndHost);
                }
                if (isDestroy) {
                    ndHost.destroy(true);
                }
            });

            plugin._run(layoutAnim);
        },
        /**
         * 滚动效果 TODO 增加滚动方向
         * @method roll
         * @param {Selector|NodeList|HTMLCollection} nlRoll 滚动节点集合
         * @param {Number} speed 速度，单位ms
         * @param {Number} delay 等待时间，单位ms
         */
        roll: function (nlRoll, speed, delay) {
            var plugin = this,
                ndHost = plugin.get('host'),
                getDimensions = $UIUtil.getDimensions,
                rollHeight = 0,
                pause = false,
                size,
                index = 0,
                ndCurrent,
                ndNext,
                curAnim,
                nextAnim;

            if (isStr(nlRoll)) {
                nlRoll = ndHost.all(nlRoll);
            }
            if (!nlRoll) return;
            size = nlRoll.size();
            if (size < 2) return;

            nlRoll.each(function (nd) {
                var height = getDimensions(nd).offsetHeight;
                if (height > rollHeight) {
                    rollHeight = height;
                }
            });
            speed = plugin._getSpeed(speed);

            setStyles();

            ndHost.on({
                mouseover: function () { pause = true; },
                mouseout: function () { pause = false; }
            });
            window.setTimeout(start, delay);
            curAnim = new Y.Anim({
                node: ndCurrent,
                to: { top: -rollHeight },
                duration: speed,
                easing: Y.Easing.easeBoth
            });
            nextAnim = new Y.Anim({
                node: ndNext,
                to: { top: 0 },
                duration: speed,
                easing: Y.Easing.easeBoth
            });

            function start() {
                if (!pause) {
                    ndCurrent = nlRoll.item(index % size);
                    ndNext = nlRoll.item((index + 1) % size);
                    index = (index + 1) % size;
                    scrolling();
                }
                window.setTimeout(start, delay);
            }

            function scrolling() {
                ndNext.setStyle('top', rollHeight + 1);
                curAnim.set('node', ndCurrent).run();
                nextAnim.set('node', ndNext).run();
            }

            function setStyles() {
                ndHost.setStyles({
                    position: 'relative',
                    height: rollHeight,
                    overflow: 'hidden'
                });

                nlRoll.each(function (ndRoll, index) {
                    ndRoll.setStyles({
                        position: 'absolute',
                        display: '',
                        top: index === 0 ? 0 : rollHeight + 1
                    });
                });
            }
        },
        /**
         * 是否在动画中
         * @method isInAnimation
         * @return {Boolean}
         */
        isInAnimation: function () {
            return this._running !== 0;
        },
        /**
         * 计算动画速度
         * @method _getSpeed
         * @private
         * @param {String|Number} [speed] 速度
         * @return {Number} 动画速度，单位为s
         */
        _getSpeed: function (speed) {
            var ret = SPEED_MAP.middle;
            if (isStr(speed) && SPEED_MAP[speed]) {
                ret = SPEED_MAP[speed];
            } else if (isNumber(speed)) {
                ret = speed;
            }

            return ret / 1000;
        },
        /**
         * 克隆一个只占用布局的节点
         * @method _cloneLayout
         * @private
         * @param {Node} node 待克隆的节点
         * @return {Node} 克隆节点
         */
        _cloneLayout: function (node, tagName) {
            var ndLayout;
            tagName = tagName || node.get('tagName');

            ndLayout = $N.create('<' + tagName + ' class="effect-assistance"></' + tagName + '>');
            ndLayout.setStyles({
                'height': 0,
                'margin': 0,
                'border': 'none',
                'padding': 0,
                'background': 'none',
                'overflow': 'hidden'
            });
            return ndLayout;
        },
        /**
         * 设置节点position属性
         * @method _setPosition
         * @private
         * @param {Node} node 节点
         */
        _setPosition: function (node) {
            var pos = node.getStyle(POSITION);
            if (pos === 'static') {
                node.setStyle(POSITION, 'relative');
            }
        },
        /**
         * 顺序执行动画
         * @method _run
         * @private
         */
        _run: function () {
            var ndHost = this.get('host'),
                i, len = arguments.length,
                first, last;
            if (len === 0) return;

            first = arguments[0];
            last = arguments[len - 1];
            for (i = 1; i < len; i++) {
                bridgeAnim(arguments[i - 1], arguments[i]);
            }
            first.on('start', function () {
                ndHost.fire(EFFECT_START);
            });
            last.on('end', function () {
                ndHost.fire(EFFECT_END);
            });

            first.run();

            // 桥接两个动画
            function bridgeAnim(ahead, behind) {
                ahead.after('end', function () {
                    behind.run();
                });
            }
        }
    });

    Y.namespace('mt.plugin').Effect = Effect;
},
//version
'1.0.0',
//dependency
{
    requires: [
        'mt-base',
        'mt-uiutil',
        'anim',
        'plugin',
        'node'
    ]
});
 

;/*jshint white:true, unused:true, curly:false, latedef:false, nonew:false, plusplus:false, sub:true*/
/*global M*/
/**
 * 主站基础模块
 *
 * @module www-base
 */
M.add('www-base', function (Y) {
    var $IO = Y.io,
        isIE =  Y.UA.ie;

    var $Util = Y.mt.util,
        $Cookie = $Util.Cookie;

    Y.namespace('mt.www');
    /**
     * Base for www.meituan.com
     * @class www
     * @namespace Y.mt
     * @static
     */
    Y.mt.www = {
        /**
         * 初始化
         * @method init
         */
        init: function () {
            var $Www = Y.mt.www;
            Y.mt.Ajax.init('body');
            $Www.BigRender.init('.J-br');
            $Www.CartEx.init();
            $Www.ImageLazyLoader.init('body');
            Y.mt.widget.init();
            // 导航栏触发统一登录弹窗
            $Www.initNavBarUnitiveLogin();

            M.load(function () {
                Y.use('www-base', function () {
                    $Www.loadLazyIcon();
                    $Www.initFavorite();
                    $Www.removeUnionCookie();
                });

                // 缩放检测
                $Www.DetectZoom.init();
            });

            // intialize hub
            Y.use("www-hub", function (Y) {
                Y.mt.www.Hub.init();
            });
        },
        History: {
            /**
             * 导航栏最近浏览
             */
            nav: {
                /**
                 * 初始化导航栏最近浏览
                 * @method init
                 *
                 */
                init: function (ndContainer) {
                    var ndHistory = ndContainer.one('#J-my-history-toggle'),
                        CLASS_DROPDOWN_EMPTY = 'dropdown-menu--empty',
                        CLASS_DROPDOWN_LOADING = 'dropdown-menu--loading',
                        ndArea = ndContainer.one('#J-my-history-menu'),
                        h;

                    ndArea.delegate('click', function (e) {
                        e.halt();
                        $Util.Cookie.remove('rvd', '/', '.' + M.DOMAIN_HOST);
                        ndArea.setHTML('<p class="' + CLASS_DROPDOWN_EMPTY + '">暂无浏览记录</p>');
                        ndContainer.removeClass('dropdown--open').removeClass('dropdown--open-history');
                    }, '#J-clear-my-history');
                    h = ndHistory.on('mouseover', function () {
                        if (!Y.mt.www.History.nav._html) {
                            if (!ndArea.hasClass(CLASS_DROPDOWN_LOADING)) {
                                ndArea.addClass(CLASS_DROPDOWN_LOADING);
                            }
                        } else {
                            ndArea.setHTML(Y.mt.www.History.nav._html).removeClass(CLASS_DROPDOWN_LOADING);
                            h.detach();
                        }
                    });
                },
                on: {
                    success: function (id, response) {
                        Y.mt.www.History.nav._html = response.html;
                    }
                }
            },
            /**
             * 侧边栏最近浏览
             */
            side: {
                /**
                 * 初始化导航栏最近浏览
                 * @method init
                 *
                 */
                init: function (ndWrapper) {
                    // 清空浏览记录
                    ndWrapper.delegate('click', function () {
                        ndWrapper.one('.J-history-list').remove();
                        ndWrapper.append('<div class="no-history">暂无浏览记录</div>');
                        $Cookie.remove('rvd', '/', '.' + M.DOMAIN_HOST);
                    }, '.J-clear');
                },
                on: {
                    success: function (id, res, args) {
                        var html = res.html,
                            ndWrapper = args.node;
                        ndWrapper.setHTML(html);
                        ndWrapper.show();
                        M.load(function () {  // sticky enhancement postpone to load
                            var refNode = Y.one('.J-mall') || Y.one('#content');
                            if (refNode) {
                                var wrapperBottomY = ndWrapper.getY() + ndWrapper.get('offsetHeight'),
                                    refBottomY = refNode.getY() + refNode.get('offsetHeight'),
                                    ref = refNode;

                                Y.use('p-node-sticky', function (Y) {
                                    if (wrapperBottomY > refBottomY){    //判断下边界是否位于滑动框的下面
                                        refNode = Y.one('#bd');
                                        refBottomY = refNode.getY() + refNode.get('offsetHeight') - 65;
                                        ref =  refBottomY;
                                    }

                                    ndWrapper.plug(Y.mt.plugin.Sticky, { bottomEdge: ref, isStaticBottomEdge: false});
                                });
                            }
                        });
                    }
                }
            }
        },
        /**
         * 导航栏登录按钮改成统一登录弹窗
         */
        initNavBarUnitiveLogin: function () {
            var selector = '.J-unitive-login-trigger',
                loginDialog,
                qs,
                ndNavBar = Y.one('.site-mast__user-nav');

            if (ndNavBar) {
                ndNavBar.delegate('click', function (e) {
                    e.halt();
                    qs = this.get('href').match(/\?(\S*)/)[1];
                    if (!loginDialog) {
                        Y.use('www-dialog', function (Y) {
                            loginDialog = Y.mt.www.Dialog.login({
                                plainQueryString: qs
                            });
                        });
                    } else {
                        loginDialog.open();
                    }
                }, selector);
            }
        },
        BigRender: {
            /**
             * @param ndContainer {Selector | Node}
             */
            init: function (ndContainer) {
                var instance = this;
                Y.all(ndContainer).each(function (ndContainer) {
                    Y.mt.BigRender.init(ndContainer, instance.on);
                });
                Y.mt.www.Webp.autoFallback(ndContainer);
            },
            on: {
                beforerender: function (e) {
                    e.content = Y.mt.www.Webp.getWebpContent(e.content);
                }
            }
        },
        ImageLazyLoader: {
            /**
             * @param ndContainer {Selector | Node}
             */
            init: function (ndContainer) {
                Y.mt.ImageLoader.init({
                    ndContainer: ndContainer,
                    stampClass: 'J-lazy-img',
                    offset: 400,
                    on: this.on
                });
                Y.mt.www.Webp.autoFallback(ndContainer);
            },
            /**
             * @param ndContainer {Selector | Node}
             */
            loadImmediately: function (ndContainer) {
                Y.mt.ImageLoader.loadImmediately(ndContainer, this.on);
                Y.mt.www.Webp.autoFallback(ndContainer);
            },
            on: {
                request: function (e) {
                    var WEBP_CLASS = 'J-webp';
                    e.src = this.hasClass(WEBP_CLASS) ? Y.mt.www.Webp.getWebpSrc(e.src) : e.src;
                    
                    // http://wiki.sankuai.com/pages/viewpage.action?pageId=371002282 为了处理 hotel-imgbox 中加 @PC
                    if (this.hasClass('J-pc')) {
                        e.src  += '@PC';
                        e.src = e.src.replace(/(.*?)@(.*)@PC$/g, '$1' + '@PC_' + '$2');
                    }
                },
                load: function () {
                    this.removeClass('lazy-img');
                }
            }
        },
        /**
         * mtevent属性生成js版，对应php版见TemplateHelper::getMTEventAttr
         * @param{Object} data 各个属性键值对
         * @param{String} [type='trigger'] 生成的元素属性类型，'trigger'表示添加属性data-mtevent，'data'表示添加属性data-mteventnd，前者会触发事件绑定，后者用于分层属性继承
         */
        getMTEventAttr: function (data, type) {
            type = type || 'trigger';
            if (!data) return '';
            var ret = type === 'trigger' ? ' data-mtevent' : ' data-mteventnd';
            return ret + '=\'' + Y.JSON.stringify(data) + '\'';
        },
        /**
         * @description 商圈浮层
         */
        showGeoMask: function (ndContainer, itemClass) {
            itemClass = itemClass || '.item';
            Y.one(ndContainer).delegate('hover', function () {
                var geo = this.one('.trade-geo');
                if (!geo) return;
                geo.addClass('geo-hover');
            }, function () {
                var geo = this.one('.trade-geo');
                if (!geo) return;
                geo.removeClass('geo-hover');
            }, itemClass);
        },
        /**
         * @description load新页面时滚动文档到合适位置
         */
        storeScrollInfo: function (ndWrapper) {
            ndWrapper = Y.one(ndWrapper);
            if (!ndWrapper) return;

            var w = M.window,
                expires = $Util.Cookie.getExpiresDate(0, 0, 10),
                scrollCookie = 'dsy',
                scrollHelpTh = 50,
                currentScrollY = w.get('pageYOffset'),
                previousScrollY = $Util.Cookie.get(scrollCookie);
            if (previousScrollY > 0) {
                if (previousScrollY - currentScrollY > scrollHelpTh) {
                    window.scrollTo(w.get('pageXOffset'), previousScrollY);
                }
                $Util.Cookie.remove(scrollCookie, '/', '.' + M.DOMAIN_HOST);
            }
            ndWrapper.delegate('click', function () {
                var scrollY = w.get('pageYOffset'),
                    target = this.getAttribute('target');
                if (target === '_blank') return;
                $Util.Cookie.set(scrollCookie, scrollY, expires, '/', '.' + M.DOMAIN_HOST);
            }, 'a');
        },
        /**
         * 初始化幻灯片
         *
         * @param {Selector|Node} srcNode 幻灯片容器
         */
        initRecoSlides: function (srcNode, type, config) {
            var ndSlides = Y.one(srcNode);
            if (!ndSlides) return;

            var params = {
                    srcNode: ndSlides,
                    effect: 'slideHorizontal',
                    duration: 400,
                    lazyloading: true,
                    pnAutoHide: true,
                    sheetContainer: ndSlides.one('.reco-slides__slides')
                },
                slider;
            if (config) {
                Y.mix(params, config, true);
            }
            if (/\bcircle\b/.test(type)) {
            // 点显示
                params.triggerContainer = ndSlides.one('.reco-slides__switch');
            } else if (/\bnumber\b/.test(type)) {
            // 显示数字
                params.triggerContainerEx = ndSlides.one('.reco-slides__tips');
            }
            if (/\bblacksquare\b/.test(type)) {
            // 黑色块
                params.previous = ndSlides.one('.reco-slides__roll--blacksquare--previous');
                params.next = ndSlides.one('.reco-slides__roll--blacksquare--next');
            } else if (/\bgreyrect\b/.test(type)) {
            // 灰色块
                params.previous = ndSlides.one('.reco-slides__roll--greyrect--previous');
                params.next = ndSlides.one('.reco-slides__roll--greyrect--next');
                params.pnAutoHide = false;
            } else if (/\bgradientrect\b/.test(type)) {
            // 黑色渐变
                params.previous = ndSlides.one('.reco-slides__roll--gradientrect--previous');
                params.next = ndSlides.one('.reco-slides__roll--gradientrect--next');
            }
            slider = new Y.mt.widget.Slider(params);
            slider.render();
            return slider;
        },
        /**
         * @description 监控用户翻页情况以及页面是否有链接点击
         * @method trackPageScroll
         */
        trackPageScroll: function (la) {
            var w = M.window,
                ca = 'UserAction',
                ac = 'Scroll',
                ndBody = Y.one(document.body),
                viewScrollInfo = {
                    vh: 0,
                    vt: w.get('pageYOffset'),
                    dh: 0,
                    clk: 0 // 页面的链接是否有点击
                };
            w.scroll(handleScroll, 6);

            var handle = ndBody.delegate('click', function () {
                viewScrollInfo.clk = 1;
                handle.detach();
            }, 'a');

            $Util.addUnloadListener(handleUnload);
            // 监控页面滚动时视口上端位置的变化
            function handleScroll() {
                var docScrollY = w.get('pageYOffset');
                if (docScrollY > viewScrollInfo.vt) {
                    viewScrollInfo.vt = docScrollY;
                }
            }

            // 页面关闭前将监控信息发走
            function handleUnload() {
                viewScrollInfo.vh = w.get('innerHeight');
                viewScrollInfo.dh = w.get('docHeight');

                window._mbq('localEventlog', ca, ac, la, viewScrollInfo);
            }
        },
        /**
         * copy to clipborad
         * @method selectAndCopy
         * @param {Selector|HTMLElement|Node} ndInput 需要复制的 input 框
         * @param {Selector|HTMLElement|Node} ndButton 复制按钮
         * @param {String} msg 复制成功的提示语
         */
        selectAndCopy: function (ndInput, ndButton, msg) {
            ndInput = Y.one(ndInput);
            ndButton = Y.one(ndButton);
            if (!ndInput || !ndButton) return;
            msg = msg || '复制成功，您可以粘帖到QQ中发给好友。';

            if (isIE) {
                ndButton.on('click', copy);
            } else {
                Y.use('w-flashcopy', 'www-dialog', function () {
                    new Y.mt.widget.FlashCopy({
                        copyTarget: ndInput,
                        onComplete: function () {
                            ndInput.select();
                            Y.mt.www.Dialog.alert(msg);
                        },
                        node: ndButton
                    });
                });
            }

            // IE复制文本
            function copy() {
                try {
                    Y.use('www-dialog', function (Y) {
                        window.clipboardData.setData("Text", ndInput.get('value'));
                        Y.mt.www.Dialog.alert(msg);
                    });
                } catch (e) {}
                window.setTimeout(function () {
                    ndInput.select();
                }, 0);
            }
        },
        /**
         * 非iframe嵌套时清除合作方的cookie
         * @method removeUnionCookie
         */
        removeUnionCookie: function () {
            if (window.top !== window.self) return;
            var cookie = $Util.Cookie,
                unionCookieName = 'us',
                // 天津每日新报合作
                tjxbSourceName = 'tjxb',
                source = cookie.get(unionCookieName);
            if (source === tjxbSourceName) {
                cookie.remove(unionCookieName, '/', '.' + M.DOMAIN_HOST);
            }
        },
        /**
         * 刷新验证码
         * @method refreshCaptcha
         * @param {Selector|HTMLElement|Node} ndForm
         * @param {Boolean} [needUpdate]
         * @param {String} [captchaName]
         * @return {Object}
         */
        refreshCaptcha: function (ndForm, needUpdate, captchaName) {
            ndForm = Y.one(ndForm);
            if (!ndForm) return null;
            if (typeof captchaName === "undefined") captchaName = 'captcha';
            var ndCaptcha = $Util.field(ndForm, captchaName),
                ndTrigger = ndCaptcha && ndCaptcha.next('a'),
                ndImg = ndCaptcha && ndCaptcha.next('img');
            if (!ndCaptcha || !ndTrigger || !ndImg) return null;

            if (needUpdate) update();
            ndTrigger.on('click', function (e) {
                // 设置e.halt(true)目的是防止因refreshCaptcha()多次调用产生的重复事件绑定
                e.halt(true);
                update();
            });
            //点击图片也能刷新验证码
            ndImg.on('click', function (e) {
                e.halt(true);
                update();
            });

            function update() {
                var src = ndImg.get('src').replace(/((\?|&)rnd=0\.\d+)?$/, '');
                if (src.indexOf("?") === -1) src += "?rnd=";
                else src += "&rnd=";
                ndImg.set('src', src + Math.random());
                ndCaptcha.set('value', '');
            }

            return {
                update: update
            };
        },

        /**
         * 初始化加入收藏
         * @method initFavorite
         */
        initFavorite: function () {
            var ndFavorite = Y.one("#J-favorite"),
                domain = "http://www.meituan.com?utm_campaign=home&utm_medium=free&utm_source=Favorites",
                title = "美团网-美团一次 美一次",
                ctrl =  (window.navigator.userAgent.toLowerCase()).indexOf('mac') !== -1 ? 'Command' : 'Ctrl',
                ADDFAILERR = "抱歉，您的浏览器不支持此操作。\n请使用按键 " + ctrl + "+D 收藏美团。";

            if (ndFavorite) {
                var ndFavoriteIcon = ndFavorite.one('.icon-keep');

                ndFavorite.on("click", addFavorite);
                ndFavorite.on("mouseenter", function () {
                    ndFavoriteIcon.removeClass('F-glob-star-border').addClass('F-glob-star');
                });
                ndFavorite.on("mouseleave", function () {
                    ndFavoriteIcon.removeClass('F-glob-star').addClass('F-glob-star-border');
                });
            }

            function addFavorite() {
                if (ndFavorite) {
                    try {
                        window.external.addFavorite(domain, title);
                    } catch (ex) {
                        try {
                            window.sidebar.addPanel(title, domain, "");
                        } catch (exTwo) {
                            Y.use('www-dialog', function (Y) {
                                Y.mt.www.Dialog.alert(ADDFAILERR);
                            });
                        }
                    }
                }
            }
        },
        /**
         * 初始化抽奖信息滚动
         */
        Lottery: {
            init: function (ndLottery) {
                var SPEED = 800,
                    DELAY = 4000;
                ndLottery.plug(Y.mt.plugin.Effect);
                ndLottery.effect.roll("ul", SPEED, DELAY);
            }
        },
        /**
         * 屏蔽表单按钮
         * @method disableButton
         * @param {Selector|HTMLElement|Node} ndButtonWrapper
         * @param {Boolean} [status=true]
         */
        disableButton: function (ndButton, status) {
            ndButton = Y.one(ndButton);
            if (!ndButton) return;

            var disabledClass = 'btn-disabled';
            status = typeof status === 'undefined' ? true : status;
            ndButton.set('disabled', status);
            if (status) {
                ndButton.addClass(disabledClass);
            } else {
                ndButton.removeClass(disabledClass);
            }
        },
        // @depreciated
        initStickQrcode: function () {
            var ndQrcode = Y.one('.stick-qrcode');
            if (!ndQrcode) return;
            Y.mt.www.StickQrcode.init(ndQrcode);
        },
        StickQrcode: {
            init: function (nd, config) {
                var COOKIE_NAME = 'stick-qrcode';
                if ($Cookie.get(COOKIE_NAME)) return;

                var sticky,
                    conf = {
                        vAlign: 'top',
                        vTopOffset: 200,
                        hAlign: 'right',
                        vBottomOffset: 140,
                        bottomEdge: '.site-info-w',
                        topEdge: '#bdw',
                        hideWhenTooNarrow: true,
                        followScrolling: true
                    };

                if (config) {
                    Y.mix(conf, config, true);
                }
                if (typeof conf.topEdge === 'undefined') {
                    conf.topEdge = Y.one('.site-mast__site-nav-w').getY() + 89;
                }

                // 把事件代理放在前面，防止平时输出了qrcode代码时，关闭按钮失效
                nd.delegate('click', function (e) {
                    e.halt();

                    // 过期时间1天
                    var expiresDate = $Cookie.getExpiresDate(1, 0, 0);
                    nd.remove(true);
                    if (typeof sticky !== 'undefined') {
                        sticky.destroy();
                    }
                    $Cookie.set(COOKIE_NAME, '1', expiresDate, '/', '.' + M.DOMAIN_HOST);
                }, '.close');

                if (conf.followScrolling !== true) {
                    nd.setStyles({
                        top: conf.topEdge,
                        position: 'absolute',
                        display: 'inline'
                    });
                    return;
                }
                Y.use('uix-sticky', function (Y) {
                    sticky = new Y.mt.uix.Sticky(nd, conf);
                });
            }
        },
        // @depreciated
        initSimpleQrcode: function () {
            var ndQrcode = Y.one('.simple-stick-qrcode');
            if (!ndQrcode) return;
            Y.mt.www.SimpleQrcode.init(ndQrcode);
        },
        SimpleQrcode: {
            init: function (ndQrcode) {
                var COOKIE_NAME = 'stick-qrcode';
                if ($Cookie.get(COOKIE_NAME)) return;

                ndQrcode.show();
                ndQrcode.delegate('click', function (e) {
                    e.halt();
                    // 过期时间1天
                    var expiresDate = $Cookie.getExpiresDate(1, 0, 0);
                    ndQrcode.remove(true);
                    $Cookie.set(COOKIE_NAME, '1', expiresDate, '/', '.' + M.DOMAIN_HOST);
                }, '.close');
            }
        },
        /**
         * 加载icon，譬如sprite图片名为sp-header.png，可取class sp-header来将背景应用在节点上，如果要延迟加载，则需要从节点上删除sp-header这个class，同时加载属性data-lazyicon="sp-header"
         * @method loadLazyIcon
         * @param {Selector|Node} ndContainer
         * @param {AttributeSelector} stampAttr
         */
        loadLazyIcon: function (ndContainer, stampAttr) {
            var ATT_STAMP = stampAttr || 'data-lazyicon';
            ndContainer = Y.one(ndContainer || 'body');
            if (!ndContainer) return;
            ndContainer.all('[' + ATT_STAMP + ']').each(function (nd) {
                var cls = nd.getAttribute(ATT_STAMP);
                if (cls) {
                    nd.addClass(cls);
                    nd.removeAttribute(ATT_STAMP);
                }
            });
        },
        /**
         * 系统信息
         * @class CartEx
         * @namespace Y.mt.www
         * @static
         */
        CartEx: {
            init: function () {
                var ndCart = Y.one('#J-my-cart'),
                    ndDropdownList = ndCart && ndCart.one('#J-my-cart-menu');

                // /cart/页面没有下拉列表
                if (ndDropdownList) {
                    this.bindNavDelete();
                    ndCart.once('mouseover', function () {
                        // 如果还没有数据返回，则在mouseover时应当有一个loading效果
                        if (!ndCart.hasClass('J-cart-updated')) {
                            ndDropdownList.addClass('dropdown-menu--loading');
                        }
                    });
                }
            },
            /**
             * 初始化导航栏购物车商品下拉菜单删除事件
             * @method bindNavDelete
             */
            bindNavDelete: function () {
                var ndCart = Y.one('#J-my-cart'),
                    Cart = this,
                    async = null;

                ndCart.delegate('click', function (e) {
                    e.preventDefault();
                    if (async && async.isInProgress()) return;

                    var ndDelete = this;
                    async = $IO('/cart/remove', {
                        method: 'POST',
                        data: $Util.toPostData({
                            cartItem: $Util.data(ndDelete, 'id') + '-' + $Util.data(ndDelete, 'calendarid') + '-' + $Util.data(ndDelete, 'dealgoodsid')
                        }),
                        on: {
                            success: function (id, o) {
                                var res = $Util.getEvalRes(o);
                                if (res.status) {
                                    handleSuccess(ndDelete);
                                }
                            }
                        }
                    });
                }, '.delete');

                function handleSuccess(ndDelete) {
                    // 购物车页删除时，必须刷新页面
                    // 购物车页有两种可能的url
                    // 1. /cart/
                    // 2. /cart/default/1958988
                    if ('/cart/' === window.location.pathname || /^\/cart\/default\//.test(window.location.pathname)) {
                        // 类似第2种url，刷新页面时会添加当前deal到购物车，所以删除时不能直接刷新，而是要跳到/cart/
                        window.location.href = '/cart/';
                        return;
                    }

                    var ndLi = ndDelete.ancestor('.dropdown-menu__item'),
                        ndUl = ndLi.ancestor(),
                        ndCount = ndCart.one('.badge'),
                        count = parseInt(ndCount.get('text'), 10) - 1;

                    ndLi.plug(Y.mt.plugin.Effect);
                    ndLi.effect.fadeOut(true, 200, function () {
                        var ndMore,
                            more = 0,
                            ndDescription;
                        // 更新气泡处提示
                        Cart.updateCount(count);

                        if (count === 0) {
                            ndUl.ancestor('.dropdown-menu').setHTML(Cart.getEmptyTips());
                            return;
                        }

                        // 更新下拉列表处提示
                        ndDescription = ndCart.one('.cart-discription');
                        if (!ndDescription) return;

                        ndMore = ndDescription.one('.cart-deal-num');
                        more = parseInt(ndMore.getHTML(), 10);
                        if (more === 1) {
                            ndDescription.hide();
                            ndUl.removeClass('long-list');
                        }
                        more--;
                        if (more < 0) more = 0;

                        // 购物车商品数量更新
                        ndMore.setHTML(more);
                    });
                }
            },
            /**
             * 更新导航栏购物车显示数量
             * @method updateCount
             */
            updateCount: function (count) {
                var ndCart = Y.one('#J-my-cart'),
                    ndCount;

                if (!ndCart) return;

                ndCount = ndCart.one('.badge');

                if (!ndCount && count > 0) {
                    ndCart.one('#J-my-cart-toggle').append('<em class="badge"><strong class="cart-count">' + count + '</strong>件</em>');
                }
                if (ndCount) {
                    ndCount.setHTML('<strong class="cart-count">' + count + '</strong>件');
                }
            },
            /**
             * 获取购物车为空时的提示信息
             * @method getEmptyTips
             */
            getEmptyTips: function () {
                return '<p class="dropdown-menu--empty">暂时没有商品</p>';
            },
            /**
             * 初始化购物车商品下拉菜单
             * @method updateDropdownList
             */
            updateDropdownList: function () {
                var ndCart = Y.one('#J-my-cart'),
                    ndDropdownList = ndCart.one('#J-my-cart-menu'),
                    CLASS_DROPDOWN_LOADED = 'dropdown-menu--loaded',
                    url = '/index/cartdeals',
                    Cart = this,
                    async = null;

                if (!ndDropdownList.addClass(CLASS_DROPDOWN_LOADED)) {
                    ndDropdownList.addClass(CLASS_DROPDOWN_LOADED);
                }

                async = $IO(url, {
                    method: 'GET',
                    on: {
                        success: function (id, o) {
                            var res = $Util.getEvalRes(o);
                            if (res.data) {
                                Cart.update(res.data);
                            }
                        }
                    }
                });

                return async;
            },
            update: function (data) {
                var ndCart = Y.one('#J-my-cart'),
                    ndDropdownList = ndCart.one('#J-my-cart-menu'),
                    CLASS_DROPDOWN_LOADING = 'dropdown-menu--loading',
                    CLASS_DROPDOWN_ITEM = 'dropdown-menu__item',
                    self = this,
                    DEALS_MAX_NUM = 5;
                var contentContainer = [],
                    gaeventPhotoTpl = ' gaevent="nav/cart/{index}/photo"',
                    gaeventTitleTpl = ' gaevent="nav/cart/{index}/title"',
                    isLongList,
                    listItemContent =
                    '<a href="{url}" title="{title}" target="_blank" rel="nofollow" class="deal-link"' + gaeventPhotoTpl + '>' +
                    '    <img class="deal-cover" src="{imgUrl}" width="80" height="50" />' +
                    '</a>' +
                    '<h5 class="deal-title"><a href="{url}" title="{title}" target="_blank" rel="nofollow" class="deal-link"' + gaeventTitleTpl + '>{title}</a></h5>' +
                    '<p class="deal-price-w"><a href="javascript:void(0);" data-id="{dealid}" data-calendarid="{calendarid}" data-dealgoodsid="{dealgoodsid}" class="delete link--black__green">删除</a><em class="deal-price">¥{price}</em></p>';

                ndCart.addClass('J-cart-updated');
                self.updateCount(data.length);
                if (!data.length) {
                    contentContainer.push(self.getEmptyTips());
                } else {
                    isLongList = data.length > DEALS_MAX_NUM;
                    contentContainer.push('<ul class="list-wrapper' + (isLongList ? ' long-list' : '') + '">');
                    Y.Array.each(data, function (deal, index) {
                        contentContainer.push('<li class="' + CLASS_DROPDOWN_ITEM + '">');
                        contentContainer.push(Y.Lang.sub(listItemContent, {
                            url: deal.url,
                            title: deal.title,
                            price: deal.price,
                            imgUrl: deal.imgurl,
                            dealid: deal.dealid,
                            calendarid: deal.calendarid,
                            dealgoodsid: deal.dealgoodsid,
                            index: index + 1
                        }));
                        contentContainer.push('</li>');
                    });
                    contentContainer.push('</ul>');
                    if (isLongList) {
                        contentContainer.push('<p class="cart-discription">购物车里还有' + '<span class="cart-deal-num">' + (data.length - DEALS_MAX_NUM) + '</span>' + '个团购</p>');
                    }
                    contentContainer.push('<p class="check-my-cart"><a href="/cart/" class="btn btn-hot btn-small" gaevent="nav/cart/checkbutton">查看我的购物车</a></p>');
                }
                if (self._updateHandle) {
                    self._updateHandle.detach();
                }
                self._updateHandle = Y.one('#J-my-cart-toggle').once('mouseover', function () {
                    ndDropdownList.removeClass(CLASS_DROPDOWN_LOADING).setHTML(contentContainer.join(''));
                    self._updateHandle = null;
                });
            }
        },
        /**
         * 统计某个API的加载时间
         * @param {Object} 后端时间
         * @param {Number} API开始时间
         * @param {String} API名称
         */
        trackApiTiming: function (data, startTime, label) {
            if (window.mta && data && data.request) {
                var totalTime = Y.Lang.now() - startTime,
                    backendTime = data.request,
                    obj = {};

                obj[label] = {
                    latency: totalTime - backendTime,
                    backend: backendTime
                };

                window.mta('timing', 'api', obj);
            }
        },
        trackFeatureTiming: function (label, data) {
            if (window.mta && data) {
                var obj = {};
                obj[label] = data;
                window.mta('timing', 'feature', obj);
            }
        }
    };

    /**
     * regular expression
     * @class REG
     * @namespace Y.mt.www
     * @static
     */
    Y.mt.www.REG = {
        email:      /^[\w\.\-\+]+@([\w\-]+\.)+[a-z]{2,4}$/i,
        username:   /^[\.\w\u4e00-\u9fa5\uF900-\uFA2D]{2,16}$/i,
        password:   /^[\x21-\x7e]{6,32}$/i,
        mobile:     /^1\d{10}$/
    };

    /**
     * zoom detect
     * @class DetectZoom
     * @namespace Y.mt.www
     * @static
     */
    Y.mt.www.DetectZoom = {
        COOKIE_NAME : 'ignore-zoom',

        /**
         * 初始化
         */
        init: function () {
            // 当用户已经点击不再提示
            if ($Cookie.get(this.COOKIE_NAME)) return;
            // 当页面被包裹在iframe中时outerWidth是整个窗体的宽度，而innerWidth是iframe的宽度
            if (window !== window.top) return;

            var isZoom = this._detect();
            if (!isZoom) return;

            this._showMsg();
            this._bindEvent();
        },
        /**
         * 检测缩放函数
         * @private
         * @method detect
         * @return isZoom {Boolean}
         */
        _detect: function () {
            var ratio = 1,
                isZoom = false,
                zoomLevels = [0.9, 1.1], // 浏览器缩放都是按照指定尺度，1.1、1.25...5，只要精确到小数点后一位且在1级缩放区间内，就不算缩放。
                ieVer = Y.UA.ie,
                chromeVer = Y.UA.chrome;

            // 对非主流浏览器不进行判断
            if ((ieVer < 8 && ieVer > 0) || (chromeVer < 21 && chromeVer > 0) || (ieVer === 0 && chromeVer === 0)) return isZoom;

            if (typeof screen.deviceXDPI === "number" && typeof screen.logicalXDPI === "number") {
                ratio = (screen.deviceXDPI / screen.logicalXDPI).toFixed(1);
            } else if (typeof window.outerWidth === "number" && typeof window.innerWidth === "number") {
                ratio = (window.outerWidth / window.innerWidth).toFixed(1);
            }

            // 防止load之后而渲染未完成而拿不到视窗大小
            if (ratio === "0.0") {
                return false;
            } else {
                isZoom = (ratio >= zoomLevels[1] || ratio <= zoomLevels[0]);
                return isZoom;
            }
        },

        /**
         * 显示缩放信息
         * @method showMsg
         */
        _showMsg: function () {
            var ctrlLabel,
                msgTpl;

            ctrlLabel = Y.UA.os === 'windows' ? ' Ctrl ' : ' ⌘ ';
            msgTpl = '<div class="zoom-detect-msg">' +
            '    <span class="content">您所访问的网页内容被缩放可能影响正常使用，可以使用键盘快捷键' + ctrlLabel + '+ 0 恢复正常</span>' +
            '    <a class="J-ignore ignore" href="javascript:void(0)">不再提醒</a>' +
            '</div>';

            if (!Y.one('.zoom-detect-msg')) {
                Y.one("body :first-child").insert(msgTpl, 'before');
            } else {
                Y.one('.zoom-detect-msg').show();
            }
        },

        /**
         * 监听事件
         * @private
         * @method bindEvent
         */
        _bindEvent: function () {
            var ndContainer = Y.one('.zoom-detect-msg'),
                expiresDate = $Cookie.getExpiresDate(15, 0, 0),
                commandKey = Y.UA.os === 'windows' ? 'ctrlKey' : 'metaKey',
                KEY_ZERO = 48,
                KEY_ADD = 187,
                KEY_MINUS = 189,
                DetectZoom = this;

            // 不再提醒
            ndContainer.delegate('click', function () {
                ndContainer.hide();
                $Cookie.set(DetectZoom.COOKIE_NAME, true, expiresDate, '/', '.' + M.DOMAIN_HOST);
            }, '.J-ignore');

            // 还原缩放
            Y.one('body').on('keydown', function (e) {
                if (e[commandKey]) {
                    if (e.keyCode === KEY_ZERO) {
                        ndContainer.hide();
                    }
                    if ((e.keyCode === KEY_ADD || e.keyCode === KEY_MINUS) && !$Cookie.get(DetectZoom.COOKIE_NAME)) {
                        DetectZoom._showMsg();
                    }
                }
            });
        }
    };

    /**
     * @class Iframe
     * @namespace Y.mt.www
     * @static
     */
    Y.mt.www.Iframe = (function () {
        return {
            /**
             * 重置父窗口滚动条的位置
             * @method scrollTop
             */
            scrollTop: function () {
                // 非iframe嵌入
                if (window.top === window.self) return;
                window.top.scroll(0, 0);
            },
            /**
             * 设置父窗口的高度
             * @method setParentHeight
             */
            setParentHeight: function () {
                var frameId = 'mtcrossdomain-height';
                var height;

                // 非iframe嵌入
                if (window.top === window.self) return;
                if (isIE) {
                    height = document.documentElement.scrollHeight;
                } else {
                    height = parseInt(Y.one(document.body).getStyle('height'), 10);
                }

                if (!height || height < 1134) {
                    height = 1134;
                }

                var elFrame = window.top.document.getElementById(frameId);
                if (elFrame) {
                    elFrame.style.height = height + 'px';
                }
            }
        };
    }());

    Y.mt.www.Webp = (function () {
        var support = false;
        (function () {
            if (!window.localStorage || typeof localStorage !== 'object') return;

            var name = 'webpa'; // webp available

            if (!localStorage.getItem(name) || (localStorage.getItem(name) !== 'available' && localStorage.getItem(name) !== 'disable')) {
                var img = document.createElement('img');

                img.onload = function () {
                    try {
                        localStorage.setItem(name, 'available');
                    } catch (ex) {
                    }
                };

                img.onerror = function () {
                    try {
                        localStorage.setItem(name, 'disable');
                    } catch (ex) {
                    }
                };

                img.src = 'data:image/webp;base64,UklGRkoAAABXRUJQVlA4WAoAAAAQAAAAAAAAAAAAQUxQSAsAAAABBxAREYiI/gcAAABWUDggGAAAADABAJ0BKgEAAQABABwlpAADcAD+/gbQAA==';
            }
        })();
        if (window.localStorage && typeof localStorage === 'object') {
            support = localStorage.getItem('webpa') === 'available';
        }
        return {
            getWebpSrc: function (src) {
                return support ? src + '.webp' : src;
            },

            /**
             * 模式识别替换，替换bigrender容器内的字符串
             */
            getWebpContent: function (content) {
                var webpPattern;
                if (!support) {
                    return content;
                }

                webpPattern = /(<img .*class=".*\bJ-webp\b.*".* (?:data-)?src=")(\S*\.jpg)(".*>)/;

                content = content.replace(/<img .*?>/, function (match) {
                    return match.replace(webpPattern, '$1$2.webp$3');
                });
                return content;
            },
            /**
             * 自动回退为jpg功能
             */
            autoFallback: function (ndContainer) {
                ndContainer = Y.one(ndContainer);
                if (!ndContainer) return;

                var pattern = /\.webp$/;
                ndContainer.all('.J-webp').each(function (nd) {
                    var src = nd.get('src');
                    if (pattern.test(src) && nd.getData('listen-fallback') !== '1') {
                        nd.on('error', function () {
                            nd.set('src', src.replace(pattern, ''));
                            nd.setData('listen-fallback', '1');
                        });
                    }
                });
            }
        };
    }());

    Y.mt.www.hotelRoomStatus = {
        /**
         * 初始化可预约酒店单的房态日历
         * @method initHotelFirstScreenCalendar
         * @param {Selector|Node} wrapper
         * @param {Object} roomTypeList
         * @param {Object} roomStatusList
         */
        initCalendar: function (wrapper, roomTypeList, roomStatusList) {
            var $Date = Y.mt.date,
                ndWrapper = Y.one(wrapper);

            if (!ndWrapper) return;

            var ndToday = ndWrapper.one('.J-today-roomstatus'),
                ndTomorrow = ndWrapper.one('.J-tomorrow-roomstatus'),
                ndCalendarBody = ndWrapper.one('.J-hotel-roomstatus-body'),
                ndCalendarMoreDiv = ndWrapper.one('.J-hotel-roomstatus-more'),
                ndCalendarMoreLink = ndWrapper.one('.J-hotel-roomstatus-more .check-all-status'),
                ndCalendarTip = ndWrapper.one('.J-hotel-roomstatus-more .hotel-roomstatus-tip'),
                arrDayOfWeek = ['周日', '周一', '周二', '周三', '周四', '周五', '周六'],
                TableHtml = [],
                roomTypeListLength = 0;
                
            var calendarDate = new Date(),
                calendarDateFullYear = calendarDate.getFullYear(),
                calendarDateMonth = calendarDate.getMonth(),
                calendarDateDay = calendarDate.getDate(),
                today = calendarDate,
                tomorrow = new Date(calendarDateFullYear, calendarDateMonth, calendarDateDay + 1),
                targetDate,
                targetDay,
                weekDay,
                roomStatus,
                roomStatusDisappoint = false;


            // 根据日期和房型ID 返回房型状态，默认为不可预约状态
            function _getRoomStatus(day, roomKey) {
                
                if (roomStatusList && roomStatusList[day]) {
                    return roomStatusList[day][roomKey];
                }

                return 4;
            }

            // 填充目标日期节点的房间状态信息
            function _setDayRoomStatus(ndRoom, date, prefix) {
                var roomStatus = 0,
                    roomStatusInfo = ['满房', '有房'],
                    weekDay = arrDayOfWeek[date.getDay()],
                    monthDay = $Date.formatDateByString(date, 'M.d'),
                    targetDay = $Date.formatDate(date, '-').replace(/-/g, '');
                
                // 取消节点的有房状态样式
                ndRoom.removeClass('avaliable');
                
                // 当天的任一房态中有可用房间时，则显示当天有房
                if (roomStatusList && roomStatusList[targetDay]) {
                    for (var i in roomStatusList[targetDay]) {
                        if (roomStatusList[targetDay][i] === 1) {
                            roomStatus = 1;
                        }
                    }
                }
                
                // 设置HTML内容
                ndRoom.setHTML(prefix + roomStatusInfo[roomStatus] + '<span class="date-info">' + monthDay + ' ' + weekDay + '</span>');
                
                //有房时，添加有房状态样式
                if (roomStatus === 1) {
                    ndRoom.addClass('avaliable');
                }
            }

            //对今天和明天的房间信息做更新
            if (ndToday && ndTomorrow) {
                _setDayRoomStatus(ndToday, today, '今日');
                _setDayRoomStatus(ndTomorrow, tomorrow, '明日');
            }

            //开始处理房型的一周状态表格
            TableHtml.push('<table class="hotel-calendar-table"><thead class="hotel-appoint-body-table-header"><tr><th class="calendar-body-cell calendar-body-cell-head"><div class="room-type-name">房型</div></th>');

            //生成表格头的日期信息
            for (var i = 0; i < 7; i++) {

                targetDate = new Date(calendarDateFullYear, calendarDateMonth, calendarDateDay + i);
                weekDay = arrDayOfWeek[targetDate.getDay()].substr(1);

                //对今天的日期显示做特殊处理
                if (i === 0) {
                    weekDay = '今天';
                }
                TableHtml.push('<th class="calendar-body-cell"><p>' + $Date.formatDateByString(targetDate, 'M.d') + '</p><p>' + weekDay + '</p></th>');
            }

            TableHtml.push('</thead><tbody class="hotel-appoint-body-table-body">');

            // 根据房型数据，生成房型的一周房态信息
            Y.each(roomTypeList, function (roomType, key) {
                roomTypeListLength++;

                // 只显示最多5个房型出来
                if (roomTypeListLength > 5) {
                    return;
                }

                TableHtml.push('<tr><td class="calendar-body-cell calendar-body-cell-head"><div class="room-type-name" title="' + roomType + '">' + roomType + '</div></td>');

                for (var i = 0; i < 7; i++) {
                    targetDate = new Date(calendarDateFullYear, calendarDateMonth, calendarDateDay + i);
                    targetDay = $Date.formatDate(targetDate, '-').replace(/-/g, '');
                    roomStatus = _getRoomStatus(targetDay, key);

                    switch (roomStatus) {
                    case 1:
                        TableHtml.push('<td class="calendar-body-cell" title="有房，可预约"><i class="room-appoint-avaliable"></i></td>');
                        break;
                    case 2:
                        TableHtml.push('<td class="calendar-body-cell calendar-body-cell-disabled">满</td>');
                        break;
                    case 3:
                        TableHtml.push('<td class="calendar-body-cell calendar-body-cell-disabled">满</td>');
                        break;
                    case 4:
                        TableHtml.push('<td class="calendar-body-cell calendar-body-cell-disabled">一</td>');
                        roomStatusDisappoint = true;
                        break;
                    }

                }

                TableHtml.push('</tr>');
            });

            TableHtml.push('</tbody></table>');

            ndCalendarBody.setHTML(TableHtml.join(''));

            //如果房型少于5个，则不显示跳转到更多的入口
            if (roomTypeListLength > 5) {
                ndCalendarMoreLink.setStyle('display', 'inline-block');
            }

            //如果有出现不可预约的提示，才显示对应tip
            if (roomStatusDisappoint) {
                ndCalendarTip.setStyle('display', 'inline-block');
            }

            if (roomTypeListLength > 5 || roomStatusDisappoint) {
                ndCalendarMoreDiv.setStyle('display', 'block');
            }
        }

    };
    // 注册自动运行函数
    M.load(function () {
        M._autoinit.push(function (element) {
            Y.mt.www.loadLazyIcon(element);
        });
    });

    M._autoinit.push(function (element) {
        Y.mt.www.ImageLazyLoader.init(element);
    });

    // widget 自动初始化
    M._autoinit.push(function (element) {
        Y.mt.widget.init(element);
    });

    // widget 自动初始化
    M._autoinit.push(function (element) {
        Y.mt.ComponentHub.boot(Y.one(element));
    });

    // uiguide 皮肤配置覆盖
    Y.applyConfig({
        skin: {
            overrides: {
                'w-date': [ 'uiguide' ]
            }
        }
    });
},
// version
'1.0.0',

// dependency
{
    requires: [
        'mt-base',
        'mt-component',
        'mt-window',
        'mt-bigrender',
        'mt-imageloader',
        'mt-ajax',
        'io-base',
        'io-form',
        'p-node-placeholder',
        'p-node-effect'
    ]
});
;/*jshint white:true, unused:true*/
/**
 * 主站追踪系统
 *
 * @module www-tracker
 */
M.add('www-tracker', function (Y) {
    var $Util = Y.mt.util,
        $Lang = Y.mt.lang;

    var DISABLE_SIGN = '$',
        DISABLE_CLASS = 'disable-tracking',
        ACM_QUERY_KEY = 'acm',
        ACM_CLICK_KEY = 'acm-clk',
        ACM_QUERY_CHECK_KEY = 'cks',
        MTT_QUERY_KEY = 'mtt',
        MTT_ATTR_KEY = 'mod-mtt',
        MTT_DEEPVIEW_ATTR = 'data-mod-deepview',
        MTT_GROUP_SELECTOR_ATTR = 'data-mod-selector',
        MODULE_RESOURCE_NODE_ATTR = 'mod',
        MODULE_LINK_SELECTOR_ATTR = 'data-mod-idx',
        MODULE_ANCHOR_DEFAULT_CONTENT = '_anchor';

    Y.namespace('mt.www.tracker');
    /**
     * 站内链接跟踪(Meituan Tracking System)。可跟踪点击事件与 PV。此处只将数据写进 cookie
     * @class tracker
     * @namespace mt.www
     * @static
     */
    Y.mt.www.tracker = {
        /**
         * 初始化由点击触发的追踪。要跟踪指定url的话，可使用trackUrl添加
         * @method init
         * @param {Object} o 跟踪配置参数，详见applyConf
         * @static
         */
        init: function (o) {
            this.applyConf(o);
            this.trackEvent();
            this.trackQuery();
            this.trackForm();
            this.update();
        },
        /**
         * 配置跟踪参数
         * @method applyConf
         * @param {Object} o
         * @static
         * @example
         * o = {
         *     cookie: '__t',    // 所使用的cookie名称
         *     delimiter: '.',    // cookie中数据的分隔符
         *     expire: { d: 0, h: 0, m: 30 },     // 一次跟踪的有效期
         *     attr: 'data-mttcode'    // 存放数据的节点属性名称
         * }
         */
        applyConf: function (o) {
            var tracker = this;
            var conf = {
                    cookie: '__t',
                    delimiter: '.',
                    expire: { d: 0, h: 0, m: 30 },
                    attr: 'data-mttcode'
                };
            if (o) {
                tracker.config = Y.mix(o, conf);
            } else {
                tracker.config = conf;
            }
            // 点击事件触发的跟踪开始时，PV为0，此后刷新页面加1
            tracker.EVENT_INI_PV = 0;
            // 特定页面访问触发的跟踪开始时，PV为1，此后刷新页面加1
            tracker.URL_INI_PV = 1;
        },
        /**
         * 更新跟踪数据
         * @method update
         */
        update: function () {
            var tracker = this;
            if (M.Env._isTCUptodate) return;
            M.Env._isTCUptodate = true;

            var delimiter = tracker.config.delimiter,
                cookie = tracker.getCookie();

            if (cookie === '') return;
            cookie = cookie.split(delimiter);
            // PV为1
            cookie[1] = (+cookie[1]) + 1;
            // 此次浏览时刻
            tracker.TS = Y.Lang.now();
            cookie[2] = tracker.TS;
            cookie = cookie.join(delimiter);

            tracker.setCookie(cookie);
        },
        /**
         * 设置 cookie
         *
         * @method setCookie
         * @param {String} cookieValue
         */
        setCookie: function (cookieValue) {
            var c = this.config.cookie,
                e = this.config.expire,
                expires = this.getExpires(e.d, e.h, e.m),
                ereg = /E\w+/,
                ecode = $Util.Cookie.get(c).match(ereg);

            if (ecode && !cookieValue.match(ereg)) {
                cookieValue += '.' + ecode[0];
            }
            $Util.Cookie.set(c, cookieValue, expires, '/', '.' + M.DOMAIN_HOST);
        },
        /**
         * 获取跟踪cookie
         *
         * @method getCookie
         * @return {String}
         */
        getCookie: function () {
            return $Util.Cookie.get(this.config.cookie);
        },
        /**
         * 准备event跟踪，或是首次url跟踪
         *
         * @method initTrackingSession
         * @param {String} code 追踪代码
         * @param {Number} initialPV 初始访问量
         */
        initTrackingSession: function (code, initialPV) {
            var tracker = this,
                t = Y.Lang.now(),
                cookie = [t, initialPV, t, code].join(tracker.config.delimiter);
            tracker.TS = t;
            tracker.setCookie(cookie);
            // 在页面载入时，默认会更新一次 cookie，更新的动作有可能发生在本函数调用之后，设标记以阻止更新
            M.Env._isTCUptodate = true;
        },
        /**
         * 获取过期时刻
         *
         * @method getExpires
         * @param {Number} days
         * @param {Number} hours
         * @param {Number} minutes
         * @return {String}
         */
        getExpires: function (days, hours, minutes) {
            var expiresDate,
                startTime = this.TS;

            if (typeof days === "number" && typeof hours === "number" && typeof minutes === "number") {
                expiresDate = new Date(startTime);
                expiresDate.setDate(expiresDate.getDate() + parseInt(days, 10));
                expiresDate.setHours(expiresDate.getHours() + parseInt(hours, 10));
                expiresDate.setMinutes(expiresDate.getMinutes() + parseInt(minutes, 10));
                return expiresDate.toUTCString();
            }
            return "";
        },
        /**
         * 站内追踪代码编码
         * @method _encode
         * @private
         * @static
         * @param {Object} o
         * @return {String}
         */
        _encode: function (o) {
            var ret = [];
            Y.Object.each(o, function (val, key) {
                if (val !== DISABLE_SIGN) {
                    ret.push(key + val);
                }
            });
            return ret.join(this.config.delimiter);
        },
        /**
         * 站内追踪代码解码
         * @method _decode
         * @private
         * @static
         * @param {String} code
         * @return {Object}
         */
        _decode: function (code) {
            if (!code) return {};
            var arr = code.split(this.config.delimiter),
                ret = {};
            Y.Array.each(arr, function (field) {
                ret[field.charAt(0)] = field.slice(1);
            });
            return ret;
        },
        /**
         * 解析URL，返回Query和Hash部分
         * @method _parseUrl
         * @private
         * @static
         * @param {String} url
         * @return {Object}
         */
        _parseUrl: function (url) {
            var search = '',
                hash = '',
                pos;

            pos = url.indexOf('#');
            if (pos > -1) {
                hash = url.slice(pos + 1);
                url = url.slice(0, pos);
            }
            pos = url.indexOf('?');
            if (pos > -1) {
                search = url.slice(pos + 1);
                url = url.slice(0, pos);
            }
            return {
                main: url,
                search: search,
                hash: hash
            };
        },
        /**
         * 层级化试验
         * 所有mttcode属性不能同时存在于有祖先和后代关系的节点上，mtnode的值用于继承
         * 被点击的节点必须是a，或有祖先元素是a，或拥有类mttriger，或其祖先元素有类mttrigger 【注：注意mttrigger的加入有可能造成空白处点击事件被处理】
         * @method trackEvent
         * @static
         */
        trackEvent: function () {
            var ndDoc = Y.one(document.body),
                tracker = this,
                triggerClass = 'mttrigger',
                disableClass = 'mtdisable',
                attr = tracker.config.attr,
                sourceName = 'mtnode',
                sourceAttr = 'data-' + sourceName;

            ndDoc.delegate('click', function (e) {
                var ndTarget = e.target;
                if (ndTarget.get('tagName').toLowerCase() !== 'a' &&
                    !ndTarget.hasClass(triggerClass) &&
                    !ndTarget.ancestor('a, .' + triggerClass)) {
                    return;
                }
                if (ndTarget.ancestor('.' + disableClass, true)) return;
                var codes = tracker._decode(this.getAttribute(attr)),
                    nlAncestor = this.ancestors('[' + sourceAttr + ']'),
                    code,
                    ancestorCodes = {};
                nlAncestor.each(function (nd) {
                    // YUI中取得的nl是从上到下的
                    Y.mix(ancestorCodes, tracker._decode($Util.data(nd, sourceName)), true);
                });
                code = tracker._encode(Y.mix(codes, ancestorCodes));
                tracker.initTrackingSession(code, tracker.EVENT_INI_PV);
            }, '[' + attr + ']');
        },
        _queryTrackingList: {
            'acm': {
                urlKey: ACM_QUERY_KEY,
                attrName: ACM_CLICK_KEY,
                enableInherit: true,
                replaceRule: function (acm) {
                    return Acm.getMergedAcm(acm);
                }
            },
            'mtt': {
                urlKey: MTT_QUERY_KEY,
                attrName: MTT_ATTR_KEY,
                enableInherit: false
            }
        },
        /**
         * 使用Query追踪站内行为
         * @method trackQuery
         * @static
         */
        trackQuery: function () {
            var tracker = this;

            Y.one(document.body).delegate('mousedown', function () {
                // 先节点上的 mtt 值并发送当前节点的点击日志
                tracker.setLinkMttCode(this);
                tracker.sendLinkClickLog(this);

                var href = this.getAttribute('href');

                if (tracker._isAnchor(href) ||
                    this.get('hostname').indexOf(M.DOMAIN_HOST) === -1 ||
                    this.ancestor('.' + DISABLE_CLASS, true)) return;

                var newHref;

                newHref = tracker.appendTrackingParams(href, {
                    node: this
                });

                if (newHref !== href) {
                    this.setAttribute('href', newHref);
                }
            }, 'a');
        },
        /**
         * 获取特定连接的的'位置-额外信息'的属性值
         * '位置' 从传入的 index 获取
         * 额外信息的获取方式如下：
         * 1. 如果链接有MODULE_LINK_SELECTOR_ATTR属性，则取该值。否则
         * 2. 如果是锚点链接，则添加默认的值。否则
         * 3. 如果传入的 needHashText 为 true，则将链接的文案进行哈希，取该哈希值。否则
         * 4. 该链接没有额外信息
         *
         * @param {Node} ndLink 链接
         * @param {Int|String} index 链接所在模块的次序
         * @param {Boolean} needHashText 是否需要使用文案的哈希值作为链接的额外信息
         */
        getModuleLinkPosition: function (ndLink, index, needHashText) {
            var posInfo = index;

            var linkContentAttr = ndLink.getAttribute(MODULE_LINK_SELECTOR_ATTR);

            if (!linkContentAttr) {
                if (this._isAnchor(ndLink.getAttribute('href'))) {
                    linkContentAttr = MODULE_ANCHOR_DEFAULT_CONTENT;
                } else if (needHashText) {
                    linkContentAttr = this.getChecksum(ndLink.get('text'));
                }
            }

            if (linkContentAttr) {
                posInfo += '-' + linkContentAttr;
            }
            return posInfo;
        },
        _isAnchor: function (href) {
            return !href || href.indexOf('#') === 0 || href.indexOf('javascript') === 0;
        },
        /**
         * 获取一个模块内部所有链接的位置-文案信息
         * @param {Node} ndModule 模块节点
         */
        getModulePositionInfo: function (ndModule) {
            var nlLinks = ndModule.all('a'),
                linkPositions = [],
                self = this;

            nlLinks.each(function (ndLink, i) {
                linkPositions.push(self.getModuleLinkPosition(ndLink, i + 1, true));
            });

            return linkPositions.join('.');
        },
        /**
         * 根据链接在模块中的位置设置链接的 mtt 属性，如果链接不在模块内部，则使用默认的 M.PAGE_VIEW_DATA.curmtt 值
         * @param {Node} ndLink 模块节点
         */
        setLinkMttCode: function (ndLink) {
            var ndModule = ndLink.ancestor('[data-' + MODULE_RESOURCE_NODE_ATTR + ']', true),
                needHashText,
                nlGroups, ndGroup, index,
                mttInfo = MttTracker.getMttFromStr(),
                mttSelector;

            if (!ndModule) {
                ndLink.setAttribute('data-' + MTT_ATTR_KEY, M.PAGE_VIEW_DATA.curmtt);
                return;
            }

            mttInfo.moduleid = ndModule.getData(MODULE_RESOURCE_NODE_ATTR);
            needHashText = ndModule.getAttribute(MTT_DEEPVIEW_ATTR);
            mttSelector = ndModule.getAttribute(MTT_GROUP_SELECTOR_ATTR) || 'a';
            nlGroups = ndModule.get('tagName') === 'A' && Y.all(ndModule) || ndModule.all(mttSelector);
            ndGroup = ndLink.ancestor(mttSelector, true);

            index = nlGroups.indexOf(ndGroup);
            if (index < 0) {
                return;
            }

            mttInfo.position = this.getModuleLinkPosition(ndLink, index + 1, needHashText);
            ndLink.setAttribute('data-' + MTT_ATTR_KEY, MttTracker.getMttStr(mttInfo));
        },
        sendLinkClickLog: function (ndLink) {
            var mttStr = ndLink.getData(MTT_ATTR_KEY),
                mtt,
                acm = this._getAncestorData(ndLink, ACM_CLICK_KEY);

            if (mttStr) {
                mtt = MttTracker.getMttFromStr(mttStr);
                MttTracker.sendClickLog(mtt);
            }

            if (acm) {
                window._mbq('sendReclsClick', acm);
            }
        },
        /**
         * 手动发送 mtt 点击日志的接口
         * @param {String} moduleId 模块的 id
         * @param {String|Integer} [position] 被点击元素的位置信息
         */
        sendMttClickLog: function (moduleId, position) {
            var mtt = MttTracker.getMtt(moduleId, position);
            MttTracker.sendClickLog(mtt);
        },
        /**
         * 使用Query追踪站内行为，针对表单的提交
         * @method trackForm
         * @static
         * @param {Node|Selector} ndForm 需要要追踪的表单的节点对象或者 selector
         */
        trackForm: function (ndForm) {
            var tracker = this,
                nlForm = ndForm && Y.all(ndForm) || Y.all('.J-wwwtracker-form');
            nlForm.each(function (ndForm) {
                var method = ndForm.getAttribute('method') || 'get',
                    action, newAction;

                method = method.toLowerCase();
                if (method === 'get') {
                    tracker.appendFormTrackingFields(ndForm);
                } else if (method === 'post') {
                    action = ndForm.getAttribute('action');
                    newAction = tracker.appendTrackingParams(action);
                    if (newAction !== action) {
                        ndForm.setAttribute('action', newAction);
                    }
                }
            });
        },
        disableQueryInherit: function (list) {
            list = list || ['acm'];
            var trackInfo = this._queryTrackingList;

            Y.Array.each(list, function (item) {
                if (trackInfo[item]) {
                    trackInfo[item].enableInherit = false;
                }
            });
        },
        /**
         * 在URL中加入追踪代码。站内链接追踪代码将被覆盖
         * @method appendTrackingParams
         * @static
         * @param {String} href
         *     @param {Object} [config]
         *     @param {String} [config.acm] 站内链接追踪代码
         *     @param {Node} [config.node] 如果传入，会在该 node 的祖先元素上寻找 data-attr 的值
         * @return {String} 替换过的URL
         */
        appendTrackingParams: function (href, config) {
            config = config || {};
            var hrefComponents = this._parseUrl(href),
                hrefSearch = hrefComponents.search,
                hrefQuery = hrefSearch && $Lang.parseStr(hrefSearch, false) || {},
                params;

            params = this.getTrackingParams(config);

            if (Y.Object.isEmpty(params)) {
                return href;
            }

            Y.mix(hrefQuery, params, true);
            return hrefComponents.main + '?' + $Util.toPostData(hrefQuery) +
                (hrefComponents.hash ? ('#' + hrefComponents.hash) : '');
        },
        /**
         * 提供简单的向 get 方法的 form 里添加 acm 和 cks field 的方法
         * @method appendFormTrackingFields
         * @static
         * @param {Node} ndForm 要添加 field 的 form
         * @param {Object} config 同 appendTrackingParams 的 config
         */
        appendFormTrackingFields: function (ndForm, config) {
            config = config || {};
            var params = this.getTrackingParams(config),
                ndInput;

            Y.Object.each(params, function (val, key) {
                ndInput = ndForm.one('[name="' + key + '"]');
                if (!ndInput) {
                    ndForm.appendChild('<input type="hidden" name="' + key + '" value="' + $Lang.htmlEscape(val.toString()) + '" />');
                }
            });
        },
        // 当前页面 url 中 querystring 部分 parse 后的结果缓存
        _locationQuery: null,
        /**
         * 获取 _queryTrackingList 中对应的各个追踪参数
         * 采用的优先级为：(config 中的 value) > (config 中的 node 祖先元素上的 data-attr) > url
         */
        getTrackingParams: function (config) {
            var rv = {},
                self = this,
                search = window.location.search,
                node = config.node,
                locationQuery;

            locationQuery = this._locationQuery ||
                (this._locationQuery = search && $Lang.parseStr(search ? search.slice(1) : '', false) || {});

            Y.Object.each(this._queryTrackingList, function (info, key) {
                var value = config[key],
                    newVal,
                    urlVal = locationQuery[info.urlKey];

                // 如果 config 中没数据，location.search 也没，则没有任何追踪参数需要添加
                if (!value && !node && !urlVal) {
                    return;
                }

                // 从 config 的 value 和 node 上去取相应的追踪参数
                if (!Y.Lang.isUndefined(value)) {
                    newVal = value;
                } else if (node && info.attrName) {
                    newVal = self._getAncestorData(node, info.attrName);
                }

                if (!info.enableInherit) {
                    if (!newVal) {
                        return;
                    }
                    urlVal = '';
                }

                // 从 url 中继承追踪参数的情况
                if (Y.Lang.isUndefined(newVal)) {
                    newVal = urlVal;
                } else if (info.replaceRule) {
                    newVal = info.replaceRule(newVal, urlVal);
                }

                if (newVal) {
                    rv[info.urlKey] = newVal;
                }
            });

            // 对于 acm 还需要加入校验值
            if (rv[ACM_QUERY_KEY]) {
                rv[ACM_QUERY_CHECK_KEY] = this.getChecksum(rv['acm']);
            }
            if (!rv[MTT_QUERY_KEY]) {
                rv[MTT_QUERY_KEY] = M.PAGE_VIEW_DATA.curmtt;
            }
            return rv;
        },
        getAcmAttr: function (acm) {
            if (!acm) {
                return '';
            }

            return ' data-' + ACM_CLICK_KEY + '="' + acm + '"';
        },
        /**
         * 获取祖先节点形如 data-xyz 的值
         * @method _getAncestorData
         * @param {Node} node 当前节点
         * @param {String} name data-xyz 中的 xyz
         * @static
         * @return {String|undefined}
         */
        _getAncestorData: function (node, name) {
            var ndAncestor = node.ancestor('[data-' + name + ']', true);
            if (ndAncestor) {
                return $Util.data(ndAncestor, name);
            }
            return;
        },
        _checksumDict: {}, // 保证每个 trackcode 的 checksum 只被计算一次
        /**
         * Fletcher checksum
         * 简单的校验算法 https://gist.github.com/jed/1200689
         * http://en.wikipedia.org/wiki/Fletcher's_checksum
         */
        getChecksum: function (str) {
            /*jshint bitwise:false */
            if (this._checksumDict[str]) {
                return this._checksumDict[str];
            }
            var b, c, d, e, f;
            for (b = c = d = 0; (e = str.charCodeAt(d++)); c = (c + b) % 255) b = (b + e) % 255;
            f = c << 8 | b;
            this._checksumDict[str] = f;
            return f;
        },
        /**
         * 跟踪特定url的访问
         * @method trackUrl
         * @param {Object} config
         * @static
         * @example
         * // config 可设为数组实现多种跟踪方式的组合
         *
         * // 方式一：单个url的跟踪
         * config = {
         *     mttcode: 'Anone',
         *     pathname: '/'
         * }
         *
         * // 方式二：多个url同一个mttocde的跟踪
         * config = {
         *     mttcode: 'Anone',
         *     pathname: ['/around/', '/around/default/3']
         * }
         *
         * // 方式三：一类页面的跟踪
         * config = {
         *     mttcode: 'Anone',
         *     fnIsTargetUrl: function () {
         *         var REG = /^\/deal\/[\s\S]+\.html$/;
         *         return REG.test(window.location.pathname);
         *     }
         * }
         */
        trackUrl: function (config) {
            var tracker = this;

            tracker.applyConf();
            if (Y.Lang.isArray(config)) {
                Y.Array.each(config, track);
            } else {
                track(config);
            }

            function track(cfg) {
                var m = cfg.mttcode,
                    p = cfg.pathname,
                    f = cfg.fnIsTargetUrl;

                if (!isTargetUrl(p, f)) return;

                tracker.initTrackingSession(m, tracker.URL_INI_PV);
            }

            // 是否为指定跟踪的url
            function isTargetUrl(pathname, fnIsTargetUrl) {
                var pn = window.location.pathname;

                if (Y.Lang.isArray(pathname) && Y.Array.indexOf(pathname, pn) !== -1) {
                    return true;
                } else if (pathname && pn === pathname) {
                    return true;
                } else if (fnIsTargetUrl && fnIsTargetUrl()) {
                    return true;
                }

                return false;
            }
        }
    };

    /**
     * 统计CTR
     * @class CTRTracker
     * @namespace mt.www
     * @static
     */
    Y.mt.www.CTRTracker = {
        /**
         * 统计模块展现次数
         * @method logView
         * @param {Selector|Node} ndContainer
         */
        logView: function (ndContainer) {
            var ATT_CTR = 'data-mod',
                ATTR_CTR_DEEPVIEW = 'data-mod-deepview',
                nlContainer = Y.all(ndContainer),
                modules = [],
                mttModules = [],
                tracker = Y.mt.www.tracker;

            nlContainer.each(function (ndContainer) {
                if (ndContainer.hasAttribute(ATT_CTR)) {
                    log(ndContainer);
                }
                ndContainer.all('[' + ATT_CTR + ']').each(log);
            });
            if (modules.length) {
                window._mbq('trackModuleview', {
                    pm: modules.join(''),
                    mttmod: mttModules.join(',')
                });
            }

            function log(nd) {
                var moduleId,
                    deepview = nd.getAttribute(ATTR_CTR_DEEPVIEW);

                if (nd.hasClass('log-mod-viewed')) {
                    return;
                }

                moduleId = nd.getAttribute(ATT_CTR);
                nd.addClass('log-mod-viewed');
                // 新的模块ID长度不为 2，如果在老的模块展现日志中加入新的模块，会造成不正确的解析
                if (moduleId.length === 2) {
                    modules.push(moduleId);
                }

                if (deepview) {
                    if (deepview === '1' || deepview === 'true') {
                        deepview = tracker.getModulePositionInfo(nd);
                    }
                    mttModules.push(moduleId + '(' + deepview + ')');
                } else {
                    mttModules.push(moduleId);
                }
            }
        }
    };

    // 推荐数据的展现量发送
    Y.mt.www.RecViewTracker = {
        logView: function (ndContainer) {
            ndContainer = Y.one(ndContainer);

            var dataName = 'data-' + ACM_CLICK_KEY,
                nlContainer = ndContainer.all('[' + dataName + ']'),
                viewedClass = 'log-acm-viewed',
                data,
                dataArr = [];

            nlContainer.push(ndContainer);

            nlContainer.each(function (nd) {
                if (nd.hasClass(viewedClass)) {
                    return;
                }

                data = nd.getAttribute(dataName);
                if (data) {
                    dataArr.push(data);
                    nd.addClass(viewedClass);
                }
            });

            window._mbq('sendReclsView', dataArr);
        }
    };

    M.load(function () {
        // 注册自动运行函数
        M._autoinit.push(function (element) {
            Y.mt.www.CTRTracker.logView(element);
            Y.mt.www.RecViewTracker.logView(element);
        });

        Y.use('node', function () {
            Y.mt.www.CTRTracker.logView('body');
            Y.mt.www.RecViewTracker.logView('body');
        });
    });


    /**
     * Acm 相关的函数
     *
     * acm属性可以理解为数据组提供的标明一次推荐中一项数据的ID，前端需要在用户浏览器页面时统计 acm 的相关指标，
     * 包括：
     * a) 页面（模块）载入时发送展现的日志
     * b) 点击相应的链接时发送点击的日志
     * c) 页面跳转时跳转的url带上被点击项的acm和当前页面url中的acm，当两个acm都有时，需要按一定规则合并两个值。
     *
     * 由于上面c)部分的局限，要求前端对acm数据的内部结构有一定了解才能进行合并，下面的几个方法主要完成了合并的工作
     * 简单来说，acm的格式是由不重复的大写字母分隔数据项的字符串，比如：
     * AsearchBquery_火锅Ccategory.123.4
     * 我们可以解析成：
     * {
     *     A: 'search',
     *     B: 'query_火锅',
     *     C: 'category.123.4'
     * }
     *
     * 编码的流程：
     * acm -> 凯撒加密 -> acm
     *
     * 解码的流程（反方向）：
     * encoded acm -> decodeURI -> 凯撒解密 -> acm
     *
     * 注意，上面有编码时没有做encodeURI的步骤
     */
    var Acm = {
        // 按照大写字母切分acm字符串的正则
        _regAcm: /[A-Z][^A-Z]+/g,

        // 匹配url中acm参数的正则
        _regUrlAcm: /[?&]acm=([^&#]+)/g,

        // 当前url中的acm字符串被解析后的对象缓存
        _urlAcmInfo: null,

        // 解析_urlAcmInfo 时页面的 url，不包括hash部分
        _pageUrl: null,

        /**
         * 获取合并acm和页面url中acm后被加密的acm值
         *
         * @method getMergedAcm
         * @param {String} acm
         * @return {String} merged and encoded acm
         */
        getMergedAcm: function (acm) {
            var acmInfo = this._parse(acm),
                urlAcmInfo = this._getUrlAcmInfo(),
                mergedAcm = this._merge(acmInfo, urlAcmInfo);

            return this._encode(mergedAcm);
        },
        /**
         * 从当前页面的 url 中解析出 acm 对象
         *
         * @method _getUrlAcmInfo
         * @return {Object} decoded acm object
         */
        _getUrlAcmInfo: function () {
            var pageHref = window.location.href,
                pageUrl = pageHref.replace('#' + window.location.hash, ''),
                urlAcm;

            // 有可能页面 url 发生了改变但页面没有刷新，这种情况下需要重新获取url中的acm对象
            if (this._pageUrl !== pageUrl || !this._urlAcmInfo) {
                this._pageUrl = pageUrl;

                // recalculate acm
                urlAcm = this._regUrlAcm.exec(pageUrl);

                urlAcm = urlAcm && this._decode(urlAcm[1]) || '';
                this._urlAcmInfo = this._parse(urlAcm);
            }
            return this._urlAcmInfo;
        },
        /**
         * 将 acm 对象或者字符串转化成被加密过后的字符串
         * @method _encode
         * @param {Object|String} acm object or string
         * @return {String} encode acm string
         */
        _encode: function (acm) {
            var key, encodedAcm = '';
            if (typeof acm === 'object') {
                for (key in acm) {
                    if (acm.hasOwnProperty(key)) {
                        encodedAcm += key + acm[key];
                    }
                }
            } else { //string
                encodedAcm = acm;
            }

            encodedAcm = this._caesarCode(encodedAcm, 20);
            return encodedAcm;
        },
        /**
         * 将被编码的acm字符串还原
         * @method _decode
         * @param {Object|String} acm object or string
         * @return {String} decoded acm string
         */
        _decode: function (encodedAcm) {
            encodedAcm = decodeURI(encodedAcm);
            return this._caesarCode(encodedAcm, -20);
        },
        /**
         * 按照一定规则合并两个 acm，注意顺序 matters，倾向于保留第一个 acm 的内容，
         * 第二个参数里的值可能被丢弃，规则如下：
         * 按照数据组提供的数据，acm 都是会有 A 字段的
         *
         * a. 如果 acm 有A、B字段，则直接取 acm 的值，否则转向b
         * b. 如果
         *
         * 比如，acm 的值为 AseachCwhatelse，baseacm 为 A
         *
         * @method _merge
         * @param {Object} acm
         * @param {Object} baseacm
         * @return {Object} mergedAcm
         */
        _merge: function (acm, baseAcm) {
            if (acm.A && acm.B) {
                return this._copy(acm);
            }

            var mergedAcm;
            if (acm.A && acm.C){
                if (acm.A === baseAcm.A) {
                    mergedAcm = this._copy(baseAcm);
                    mergedAcm.C = acm.C;
                } else {
                    mergedAcm = this._copy(acm);
                }
            }

            return mergedAcm || {};
        },
        /**
         * shallow copy for plain object
         */
        _copy: function (obj) {
            var i, c = {};
            for (i in obj) {
                if (obj.hasOwnProperty(i)) {
                    c[i] = obj[i];
                }
            }
            return c;
        },
        /**
         * 切分和解析处理acm
         * @method _parse
         * @param {String} acm 要处理的acm
         * @return {Object} acm object
         */
        _parse: function (acm) {
            var partialList = acm.match(this._regAcm),
                acmInfo = {},
                partial;

            while (partialList && partialList.length) {
                partial = partialList.shift();
                acmInfo[partial.substr(0, 1)] = partial.substr(1);
            }

            return acmInfo;
        },
        /**
         * 凯撒密码加密/解密
         * @method _caesarCode
         * @param {String} str 要加密的字符串
         * @param {Number} shift 字母移的位数，取值为1到25或-25到-1，例 shift=2 时 A 转成 C
         * 如果加密的shift是20的话，解密填个-20就行了
         */
        _caesarCode: function (str, shift) {
            var code, i, base,
                newStr = '';
            for (i = code = 0; (code = str.charCodeAt(i)); i++) {
                // 当code的值为大写或者小写字母的时候，同时设置base为A或者a的ASCII值
                if ((code >= 65 && code <= 90 && (base = 65)) || (code >= 97 && code <= 122 && (base = 97))) {
                    // 在增加偏移数量后取模使得值域和定义域相同，+26 是为了保证括号内计算的值为正数
                    code = (code - base + shift + 26) % 26 + base;
                }
                newStr += String.fromCharCode(code);
            }
            return newStr;
        }
    };

    /**
     * 一些和 mtt 相关的帮助函数
     */
    var INIT_MODULE_ID = '0',
        INIT_POSITION_ID = '0';

    var MttTracker = {
        /**
         * 根据传入的模块 id 和模块内部元素的位置信息，获取其对应的完整的mtt对象
         * @param {String} moduleId
         * @param {String|Integer} position
         * @return {Object} 包含mtt五段信息的对象
         */
        getMtt: function (moduleId, position) {
            // siteid pageid pvid 在一个页面中都是确定的
            return {
                siteid: '1',
                pageid: M.PAGE_VIEW_DATA.page,
                moduleid: moduleId || INIT_MODULE_ID,
                position: position || INIT_POSITION_ID,
                pvid: M.PAGE_VIEW_ID
            };
        },
        /**
         * 根据mtt的字符串获取mtt的对象
         * @param {String} [mttStr] 形如 siteid.pageid.moduleid.position.requestid 的字符串
         * @return {Object} 返回根据mttStr解析的mtt对象
         */
        getMttFromStr: function (mttStr) {
            var mttInfo = [];

            if (mttStr) {
                mttInfo = mttStr.split('.');
            }

            return MttTracker.getMtt(mttInfo[2], mttInfo[3]);
        },
        /**
         * 根据mtt对象生成mtt的字符串
         * @param {Object} mtt对象
         * @return {String} mtt 字符串
         */
        getMttStr: function (mtt) {
            return [
                mtt.siteid,
                mtt.pageid,
                mtt.moduleid,
                mtt.position,
                mtt.pvid
            ].join('.');
        },
        /**
         * 发送mtt点击日志
         * @param {Object} mtt对象
         */
        sendClickLog: function (mtt) {
            var mttStr;
            // 如果链接 mtt 信息中的 moduleid 不是初始值，则链接在一个模块中
            // 满足该条件发送模块的点击日志
            if (mtt.moduleid !== INIT_MODULE_ID) {
                mttStr = MttTracker.getMttStr(mtt);
                window._mbq('localEventlog', 'UserAction', 'Click', 'module/click', {
                    mttmod: mttStr
                });
            }
        }
    };
},
// version
'1.0.0',

// dependency
{
    requires: [
        'www-base'
    ]
});
;/*jshint unused:false, plusplus:false, curly:false, laxbreak:true, bitwise:false, devel:true, bitwise:false */
/*global M*/
/* 元素生命周期管理 */
/* 维护者/有问题找: liujingwei02@meituan.com */
/**
 * @module www-hub
 */
M.add("www-hub", function (Y) {
    "use strict";

    Y.namespace('mt.www.Hub');

    var HUB_CLASSNAME = "J-hub";

    var APP_PREFIX = M.APP || "www";

    var DEBUG = ~("" + location.search).indexOf("jslog=verbose");

    // requires array-extras
    if (Y.Promise && typeof Y.Promise.all !== "function") Y.Promise.all = function() { return Y.batch.apply(null, arguments);};

    M._ERRORS = M._ERRORS || [];

    /**
     * chainFn for createPersistentPromise
     * @param {Object}
    **/
    function chainFn(o) {
        if (o instanceof Error) M._ERRORS.push(o);

        return new Y.Promise(function(resolve) {
            resolve(null);
        });
    }

    /**
     * create persistent Promise queue that normalize rejected Promises
     * @param {Function|Promise|String}
    **/
    function createPersistentPromise(p) {
        if (typeof p === "function") p = new Y.Promise(p);
        return Y.when(p, chainFn, chainFn);
    }

    var evtCode = {
        scan: 10,
        render: 20,
        init: 30,
        beforeupdate: 40,
        update: 50,
        teardown: 0
    };

    // get the max evtCode
    var maxEvtCode = 0;
    Y.Object.each(evtCode, function(value, key) {
        if (value > maxEvtCode) maxEvtCode = value;
    });

    // default lifecycle behavior
    var defaultScope = {
        render: function(ndElement) {
        },
        // initialization: setup and event delegation
        init: function (ndElement, config) {},
        // add post/get data before sending AJAX requests
        beforeupdate: function (url, config) {
            if (!url) return null;
            var _config = config || {};
            if (!Y.Lang.isObject(_config)) {
                _config = {
                    args: config
                };
            }
            return Y.merge(_config, { act: url});
        },
        // update DOM with AJAX return values
        update: function (ndElement, value, config) {
            if (!value || !ndElement) return;
            try {
                value = Y.JSON.parse(value);
                if (Y.Lang.isString(value)) {
                    ndElement.setHTML(value);
                } else if (Y.Lang.isObject(value) && value.status !== 404 && value.html) {
                    ndElement.setHTML(value.html);
                }
            } catch (err) {
                ndElement.setHTML(value);
            }
        },
        // when element is about to be destroyed
        teardown: function (ndElement) {
            if (!ndElement) return;

            // remove all event listener
            // on ndElement and its descendents
            ndElement.purge(true);
        }
    };

    /**
     * RequestAnimationFrame Polyfill
     * @param {Function}
    **/
    var raf = window.requestAnimationFrame
        || window.webkitRequestAnimationFrame
        || window.mozRequestAnimationFrame
        || function(cb) { window.setTimeout(cb, 1000 / 60); };

    /**
     * output debug message if requested
     **/
    function logMessage() {
        if (DEBUG && window.console && console.log && console.log.apply) console.log.apply(console, arguments);
    }

    /**
     * converting module name into namespace
     * e.g.
     * www-deal-firstscreen -> DealFirstscreen
     * www-hub -> Hub
     * if namespace and module name are quite different
     * you could define data-hubnamespace
     * @param {String}
    **/
    function toCamelCase(str) {
        if (!str) return "";

        var arr = str.split("-");
        var result = "";
        var item = null;
        while ((item = arr.shift())) {
            result += item.charAt(0).toUpperCase() + item.substr(1);
        }
        return result;
    }


    /**
     * find scope by namespace
     * e.g.
     * Deal -> Y.mt.www.Deal
     * Deal.initDemo -> Y.mt.www.Deal.initDemo
     * @param {String}
     **/
    function findScope(namespace) {
        if (!namespace) return defaultScope;

        // where to look for
        var rootScope = Y.mt[APP_PREFIX] || Y.mt;

        try {
            var chunks = namespace.split(".");
            var chunk;
            var podScope = rootScope;
            while ((chunk = chunks.shift())) {
                podScope = podScope[chunk];
            }
            // safety caution:
            // prevent executing rootScope.init()
            return (!podScope || podScope === rootScope)? defaultScope : podScope;
        } catch (e) {
            return defaultScope;
        }
    }

    /**
     * customized Y.bind
     * @param {String}
     * @param {String}
     **/
    function _execute(name, namespace) {
        return function() {
            // delay find scope until Y.use brings us the script
            var scope = findScope(namespace) || defaultScope;
            return (scope[name] || defaultScope[name]).apply(scope, arguments);
        };
    }

    /**
     * wrapper for easier behavior override
     * when calling execute.init(scope, arg1, arg2, ...)
     * if scope.init exists, it would call scope.init(arg1, arg2, ...)
     * or else calls defaultScope.init(arg1, arg2, ...)
     * @param {String}
     **/
    function getExecute(namespace) {
        var execute = {};
        Y.Object.each(defaultScope, function(fn, name) {
            execute[name] = _execute(name, namespace);
        });
        return execute;
    }


    // end of helper functions


    /**
     * encapuslation of DOM mainpulation
     * handles event from PodCollection
     * and forward it to the proper function
     * @constructor
     **/
    function Pod(element) {
        if (!element) return null;

        this.element = element;
        this.stamp = Y.stamp(element);

        // not used at the moment,
        // in case we should need located the element
        this.setData("stamp", this.stamp);

        // set state to attribute
        // see this.evtCode
        if (!this.getState()) this.setState(0);

        this.url = this.getData("url");

        var config = this.getData("config");
        try {
            this.config = Y.JSON.parse(config);
        } catch (err) {
            this.config = config;
        }

        if (this.getData("laziness") === "load") {
            this.setState(1);
        }

        config = null;

        this.publishEvent();
        this.publishNodeEvent();
    }

    /**
     * publish all Pod events
     **/
    Pod.prototype.publishEvent = function() {
        Y.Array.each([
            "scan",
            "render",
            "init",
            "beforeupdate",
            "update",
            "teardown"
        ], function(evt) {
            this.publish(evt, {
                defaultFn: function(e) {
                    logMessage(this.constructor.name, e.type);
                    this.setState(e.type);

                    this[evt](e);
                }
            });

            // check if defaultFn should be run
            this.on(evt, this.checkState);
        }, this);

        this.publish("ready", {
            fireOnce: true,
            async: true
        });

        var enableBubbles = { bubbles: true };

        // allow some events to bubble to PodCollection
        this.publish("resume", enableBubbles);
        this.publish("didReceiveScan", enableBubbles);
        this.publish("didReceiveBeforeupdate", enableBubbles);
        this.publish("didReceiveUpdate", enableBubbles);
    };

    /**
     * 提供回调事件给 pod
     * 对于需要特定事件（比如滚动）触发更新的模块
     * 我们并不希望低级 pod 知道上一层的函数构造
     * 我们也不希望由开发者手工调用 Y.mt.www.Hub 的公开接口
     * 而是要求元素发送事件，由 Pod 转发给自己然后负责处理
     * USAGE:
     * 任何时候 Node.fire("hub:pause")
     * 可以暂停 Node 元素的处理
     * 然后在需要的时候 Node.fire("hub:resume")
     * 可以继续执行这个 Node 的处理/更新
     **/
    Pod.prototype.publishNodeEvent = function() {
        Y.Array.each(["pause", "resume"], function(evt) {
            // these events will bubble to podCollection
            this.on(evt, this[evt]);
            this.element.on("hub:" + evt, function() {
                this.fire(evt);
            }, this);
        }, this);
    };

    /**
     * fire defaultScope.scan
     * @param {EventFacade}
     **/
    Pod.prototype.scan = function(e) {
        // accepts module name with M.APP prefix
        // www-deal-firstscreen -> this.module: www-deal-firstscreen, module: deal-firstscreen
        var module = this.getData("module");
        var prefix = APP_PREFIX + "-";

        var namespace;

        if (module) {
            if (module.indexOf(prefix) === 0) {
                namespace = module.substr(prefix.length);
            }
            else {
                namespace = module;
            }
        }

        namespace = this.getData("namespace") || toCamelCase(namespace);
        this.execute = getExecute(namespace);

        // if we were to stop JS/CSS loading
        // then there is no need to call any function

        var css = this.getData("css");

        if (css || module) {
            this.fire("didReceiveScan", {
                css: css,
                module: module
            });
        }
    };

    /**
     * fire pod.scope.render
     * @param {EventFacade}
     **/
    Pod.prototype.render = function(e) {
        var type = e.type;
        this.execute[type](this.element);
    };

    /**
     * fire pod.scope.init
     * @param {EventFacade}
     **/
    Pod.prototype.init = function(e) {
        var type = e.type;
        this.execute[type](this.element, this.config);
    };

    /**
     * fire pod.scope.beforeupdate
     * @param {EventFacade}
     **/
    Pod.prototype.beforeupdate = function(e) {
        var type = e.type;
        var updateInfo = this.execute[type](this.url, this.config);

        if (!updateInfo) return;
        this.fire("didReceiveBeforeupdate", {
            updateInfo: Y.JSON.stringify(updateInfo),
            stamp: this.stamp
        });
    };

    /**
     * fire pod.scope.update
     * @param {EventFacade}
     **/
    Pod.prototype.update = function(e) {
        var type = e.type;
        // just so we are almost compatiable with traditional Y.io callback
        raf(Y.bind(this.execute[type], this, this.element, e.value, this.config));
        this.fire("didReceiveUpdate", {
            stamp: this.stamp
        });
    };

    /**
     * fire pod.scope.teardown
     * @param {EventFacade}
     **/
    Pod.prototype.teardown = function(e) {
        var type = e.type;
        this.execute[type](this.element);
    };

    /**
     * check if element is paused
     * if so, abort following events
     * @param {EventFacade}
     **/
    Pod.prototype.checkState = function (e) {
        var type = e.type;

        // on some rare occasion,
        // the element could be removed from the document
        // we have to validat it before retrieving the element
        if (!this.element._node) e.preventDefault();

        var state = this.getState();

        if (state & 1) e.preventDefault();

        if (state >= evtCode[type]) e.preventDefault();
    };

    /**
     * pause execution of a certain pod
     **/
    Pod.prototype.pause = function() {
        logMessage("pause from", this.stamp);
        var state = this.getState();

        // e.g.
        // 10 -> 11
        // 11 -> 11
        this.setState(state | 1);
    };

    /**
     * resume execution of a certain pod
     **/
    Pod.prototype.resume = function() {
        logMessage("resume from", this.stamp);
        var state = this.getState();

        // e.g.
        // 11 -> 10
        // 10 -> 10
        this.setState((state | 1) ^ 1);
    };

    /**
     * wrapper for data attribute getter
     * @param {String}
     **/
    Pod.prototype.getData = function (attr) {
        return this.element.getAttribute("data-hub" + attr);
    };

    /**
     * wrapper for data attribute setter
     * YUI's setData does not guarantee to write the
     * value on the DOM
     * @param {String, String}
     **/
    Pod.prototype.setData = function (attr, value) {
        if (!attr) return;
        if (typeof value === "undefined") return this.element.removeAttribute("data-hub" + attr);

        this.element.setAttribute("data-hub" + attr, value);
    };

    /**
     * wrapper for getting pod state
     * state needs to be converted to number
     **/
    Pod.prototype.getState = function() {
        return 1 * this.getData("state") || 0;
    };

    /**
     * wrapper for setting pod state
     **/
    Pod.prototype.setState = function(value) {
        if (evtCode.hasOwnProperty(value)) value = evtCode[value];
        this.setData("state", value);
    };

    /**
     * determine whether the pod is ready or not
     **/
    Pod.prototype.isReady = function() {
        // if you pause in `update`
        // pod.ready will NOT be fired
        return (this.getState() === maxEvtCode);
    };



    /**
     * collection of Pods
     * receives commands from public interface or global event
     * and dispatch proper events to each Pod
     * @constructor
     **/
    function PodCollection() {
        // store JavaScript module for Y.use
        this.uses = [APP_PREFIX + "-base"];

        // store CSS filenames
        this.css = [];
        // how many CSS file is loaded
        this.cssIndex = 0;

        // a persistent Promise
        this.promise = Y.when();

        // store all Pods
        this.podList = {};

        // store queries before sending them to server
        this.updateInfo = {};

        // store values from AJAX
        this.updateValue = {};

        // declares all PodCollection events
        this.publishEvent();
    }

    /**
     * publish all Pod events
     **/
    PodCollection.prototype.publishEvent = function() {
        Y.Array.each([
            "didReceiveScan",
            "didReceiveBeforeupdate",
            "didReceiveUpdate"
        ], function(evt) {
            this.publish(evt, {
                defaultFn: this[evt]
            });
        }, this);

        Y.Array.each([
            "resume",
            "ready"
        ], function(evt) {
            this.publish(evt, {
                async: true,
                defaultFn: this[evt]
            });
        }, this);
    };

    /**
     * fire `scan` of each Pod
     * and fetch resources if necessary
     * @param {EventFacade}
     **/
    PodCollection.prototype.scan = function() {
        logMessage(this.constructor.name, "scan");

        // enumerate a new list of stylesheets
        this.styleSheets = Y.all("link[rel='stylesheets']");

        Y.Object.each(this.podList, function(pod) {
            pod.fire("scan");
        });

        // mark as not needed
        this.styleSheets = null;

        var self = this;

        return new Y.Promise(function(resolve) {
            Y.batch(self.when(self.getCSS()), self.when(self.getJS())).then(function() {
                resolve();
            });
        });
    };

    /**
     * receive `scan` from Pod
     * @param {EventFacade}
     **/
    PodCollection.prototype.didReceiveScan = function(e) {
        logMessage(this.constructor.name, e.type, "css", e.css, "module", e.module);
        if (e.css) this.addCSS(e.css);
        if (e.module) this.uses.push(e.module);
    };

    /**
     * fire `render`, `init` or `beforeupdate` `ready` of each Pod
     * @param {EventFacade}
     **/
    Y.Array.each(["render", "init", "beforeupdate"], function(evt) {
        PodCollection.prototype[evt] = function() {
            logMessage(this.constructor.name, evt);

            // just for beforeupdate
            this.updateInfo = {};

            Y.Object.each(this.podList, function(pod) {
                pod.fire(evt);
            });
        };
    }, this);

    /**
     * receive `beforeupdate` from Pod
     * @param {EventFacade}
     **/
    PodCollection.prototype.didReceiveBeforeupdate = function(e) {
        logMessage(this.constructor.name, e.type);
        if (e.updateInfo && e.stamp) {
            this.updateInfo[e.stamp] = e.updateInfo;
        }
    };

    /**
     * fire `update` of each Pod
     * when multiact AJAX returns
     * @param {EventFacade}
     **/
    PodCollection.prototype.update = function() {
        logMessage(this.constructor.name, "update");
        this.updateValue = this.updateValue || {};

        var self = this;

        return new Y.Promise(function(resolve) {
            // protect this.updateInfo against volatility
            createPersistentPromise(self.requestUpdate(self.updateInfo))
            .then(Y.bind(self.dispatchUpdate, self))
            .then(function() {
                resolve(null);
            });
        });
    };

    /**
     * send multiact request
     * @param {Object} updateInfo from podList
     **/
    PodCollection.prototype.requestUpdate = function (updateInfo) {
        logMessage(this.constructor.name, "requestUpdate", updateInfo);

        var self = this;
        
        return new Y.Promise(function(resolve, reject) {
            if (Y.Object.isEmpty(updateInfo)) return resolve(null);
            // to be safe
            self.updateValue = self.updateValue || {};
            // aggregation service URL
            // include base document pathname so that multiact would not appear to be the same url
            // http://task.sankuai.com/browse/FE-2769
            var MULTIACT_URL = "/multiact/default/" + (location.pathname || "");
            Y.io(MULTIACT_URL, {
                method: "post",
                data: updateInfo,
                on: {
                    success: function(id, response) {
                        // clear updateInfo so the same data/URL is never sent twice
                        self.updateInfo = {};

                        var data = Y.JSON.parse(response.responseText);
                        Y.mix(self.updateValue, data, true);
                        resolve(null);
                    },
                    // reject on failure and log status
                    failure: function(id, response) {
                        reject(new Error("multiact fails with status " + (response.status || 'null')));
                    }
                }
            });
        });
    };

    /**
     * dispatch updateValue to each pod after multiact success
     **/
    PodCollection.prototype.dispatchUpdate = function() {
        logMessage(this.constructor.name, "dispatchUpdate");

        Y.Object.each(this.podList, function(pod, stamp) {
            if (this.updateValue.hasOwnProperty(stamp)) {
                logMessage("Dispatching update to", stamp);
                pod.fire("update", { value: this.updateValue[stamp] });
            }
        }, this);
    };

    /**
     * receive `update` from Pod
     * @param {EventFacade}
     **/
    PodCollection.prototype.didReceiveUpdate = function(e) {
        logMessage(this.constructor.name, e.type, e.stamp);
        if (e.stamp) {
            delete this.updateValue[e.stamp];
        }
    };

    /**
     * make sure `resume` fires after current digest cycle
     * @private
     **/
    PodCollection.prototype.resume = function() {
        this.runAfterCurrentDigestCycle(this.fireSequence);
    };

    PodCollection.prototype.fireSequence = function() {
        this.readiness = false;
        this.then(this.scan, this.render, this.init, this.beforeupdate, this.update);
        this.then(Y.bind(this.fire, this, "ready"));
    };

    /**
     * start PodCollection lifecycle process
     * @param {Node|DOMElement|String|Null}
     **/
    PodCollection.prototype.start = function(ndElement) {
        ndElement = ndElement || ("." + HUB_CLASSNAME);

        // update Pod list
        this.then(function() {
            logMessage(this.constructor.name, "start");

            Y.all(ndElement).each(this.addElement, this);
        });

        this.fireSequence();
    };

    /**
     * fire PodCollection ready when lifecycle completes
     **/
    PodCollection.prototype.ready = function() {
        logMessage(this.constructor.name, "ready");

        Y.Global.fire('PageHeight:change');
        Y.Object.each(this.podList, function(pod) {
            if (pod.isReady()) pod.fire("ready");
        });
        this.readiness = true;
    };

    /**
     * append function to internal event queue
     * similar to Promise.then
     * @param {Function}
     * then resolved value will be stored in an object with given key
     **/
    PodCollection.prototype.then = function() {
        if (arguments.length) {
            for (var i = 0; i < arguments.length; i++) {
                this.promise = this.promise.then(Y.bind(arguments[i], this));
            }
        }

        return this;
    };

    /**
     * append function to internal event queue
     * similar to Y.when
     * @param {Function|Promise|Object}
     * then resolved value will be stored in an object with given key
     **/
    PodCollection.prototype.when = function(p) {
        if (typeof p === "function") p = Y.bind(p, this);
        return createPersistentPromise(p);
    };

    /**
     * fire PodCollection ready when lifecycle completes
     **/
    PodCollection.prototype.runAfterCurrentDigestCycle = function(fn) {
        if (this.readiness) {
            fn.call(this);
        } else {
            this.onceAfter("ready", function() {
                logMessage("Running after current digest cycle");
                fn.call(this);
            });
        }
    };

    /**
     * init each Pod
     * @param {Node}
     **/
    PodCollection.prototype.addElement = function(ndElement) {
        if (!ndElement.hasClass(HUB_CLASSNAME)) return;

        if (this.getPodByElement(ndElement)) return;

        var pod = new Pod(ndElement);

        if (!pod) return;

        // bubbles Pod event to PodCollection
        pod.addTarget(this);

        this.podList[pod.stamp] = pod;
    };

    /**
     * find pod by element
     * @param {Node|DOMElement|String|Null}
     **/
    PodCollection.prototype.getPodByElement = function(ndElement) {
        if (!ndElement) return null;

        // check if is instanceof Y.Node
        if (!ndElement.hasOwnProperty("_node")) ndElement = Y.one(ndElement);

        var stamp = ndElement.getAttribute("data-hubstamp");
        if (!stamp) return null;

        var pod = this.podList[stamp];

        if (pod && pod instanceof Pod) return pod;

        return null;
    };

    /**
     * check if podCSS is loaded
     * if not, add it to this.css array
     * @param {String}
     **/
    PodCollection.prototype.addCSS = function(podCSS) {
        var foundInStyleSheets;
        Y.Array.each(podCSS.split(";"), function (css) {
            // if css is empty
            if (!css) return;
            // if css is already in the list
            if (~Y.Array.indexOf(this.css, css)) return;

            // if css is loaded in anyother way
            foundInStyleSheets = false;

            if (this.styleSheets instanceof Y.NodeList) {
                this.styleSheets.some(function(stylesheet) {
                    if (~stylesheet.get("href").indexOf(css)) {
                        foundInStyleSheets = true;
                        return true;
                    }
                });
            }

            // push it in the list if css is new
            if (!foundInStyleSheets) this.css.push(css);
        }, this);
    };

    /**
     * Promise wrapper for Y.Get.css
     **/
    PodCollection.prototype.getCSS = function() {
        var unsentCSS = this.css.slice(this.cssIndex);

        return new Y.Promise(function(resolve, reject) {
            if (!unsentCSS.length) return resolve(null);

            unsentCSS = (M.COMBO_BASE || "") + unsentCSS.join(";");

            Y.Get.css(unsentCSS, function(err) {
                if (err) {
                    reject(err);
                    return;
                }

                podCollection.cssIndex = podCollection.css.length;
                logMessage("Fetched CSS From", unsentCSS);
                resolve(null);
            });
        });
    };

    /**
     * Promise wrapper for Y.use
     **/
    PodCollection.prototype.getJS = function () {
        var uses = this.uses = Y.Array.dedupe(this.uses);

        return new Y.Promise(function(resolve) {
            // 一次拿回所有模块，减少 JS 请求
            Y.use(uses, function() {
                logMessage("Fetched JS", uses);
                resolve(null);
            });
        });
    };


    /**
     * check laziness parameter
     * @param {String}
     **/
    PodCollection.prototype.checkLaziness = function(laziness) {
        this.runAfterCurrentDigestCycle(function() {
            Y.Object.each(this.podList, function(pod) {
                if (pod.getData("laziness") === laziness) {
                    pod.element.fire("hub:resume");
                }
            }, this);
        });
    };

    var eventDefaultConfig = {
        emitFacade: true,
        async: false,
        bubbles: false
    };

    // adopts EventTarget with customized prefix
    Y.augment(Pod, Y.EventTarget, false, null, eventDefaultConfig);
    Y.augment(PodCollection, Y.EventTarget, false, null, eventDefaultConfig);

    // we are not scanning elements at the stage
    // so it is safe to create a new instance before domready
    var podCollection = new PodCollection();

    // create synthetic event for attaching callback
    Y.Event.define("hub:ready", {
        on: function(node, subscription, notifier) {
            var pod = podCollection.getPodByElement(node);

            if (pod) {
                subscription._handle = pod.on("ready", function(e) {
                    notifier.fire(e);
                });
            }
        },

        // maybe not a good idea to allow detaching
        detach: function (node, subscription, notifier) {
            subscription._handle.detach();
        }
    });

    /**
     * Lifecycle management for elements
     * @namespace Y.mt
     * @param {Node|DOMElement|String|Null}
     */
    Y.mt.www.Hub.init = function(ndElement) {
        podCollection.start(ndElement);
    };

    // register auto init function
    M._autoinit = M._autoinit || [];
    M._autoinit.push(function(element) {
        // TODO: handle children element as well
        if (element && element.className && (-1 !== element.className.indexOf(HUB_CLASSNAME))) {
            logMessage("start Hub autoinit", element);
            Y.mt.www.Hub.init(element);
        }
    });

    // quick fix for afterload
    Y.mt.Event.load(function() {
        podCollection.checkLaziness("load");
    });
},
//version
"1.0.0",
//dependency
{
    requires: ["node", "mt-event", "event-custom", "io-base", "promise"]
});
;/*jshint white:true, unused:true*/
/**
 * 主站顶部搜索
 *
 * @module search-box
 */
M.add('search-box', function (Y) {
    var $IO = Y.io,
        trim = Y.Lang.trim;

    var $Util = Y.mt.util,
        $Cookie = $Util.Cookie,
        $Macro = Y.mt.macro,
        Tracker = Y.mt.www.tracker,
        Placeholder = Y.mt.plugin.Placeholder;

    // NOTE 全局变量，标示是否为商家搜索
    var isShopSearch = false;

    /**
     * 搜索历史管理: 添加条目，删除条目，搜索匹配
     * 存储的数据结构为[[用户输入的关键词, [用户选中的关键词, ...]], ...]
     */
    function SearchHistory() {
        this.initEntries();
    }
    // 由于cookie限制最多保存的搜索历史条数
    try {
        var supported = window.localStorage ? true : false;
        if (supported) {
            // 测试是否可以设置localstorage，在safari隐私模式下localstorage不可设值
            localStorage.setItem('testSotrage', '');
            localStorage.removeItem('testSotrage');
        }
        SearchHistory.SUPPORT_LOCALSTORAGE = supported;
    }catch (ex) {
        SearchHistory.SUPPORT_LOCALSTORAGE = false;
    }
    SearchHistory.SEARCH_HISTORY_MAX_LIMIT = SearchHistory.SUPPORT_LOCALSTORAGE ? 10 : 5;
    // 存储搜索历史的cookie
    SearchHistory.SEARCH_HISTORY_COOKIE_NAME = 'sh';
    SearchHistory.prototype = {
        constructor: SearchHistory,
        /**
         * 更新搜索历史的cookie缓存
         * @method initEntries
         */
        initEntries: function () {
            var cookie;
            if (SearchHistory.SUPPORT_LOCALSTORAGE) {
                cookie = localStorage.getItem(SearchHistory.SEARCH_HISTORY_COOKIE_NAME);
            } else {
                cookie = $Cookie.get(SearchHistory.SEARCH_HISTORY_COOKIE_NAME);
            }
            this.entries = cookie ? Y.JSON.parse(cookie) : [];
        },
        /**
         * 添加1条搜索历史,添加时保证队列长度不超过限额
         * @method add
         * @param {String} item
         */
        add: function (originKeyword, item) {
            if (originKeyword === '') return;
            var instance = this,
                entries = instance.entries,
                lowcaseItem = item.toLowerCase(),
                isItemFound,
                originKeywordIndex = -1;

            isItemFound = Y.some(entries, function (entry, i) {
                var len = entry[1].length;
                while (len--) {
                    if (entry[1][len].toLowerCase() === lowcaseItem) {
                        entry[1].splice(len, 1);
                        entry[1].push(item);
                        entries.splice(i, 1);
                        entries.push(entry);
                        instance.update();
                        return true;
                    }
                }
            });
            if (isItemFound) return;

            Y.some(entries, function (entry, index) {
                if (entry[0] === originKeyword) {
                    originKeywordIndex = index;
                    return true;
                }
            });
            if (originKeywordIndex > -1) {
                entries[originKeywordIndex][1].push(item);
            } else {
                entries.push([originKeyword, [item]]);
            }
            if (instance.getLength() > SearchHistory.SEARCH_HISTORY_MAX_LIMIT) {
                instance.shift();
            }
            instance.update();
        },
        /**
         * 删除1条搜索历史
         * @method remove
         * @param {String} item
         */
        remove: function (item) {
            var instance = this,
                entries = instance.entries,
                entry,
                pos,
                i, len = entries.length;
            if (len === 0) return;

            for (i = 0; i < len; ++i) {
                entry = entries[i];
                pos = Y.Array.indexOf(entry[1], item);
                if (pos === -1) continue;
                entry[1].splice(pos, 1);
                if (entry[1].length === 0) {
                    entries.splice(i--, 1);
                    len--;
                }
            }

            instance.update();
            instance.fire('remove', item);
        },
        /**
         * 搜索是否有匹配
         * @method search
         * @param {String} keyword
         */
        search: function (keyword) {
            var hitEntries = [],
                showMaxEntries = keyword ? 3 : 10;

            Y.each(this.entries, function (entry) {
                // 如果输入了“ao”，选择了“澳门”
                var lowcaseEntry = entry[0].toLowerCase(),
                    lowcaseKw = keyword.toLowerCase();
                if (lowcaseEntry.indexOf(lowcaseKw) === 0) {
                    hitEntries = hitEntries.concat(entry[1]);
                } else {
                    var len = entry[1].length;
                    while (len--) {
                        if (entry[1][len].indexOf(lowcaseKw) === 0) {
                            hitEntries.push(entry[1][len]);
                        }
                    }
                }
            });
            
            hitEntries = hitEntries.splice(0, showMaxEntries);

            return hitEntries;
        },
        /**
         * 更新搜索历史的cookie缓存
         * @method update
         */
        update: function () {
            if (SearchHistory.SUPPORT_LOCALSTORAGE) {
                localStorage.setItem(SearchHistory.SEARCH_HISTORY_COOKIE_NAME, Y.JSON.stringify(this.entries));
            } else {
                $Cookie.set(
                    SearchHistory.SEARCH_HISTORY_COOKIE_NAME,
                    Y.JSON.stringify(this.entries),
                    $Cookie.getExpiresDate(30, 0, 0),
                    '/',
                    '.' + M.DOMAIN_HOST
                );
            }
        },
        /**
         * 弹出一项内容
         * @method shift
         */
        shift: function () {
            var entries = this.entries;
            if (entries[0][1].length === 1) {
                entries.shift();
            } else {
                entries[0][1].shift();
            }
        },
        /**
         * 计算搜索历史条目数
         * @method getLength
         * @return {Number}
         */
        getLength: function () {
            var entries = this.entries,
                res = 0, len = entries.length;
            while (len--) {
                res += entries[len][1].length;
            }
            return res;
        }
    };
    Y.augment(SearchHistory, Y.EventTarget);

    /***
     * 搜索数据
     */
    function SearchData(action) {
        this.action = action;
        this.originKeyword = '';
        this.async = null;
        this.timer = null;
        this.cache = {};
        // 当获取的数据可以加入历史并且其显示的来源为历史,默认为true
        this.showHistory = true;
    }
    SearchData.DELAY = 500;
    SearchData.prototype = {
        fetch: function (keyword, callback) {
            var instance = this,
                cacheTips = instance.cache[keyword];

            if (Y.Lang.isArray(cacheTips)) {
                instance.setShowHistoryStatus(cacheTips.concat(), instance);
                callback(cacheTips.concat());
                return;
            }

            if (instance.timer) return;
            instance.timer = window.setTimeout(function () {
                instance.request(callback);
                instance.timer = null;
            }, SearchData.DELAY);
        },
        request: function (callback) {
            var instance = this,
                action,
                keyword = instance.originKeyword.replace(/'/g, ''),
                async = instance.async;
            if (!keyword) {
                callback([]);
                return;
            }
            if (async && async.isInProgress()) {
                async.abort();
            }
            action = Tracker.appendTrackingParams(instance.action + keyword);
            async = $IO(action, {
                method: 'POST',
                on: {
                    success: function (id, o) {
                        var res = $Util.getEvalRes(o);

                        instance.setShowHistoryStatus(res, instance);
                        instance.updateCache(keyword, res);
                        callback(res);
                    }
                }
            });
        },

        // 判断是否禁止展示搜索历史
        setShowHistoryStatus: function (data, instance) {
            // 获取数据，设置增加历史记录为默认行为
            instance.showHistory = true;
        },

        updateCache: function (kw, content) {
            if (Y.Lang.isArray(content) && content.length > 0) {
                this.cache[kw] = content.concat();
            } else {
                this.cache[kw] = null;
            }
        }
    };

    /*
     * 下拉框建议
     */
    function Suggestion(ndContainer) {
        this.ndContainer = ndContainer;
        this.ndPanel = ndContainer.next('.' + Suggestion.CLASS_SMART_QUERY_PANEL);
        this.list = [];
        this.status = Suggestion.STATUS_NULL;
        this.originKeyword = '';
        // smartbox 区状态变化的计数器，显示、隐藏、内容变化都会加一
        this.suggestChangeCounter = 0;
        this.acmViewTimer = null;
        this.preventSubmission = false;
    }
    Suggestion.MAX_SUGGEST_COUNT = 10;
    Suggestion.STATUS_NULL = 0;
    Suggestion.STATUS_SHOW = 1;
    Suggestion.STATUS_HIDDEN = 2;
    Suggestion.STATUS_SELECTED = 3;
    Suggestion.DELAY = 200;
    Suggestion.CLASS_ITEM = 'search-suggest__item';
    Suggestion.CLASS_ITEM_BORDER = 'search-suggest__item--border';
    Suggestion.CLASS_HOVER = 'search-suggest__item--over';
    Suggestion.CLASS_HISTORY = 'search-suggest__item--history';
    Suggestion.CLASS_ITEM_COUNT = 'search-suggest__item--count';
    Suggestion.CLASS_SMART_QUERY_HTML = 'smart-query-html';
    Suggestion.CLASS_SMART_QUERY_PANEL = 'smart-query-panel';
    Suggestion.CLASS_SMART_QUERY = 'smart-query-content';
    Suggestion.CLASS_SMART_QUERY_ITEM = 'smart-query__item';
    Suggestion.SEARCH_HISTORY_TPL = '<li class="' + Suggestion.CLASS_ITEM + ' {tipClass}"><a href="javascript:void(0)" class="remove" data-tip="{tipContent}">删除</a>{tipContent}</li>';
    Suggestion.ACM_VIEW_SEND_THRESHOLD = 200;
    Suggestion.prototype = {
        constructor: Suggestion,
        render: function () {
            var html = '',
                encode = Y.mt.lang.htmlEscape,
                instance = this,
                addRankIcon = false,
                addHotIcon = false,
                hasCounter = true,
                iconCounter = 0;

            for (var index = 0, len = instance.list.length; index < len; index++) {
                var item = instance.list[index],
                    content = '',
                    amount = '',
                    smartQuery = '',
                    text = '';

                initItemProperty();

                switch (item.type) {
                case 'tip':
                    content = '<li class="search-suggest__tip">' + item.tip + '</li>';
                    break;
                case 'history':
                    content = Y.Lang.sub(Suggestion.SEARCH_HISTORY_TPL, {
                        tipClass: Suggestion.CLASS_HISTORY,
                        tipContent: encode(item.word)
                    });
                    break;
                case 'suggestion':
                    smartQuery = renderCldTemplate(item);

                    if (!isShopSearch && item.count >= 0 && hasCounter) {
                        amount = '<span class="' + Suggestion.CLASS_ITEM_COUNT + '">约' + item.count + '个团购</span>';
                    }
                    if (smartQuery) {
                        text = '<span class="search-suggest__keyword">';
                        if (addRankIcon) {
                            text += '<span class="info-index info-index-' + (++iconCounter) + '"></span>';
                        }
                        text += item.word;
                        if (addHotIcon) {
                            text += '<span class="hot"></span>';
                        }
                        text += '</span>';
                    } else {
                        text = item.word;
                    }

                    content = '<li class="' + Suggestion.CLASS_ITEM + ' cf">' + amount + smartQuery + text + '</li>';
                    break;
                }

                html += content;
            }

            function renderCldTemplate(item) {
                var retCld = '';
                if (item['sec_rec'].length) {
                    retCld += '' +
                        '<div class="' + Suggestion.CLASS_SMART_QUERY_HTML + '" style="display:none">' +
                        '   <h5>' + item.word + '</h5>' +
                        '   <div class="smart-query-wrapper">' +
                        '       <ul class="smart-query">';
                    Y.each(item['sec_rec'], function (query) {
                        retCld += '<li class="' + Suggestion.CLASS_SMART_QUERY_ITEM  + '">' + query.word + '</li>';
                    });
                    retCld += '' +
                        '       </ul>' +
                        '   </div>' +
                        '</div>';
                }
                return retCld;
            }

            function initItemProperty() {
                addHotIcon = false;
                addRankIcon = false;
                hasCounter = true;
            }
            this.ndContainer.setHTML(html);
        },
        bind: function () {
            var instance = this,
                ndContainer = instance.ndContainer,
                ndPanel = instance.ndPanel,
                ndItemHover = null,
                mouseHandle = null,
                isTracing = false,
                timerTrace = null,
                regionPanel,
                pCursor = {},
                cursor = {};

            ndContainer.on('hover', beginMouseTrace, stopMouseTrace);
            ndPanel.on('hover', beginMouseTrace, stopMouseTrace);

            ndContainer.delegate('hover', function () {
                ndItemHover = this;
                if (isTracing) return;
                instance.highlight(this);
            }, function () {
                ndItemHover = null;
                if (ndPanel.getStyle('display') !== 'none' && !isTracing) {
                    regionPanel = ndPanel.get('region');
                    trackMouse();
                }
            }, '.' + Suggestion.CLASS_ITEM);

            ndContainer.delegate('click', function (e) {
                e.halt(true);
                instance.remove(this.ancestor('.' + Suggestion.CLASS_ITEM));
            }, '.remove');

            ndContainer.delegate('click', function () {
                var index = instance.indexOf(this),
                    item = instance.list[index];
                instance.fire('select', item);
            }, '.' + Suggestion.CLASS_ITEM);

            ndPanel.delegate('click', function (e) {
                e.halt();
                var indexItem = instance.getHighlightedIndex(),
                    index = ndPanel.all('.' + Suggestion.CLASS_SMART_QUERY_ITEM).indexOf(this),
                    itemQuery = instance.list[indexItem]['sec_rec'][index];
                itemQuery.type = 'smartquery';
                instance.fire('selectSmartQuery', itemQuery);
            }, '.' + Suggestion.CLASS_SMART_QUERY_ITEM);

            function trackMouse() {
                if (inTri()) {
                    isTracing = true;
                    window.setTimeout(function () {
                        trackMouse();
                    }, Suggestion.DELAY);
                    return;
                }
                isTracing = false;
                if (timerTrace) window.clearTimeout(timerTrace);
                if (!inPanel()) {
                    instance.dehighlight();
                    if (ndItemHover) instance.highlight(ndItemHover);
                }
            }

            function beginMouseTrace() {
                if (mouseHandle) return;
                mouseHandle = Y.one(document).on('mousemove', function (e) {
                    pCursor.x = cursor.x;
                    pCursor.y = cursor.y;
                    cursor.x = e.pageX;
                    cursor.y = e.pageY;
                });
            }

            function stopMouseTrace() {
                if (!mouseHandle) return;
                mouseHandle.detach();
                mouseHandle = null;
            }

            function inTri() {
                var pTL = {
                        x: regionPanel[0],
                        y: regionPanel[1]
                    },
                    pBL  = {
                        x: regionPanel[0],
                        y: regionPanel[1] + regionPanel.height
                    },
                    crossProductTL,
                    crossProductBL;
                crossProductTL = (pTL.x - pCursor.x) * (cursor.y - pCursor.y) - (pTL.y - pCursor.y) * (cursor.x - pCursor.x);
                crossProductBL = (pBL.x - pCursor.x) * (cursor.y - pCursor.y) - (pBL.y - pCursor.y) * (cursor.x - pCursor.x);
                return crossProductTL > 0 && crossProductBL < 0 && cursor.x < pTL.x;
            }

            function inPanel() {
                return (cursor.x >= regionPanel[0] && cursor.x <= regionPanel[0] + regionPanel.width) &&
                    (cursor.y >= regionPanel[1] && cursor.y <= regionPanel[1] + regionPanel.height);
            }
        },
        show: function (list) {
            if (this.status === Suggestion.STATUS_SHOW || this.status === Suggestion.STATUS_SELECTED) {
                this.hide();
            }
            this.status = Suggestion.STATUS_SHOW;
            this.list = list.slice(0, Suggestion.MAX_SUGGEST_COUNT);
            this.render();
            // 内容为空时不显示
            if (this.ndContainer.getHTML()) {
                this.ndContainer.setStyle('height', 'auto').show();
            }

            this.suggestChangeCounter++;
            var self = this,
                viewCount = self.suggestChangeCounter;
            if (self.acmViewTimer) {
                clearTimeout(self.acmViewTimer);
            }

            self.acmViewTimer = setTimeout(function () {
                if (viewCount !== self.suggestChangeCounter) {
                    return;
                }

                self._sendSmartBoxView(self.list);
            }, Suggestion.ACM_VIEW_SEND_THRESHOLD);
        },
        hide: function () {
            this.status = Suggestion.STATUS_HIDE;
            this.ndContainer.hide();
            this.ndPanel.hide();
            this.suggestChangeCounter++;
        },
        remove: function (ndItem) {
            var instance = this,
                index,
                data;
            index = instance.indexOf(ndItem);
            ndItem.remove();
            data = instance.list[index];
            instance.list.splice(index, 1);
            instance.fire('remove', data);
        },
        destroy: function () {
            this.status = Suggestion.STATUS_NULL;
            this.ndContainer.hide().setHTML('');
            this.ndPanel.hide();
            this.list = [];
        },
        size: function () {
            return this.list.length;
        },
        indexOf: function (ndItem) {
            return this.ndContainer.all('> li').indexOf(ndItem);
        },
        highlight: function (ndItem) {
            var index,
                dataHovered = 'has-hovered',
                item;
            this.dehighlight();
            // NOTE 需先执行dehighlight，该方法会修改status
            this.status = Suggestion.STATUS_SELECTED;
            if (ndItem.hasClass(Suggestion.CLASS_ITEM)) {
                ndItem.addClass(Suggestion.CLASS_HOVER);
                index = this.indexOf(ndItem);
                item = this.list[index];
                if (item['sec_rec'] && item['sec_rec'].length) {
                    this.showSmartQuery(ndItem, false);
                    if (!ndItem.getData(dataHovered)) {
                        this._sendSmartBoxView(item['sec_rec']);
                        ndItem.setData(dataHovered, true);
                    }
                } else {
                    this.hideSmartQuery();
                }
                this.fire('highlight', item);
            } else {
                this.hideSmartQuery();
            }
        },
        dehighlight: function () {
            if (this.status !== Suggestion.STATUS_SELECTED) return;
            this.ndContainer.all('.' + Suggestion.CLASS_ITEM).removeClass(Suggestion.CLASS_HOVER);
            this.hideSmartQuery();
            this.status = Suggestion.STATUS_SHOW;
        },
        highlightVertical: function (direction) {
            var instance = this,
                status = instance.status,
                nlItem = instance.ndContainer.all('> li'),
                list = instance.list,
                validIndexList = [],
                len,
                index,
                itemData;

            Y.each(instance.list, function (item, index) {
                if (item.type === 'suggestion' || item.type === 'history') {
                    validIndexList.push(index);
                }
            });
            len = validIndexList.length;
            if (status === Suggestion.STATUS_HIDDEN) {
                instance.show();
            }
            if (status === Suggestion.STATUS_SHOW) {
                index = validIndexList[direction === 'up' ? len - 1 : 0];
                this.highlight(nlItem.item(index));
                itemData = list[index];
            } else {
                index = this.getHighlightedIndex();
                if (direction === 'up') {
                    if (index === validIndexList[0]) {
                        this.dehighlight();
                        itemData = {
                            type: 'origin',
                            word: this.originKeyword
                        };
                    } else {
                        index = validIndexList[Y.Array.indexOf(validIndexList, index) - 1];
                        this.highlight(nlItem.item(index));
                        itemData = list[index];
                    }
                } else {
                    if (index === validIndexList[len - 1]) {
                        this.dehighlight();
                        itemData = {
                            type: 'origin',
                            word: this.originKeyword
                        };
                    } else {
                        index = validIndexList[Y.Array.indexOf(validIndexList, index) + 1];
                        this.highlight(nlItem.item(index));
                        itemData = list[index];
                    }
                }
            }
            this.fire('updateKeyword', itemData);
        },
        getHighlightedIndex: function () {
            if (this.status !== Suggestion.STATUS_SELECTED) return -1;
            var ndItem = this.ndContainer.one('.' + Suggestion.CLASS_HOVER);
            if (!ndItem) return -1;
            return this.indexOf(ndItem);
        },
        showSmartQuery: function (ndItem, isPanelTop) {
            var ndHtml = ndItem.one('.' + Suggestion.CLASS_SMART_QUERY_HTML),
                ndContainer = this.ndContainer,
                ndPanel = this.ndPanel,
                ndSmartQuery = ndPanel.one('.' + Suggestion.CLASS_SMART_QUERY),
                regionContainer,
                regionItem,
                regionPanel,
                regionSmartQuery,
                leftHeight,
                deltaHeight;

            ndSmartQuery.setStyle('marginTop', 0)
                .setHTML(ndHtml.getHTML());

            regionItem = ndItem.get('region');
            regionContainer = ndContainer.get('region');
            ndPanel.setStyles({
                visibility: 'hidden',
                display: '',
                height: 'auto'
            }).removeAttribute('hidden');
            regionPanel = ndPanel.get('region');
            if (regionPanel.height > regionContainer.height) {
                ndContainer.setStyle('height', regionPanel.height);
                ndPanel.setStyle('height', regionPanel.height);
                regionContainer = ndContainer.get('region');
            } else {
                ndPanel.setStyle('height', regionContainer.height - 1);
                regionPanel = ndPanel.get('region');
            }
            ndPanel.setStyle('visibility', 'visible');

            regionSmartQuery = ndSmartQuery.get('region');
            leftHeight = regionPanel.height - regionSmartQuery.height;
            deltaHeight = regionItem[1] - regionPanel[1];
            if (isPanelTop) {
                ndSmartQuery.setStyle('marginTop', 0);
            } else {
                ndSmartQuery.setStyle('marginTop', Math.max(Math.min(leftHeight, deltaHeight), 0));
            }
        },
        hideSmartQuery: function () {
            this.ndPanel.hide();
        },
        _sendSmartBoxView: function (list) {
            // 过滤一遍 list 只保留 acms 的数据
            var acms = [];
            Y.Array.each(list, function (item) {
                if (item.acm) {
                    acms.push(item.acm);
                }
            });

            if (acms.length) {
                window._mbq('sendReclsView', acms);

                // PageModuleManager中模块名 "search/suggest"
                window._mbq('trackModuleview', {pm: 'sm'});
            }
        }
    };
    Y.augment(Suggestion, Y.EventTarget);

    /**
     * 主站顶部搜索
     * @class SmartBox
     * @namespace Y.mt.www
     * @static
     */
    Y.mt.www.SmartBox = {
        /**
         * 搜索框初始化
         * @method init
         * @param {Node|HTMLElement|Selector} [container] 父节点选择器
         */
        init: function (container) {
            var ndContainer = Y.one(container) || Y.one('.site-mast'),
                ndForm = ndContainer.one('.J-search-form');
            if (!ndForm) return;

            var ndKeyword = $Util.field(ndForm, 'w'),
                ndSuggestion = ndContainer.one('.J-search-suggest'),
                ndSearchTabContainer = ndContainer.one('.search-box__tabs-container'),
                nlSearchTab = ndContainer.all('.search-box__tab'),
                ndSearchTabDeal = ndContainer.one('.J-search-box__tab--deal'),
                ndSearchTabShop = ndContainer.one('.J-search-box__tab--shops'),
                requestAction = ndKeyword.getData('smartbox'),
                hasShopSearch = nlSearchTab.size() > 0,
                searchHistory,
                searchData,
                suggestion,
                mttcode,
                keyword,
                isLocked = false,   // 如果是 js 更改的 value，则不处理 valuechange 事件
                blurTimer,
                DELAY_GA = 200,
                CLASS_TAB_CONTAINER_OVER = 'search-box__tabs-container--over',
                CLASS_TAB_CURRENT = 'search-box__tab--current';

            isShopSearch = ndForm.getAttribute('action') === '/shops/';
            searchHistory = new SearchHistory();
            searchData = new SearchData(requestAction);
            suggestion = new Suggestion(ndSuggestion);
            keyword = getKeyword();
            searchData.originKeyword = keyword;
            suggestion.originKeyword = keyword;
            ndKeyword.plug(Placeholder);

            updateTrackingCode();
            bind();

            function bind() {
                ndForm.on('submit', function (e) {
                    e.halt();
                    var val = getKeyword();
                    if (val) {
                        updateTrackingCode();
                        submit(val);
                    } else if (isShopSearch) {
                        location.href = '/shops/';
                    }
                });
                ndKeyword.on({
                    valuechange: handleValueChange,
                    click: handleValueChange,
                    blur: function () {
                        blurTimer = window.setTimeout(function () {
                            suggestion.hide();
                        }, DELAY_GA);
                    },
                    keydown: handleKeydown
                });
                searchHistory.on('remove', function () {
                    ndKeyword.focus();
                });
                suggestion.bind();
                suggestion.on('remove', function (item) {
                    if (blurTimer) {
                        window.clearTimeout(blurTimer);
                        blurTimer = null;
                    }
                    searchHistory.remove(item.word);
                });
                suggestion.on('select', function (item) {
                    updateTrackingCode(item.type);
                    submit(item.word, item.acm);
                });
                suggestion.on('selectSmartQuery', function (item) {
                    updateTrackingCode(item.type);
                    submit(item.word, item.acm);
                });
                suggestion.on('updateKeyword', function (item) {
                    updateTrackingCode(item.type);
                    ndKeyword.set('value', item.word);
                });

                if (hasShopSearch) {
                    ndSearchTabContainer.on('hover', function () {
                        this.addClass(CLASS_TAB_CONTAINER_OVER);
                    }, function () {
                        this.removeClass(CLASS_TAB_CONTAINER_OVER);
                    });
                    ndSearchTabContainer.delegate('click', function () {
                        if (this.hasClass(CLASS_TAB_CURRENT)) return;
                        var type = this.hasClass('J-search-box__tab--deal') ? 'deal' : 'shop';
                        switchSearchTab(type);
                        ndSearchTabContainer.removeClass(CLASS_TAB_CONTAINER_OVER);
                    }, '.search-box__tab');
                }
            }

            function getKeyword() {
                return trim(ndKeyword.get('value')).replace(/\s{2,}/g, ' ');
            }

            function submit(kw, acm) {
                acm = acm || '';
                var trackConfig = {
                    acm: acm
                };
                searchHistory.add(getKeyword(), kw);

                ndKeyword.set('value', kw);
                Tracker.appendFormTrackingFields(ndForm, trackConfig);

                // 阻止表单提交，让链接跳转
                if (!suggestion.preventSubmission) {
                    window.setTimeout(function () {
                        ndForm.submit();
                    }, DELAY_GA);
                }

                trackSearchSubmit(kw, acm);
                // 处理站内链接跟踪
                if (!Tracker.config) {
                    Tracker.applyConf();
                }
                Tracker.initTrackingSession(mttcode, Tracker.EVENT_INI_PV);
            }

            function updateTrackingCode(type) {
                var trackParamPrefix = isShopSearch ? 'poi' : '';
                switch (type) {
                case 'history':
                    mttcode = 'A' + trackParamPrefix + 'search.Bhistory';
                    break;
                case 'suggestion':
                    mttcode = 'A' + trackParamPrefix + 'search.Bsmartbox';
                    break;
                case 'smartquery':
                    mttcode = 'A' + trackParamPrefix + 'search.Bsmartquery';
                    break;
                default:
                    mttcode = 'A' + trackParamPrefix + 'search.Bmaintop';
                    break;
                }
            }

            function trackSearchSubmit(kw, acm) {
                // PageModuleManager中模块名 "search/suggest"
                var moduleID = 'sr';
                if (acm) {
                    window._mbq('sendReclsClick', acm);
                    // PageModuleManager中模块名 "search/suggest"
                    moduleID = 'sm';
                }

                window._mbq('localEventlog', 'UserAction', 'Click', 'module/click', {pm: moduleID});
                Tracker.sendMttClickLog(moduleID);
            }

            function switchSearchTab(type) {
                var kw = ndKeyword.get('value'),
                    ndDealHotQuery = Y.one('#J-deal-hot-query'),
                    ndShopHotQuery = Y.one('#J-poi-hot-query'),
                    placeholder = ndKeyword.getAttribute('placeholder');
                if (kw !== '' && kw !== placeholder) {
                    location.href = (type === 'deal' ? '/s/?w=' : '/shops/?w=') + kw;
                    return;
                }
                nlSearchTab.removeClass(CLASS_TAB_CURRENT);
                if (type === 'deal') {
                    isShopSearch = false;
                    ndSearchTabDeal.addClass(CLASS_TAB_CURRENT)
                        .insert(ndSearchTabShop, 'after');
                    ndKeyword.setAttribute('placeholder', '请输入商品名称、地址等');
                    ndForm.setAttribute('action', '/s/');
                    if (ndDealHotQuery) ndDealHotQuery.show();
                    if (ndShopHotQuery) ndShopHotQuery.hide();
                } else {
                    isShopSearch = true;
                    ndSearchTabShop.addClass(CLASS_TAB_CURRENT)
                        .insert(ndSearchTabDeal, 'after');
                    ndKeyword.setAttribute('placeholder', '请输入商家名称、地址等');
                    ndForm.setAttribute('action', '/shops/');
                    if (ndDealHotQuery) ndDealHotQuery.hide();
                    if (ndShopHotQuery) ndShopHotQuery.show();
                }
                // placeholder plug 暂不支持监听 input 的 placeholder 属性 change 事件
                ndKeyword.unplug(Placeholder);
                ndKeyword.plug(Placeholder);
            }

            function handleValueChange() {
                // 不是用户手动输入造成的 valuechange 则不处理
                if (isLocked) return;
                var kw = getKeyword();
                searchData.originKeyword = kw;
                suggestion.originKeyword = kw;
                searchData.fetch(kw, function (data) {
                    if (data) {
                        updateSuggestionList(data);
                    } else {
                        suggestion.destroy();
                    }
                });
            }

            function updateSuggestionList(data) {
                var kw = getKeyword(),
                    historyTips = searchHistory.search(kw),
                    list = [],
                    tipsLen, tip, acm, dataLen, dataItem, listItem;

                // 若输入词为空
                if (kw === '') {
                    searchData.showHistory = true;
                }

                // 如果有电影热词返回，则把'正在热映'放在最先面
                // 否则如果有历史，则把历史放在最先面
                if (searchData.showHistory && historyTips.length) {
                    list.push({
                        type: 'tip',
                        tip: '最近搜过'
                    });

                    tipsLen = historyTips.length;
                    while (tipsLen--) {
                        tip = historyTips[tipsLen];
                        acm = 'AsearchBs_query_history.0.0';
                        dataLen = data.length;

                        while (dataLen--) {
                            dataItem = data[dataLen];
                            if (tip === dataItem.word) {
                                acm = dataItem.acm;
                                break;
                            }
                        }

                        listItem = {
                            type: 'history',
                            word: tip,
                            acm: acm
                        };

                        list.push(listItem);
                    }
                }

                // 然后是缓存或异步后的结果
                Y.each(data, function (tip) {
                    if (Y.Array.indexOf(historyTips, tip.word) !== -1) return;

                    tip.type = 'suggestion';
                    list.push(tip);
                });

                suggestion.show(list);
                ndKeyword.focus();
            }

            function handleKeydown(e) {
                var index, item;
                if (suggestion.status === Suggestion.STATUS_NULL) return;
                switch (e.keyCode) {
                // esc
                case $Macro.key.ESC:
                    ndKeyword.blur();
                    break;
                // up
                case $Macro.key.UP:
                    isLocked = true;
                    suggestion.highlightVertical('up');
                    break;
                // down
                case $Macro.key.DOWN:
                    isLocked = true;
                    suggestion.highlightVertical('down');
                    break;
                // enter
                case $Macro.key.ENTER:
                    isLocked = true;
                    e.halt();
                    index = suggestion.getHighlightedIndex();
                    if (index > -1) {
                        item = suggestion.list[index];
                        submit(getKeyword(), item.acm);
                    } else {
                        submit(getKeyword());
                    }
                    break;
                default:
                    isLocked = false;
                    break;
                }
            }
        }
    };

    Y.mt.ComponentHub.attach('search-box', function () {
        Y.mt.www.SmartBox.init();
    });
},
// version
'1.0.0',
// dependency
{
    requires: [
        'www-base',
        'www-tracker'
    ]
});
;/*jshint white:true, unused:true*/
M.add('cate-nav', function (Y) {
    var $IO = Y.io,
        isIE =  Y.UA.ie,
        $Util = Y.mt.util;

    Y.mt.ComponentHub.attach('cate-nav', function () {
        var ndNavList = this.one('.J-nav__list'),
            cateNavNew = ndNavList.hasClass("cateNavNew"),
            CLS_LOADING = 'dropdown-menu--loading',
            updated = false;

        if (ndNavList.hasClass('J-nav__list--present')) {
        // 一开始展示一级菜单
            // 添加二级菜单容器
            ndNavList.all('.cate-nav__item--has-l2').each(function (nd) {
                var ndLevel2;
                if (cateNavNew) {
                    nd.append('<div class="nav-level2 J-nav-level2 nav-waterfall" style="display:none;"></div>');
                } else {
                    nd.append('<div class="nav-level2 J-nav-level2" style="display:none;"></div>');
                }
                ndLevel2 = nd.one('.J-nav-level2');
                ndLevel2.once('mouseover', addLoading);
                M.load(addLoading);
                function addLoading() {
                    if (!updated) {
                        ndLevel2.addClass(CLS_LOADING);
                    }
                }
            });
            // 绑定展示事件
            bindNavEvent();

            // 异步获取内容，填回二级菜单
            bindAsyncContentEvent(ndNavList, function (data) {
                var ndFrag = Y.Node.create('<div>' + data + '</div>'),
                    nlLevel2 = ndFrag.all('.J-nav-level2');
                updated = true;
                nlLevel2.each(function (nd) {
                    var cateid = nd.getAttribute('data-level1'),
                        ndContainer = ndNavList.one('.J-cate-nav__item--' + cateid),
                        ndLevel2 = ndContainer && ndContainer.one('.J-nav-level2');
                    if (ndLevel2) {
                        ndLevel2.removeClass(CLS_LOADING).setHTML(nd.getHTML());
                    }
                });
                // 返回结果中无相应内容的二级菜单应该删掉
                ndNavList.all('.' + CLS_LOADING).remove();
            });
        } else {
            bindAsyncContentEvent('.J-nav__trigger', function (data) {
                ndNavList.append(data);
                bindNavEvent(true);
            });
        }

        function bindAsyncContentEvent(ndTrigger, cb) {
            var hoverHandler,
                asyncTimer,
                async,
                asyncUrl,
                delay = 200,
                isOnTrigger = false;
            ndTrigger = Y.one(ndTrigger);
            if (!ndTrigger) return;

            hoverHandler = ndTrigger.on('hover', function () {
                isOnTrigger = true;
                if (asyncTimer) {
                    window.clearTimeout(asyncTimer);
                }

                asyncTimer = window.setTimeout(function () {
                    asyncTimer = null;

                    if (!isOnTrigger || async) return;
                    hoverHandler.detach();
                    if (cateNavNew) {
                        asyncUrl = '/index/navcatelistNew';
                    } else {
                        asyncUrl = '/index/navcatelist';
                    }
                    async = $IO(asyncUrl, {
                        method: 'get',
                        on: {
                            success: function (id, o) {
                                var res = $Util.getEvalRes(o);
                                if (res.status && res.data) {
                                    cb(res.data);
                                }
                            }
                        }
                    });
                }, delay);
            }, function () {
                isOnTrigger = false;
            });
        }

        function bindNavEvent(forceShow) {
            var ndTrigger = Y.one('.J-nav__trigger'),
                ndNav = Y.one('.J-nav__list'),
                NAV_ACTIVE = 'nav-unfolded',
                CLASS_HOVER = 'nav-hover',
                CLASS_ACTIVE  = 'nav-active',
                Y_CLS_LEVEL2 = '.J-nav-level2',

                majorDelay = 300,
                minorDelay = 100,

                isAlwaysOn = false,
                isOnTrigger = false,
                isOnNav = false,
                isNavActive = false,

                firstShowTimer = null,
                navHoverTimer = null,
                navItemHoverTimer = null;

            if (!ndTrigger || !ndNav) return;

            if (ndNav.getStyle('display') !== 'none') {
                isAlwaysOn = true;
                initCates();
            }

            if (forceShow) showNav();

            if (!isAlwaysOn) {
                ndTrigger.on('hover', function () {
                    isOnTrigger = true;

                    // 需要停留足够长时间才算是有意hover，而不只是路过
                    window.setTimeout(function () {
                        // 过路而已
                        if (!isOnTrigger) return;

                        // 若此时菜单处于未展开状态，则展开之
                        if (!isNavActive) showNav();
                    }, majorDelay);

                }, function () {
                    isOnTrigger = false;

                    // 需要在外停留足够长时间才算是有意离开，而不是手抖
                    window.setTimeout(function () {
                        // 手抖
                        if (isOnTrigger || isOnNav) return;

                        hideNav();

                    }, majorDelay);
                });
            }

            function initCates() {
                var timer = null,
                    x1 = ndNav.getX() + ndNav.get('offsetWidth'), y1 = ndNav.getY(), y2 = y1 + ndNav.get('offsetHeight'),
                    point1 = { x: x1, y: y1 },
                    point2 = { x: x1, y: y2 },

                    CLASS_NAV_ITEM  = '.J-nav-item',
                    ndDoc = Y.one(document),
                    mouseTimer = null,
                    isMouseHalted = true,
                    cursor = {},
                    pCursor = {},
                    ppCursor = {},
                    pts = [cursor, pCursor, point1, point2],
                    mouseTracker = null;

                navHoverTimer = ndNav.on('hover', function () {
                    isOnNav = true;

                    // 跟踪鼠标位置
                    if (!mouseTracker) {
                        mouseTracker = ndDoc.on('mousemove', function (e) {
                            ppCursor.x = pCursor.x;
                            ppCursor.y = pCursor.y;
                            pCursor.x = cursor.x;
                            pCursor.y = cursor.y;
                            cursor.x = e.pageX;
                            cursor.y = e.pageY;

                            // 处理鼠标停止的情况
                            isMouseHalted = false;
                            window.clearTimeout(mouseTimer);
                            mouseTimer = window.setTimeout(function () {
                                isMouseHalted = true;
                            }, majorDelay);
                        });
                    }

                }, function () {
                    isOnNav = false;

                    if (mouseTracker) {
                        mouseTracker.detach();
                        mouseTracker = null;
                    }

                    // 不用隐藏菜单
                    if (isAlwaysOn) return;

                    // 观察是否手抖，或者跑到trigger上面去了
                    window.setTimeout(function () {
                        // 手抖或在trigger上
                        if (isOnTrigger || isOnNav) return;

                        hideNav();
                    }, majorDelay);
                });

                navItemHoverTimer = ndNav.delegate('hover', over, out, CLASS_NAV_ITEM);

                function over() {
                    var ndItem = this;
                    ndItem.addClass(CLASS_HOVER);

                    if (isOnNav && !ndNav.one('.' + CLASS_ACTIVE)) {
                    // 刚进入导航区，可立即展开二级菜单
                        if (firstShowTimer) {
                            window.clearTimeout(firstShowTimer);
                        }
                        firstShowTimer = window.setTimeout(function () {
                            if (ndItem.hasClass(CLASS_HOVER)) {
                                show(ndItem);
                            }
                            firstShowTimer = null;
                        }, 200);
                    }

                }

                function out() {
                    this.removeClass(CLASS_HOVER);

                    if (timer) return;

                    var ndActive = this;

                    timer = window.setTimeout(check, getDelay());

                    function check() {
                        var ndHover = ndNav.one('.' + CLASS_HOVER);

                        window.clearTimeout(timer);
                        timer = null;

                        // 又回来了
                        if (ndHover === ndActive) return;

                        var delay = getDelay();
                        if (delay > 0) {
                            timer = window.setTimeout(check, delay);
                            return;
                        }

                        if (!ndHover) {
                        // 决策时刻到了，没有回到菜单上
                            window.setTimeout(function () {
                                switchNav();
                            }, majorDelay);
                        } else {
                        // 回到了菜单上，立即切换
                            switchNav();
                        }
                    }
                }

                function getDelay() {
                    if (isMouseHalted) return 0;

                    if (cursor.x === pCursor.x && Math.abs(cursor.y - pCursor.y) < 2 && ppCursor.x && cursor.x > ppCursor.x) return minorDelay;

                    var b0 = (pts[2].x - pts[1].x) * (pts[3].y - pts[1].y) - (pts[3].x - pts[1].x) * (pts[2].y - pts[1].y),
                        b1 = ((pts[2].x - pts[0].x) * (pts[3].y - pts[0].y) - (pts[3].x - pts[0].x) * (pts[2].y - pts[0].y)) / b0,
                        b2 = ((pts[3].x - pts[0].x) * (pts[1].y - pts[0].y) - (pts[1].x - pts[0].x) * (pts[3].y - pts[0].y)) / b0,
                        b3 = ((pts[1].x - pts[0].x) * (pts[2].y - pts[0].y) - (pts[2].x - pts[0].x) * (pts[1].y - pts[0].y)) / b0;
                    // 用外积判断是否在三角形内，b0用于指定外积计算的顺序，如果三个外积的符号一致，表示点在三角形中
                    return b1 > 0 && b2 > 0 && b3 > 0 ? majorDelay : 0;
                }

                // 在不同的一级菜单间切换
                function switchNav() {
                    var ndActive = ndNav.one('.' + CLASS_ACTIVE),
                        ndHover = ndNav.one('.' + CLASS_HOVER);
                    if (ndHover === ndActive) return;
                    if (ndActive) {
                        hide(ndActive);
                    }
                    if (ndHover) {
                        show(ndHover);
                    }
                }

                function show(nd) {
                    var ndL2 = nd.one(Y_CLS_LEVEL2),
                        diff,
                        bottomLim,
                        navTop,
                        err = -10;

                    nd.addClass(CLASS_ACTIVE);
                    if (ndL2) {
                        ndL2.setStyle('visibility', 'hidden');
                        ndL2.show();
                        // 一级菜单top
                        pts[2].y = nd.getY();
                        // 将要设置的二级菜单底部位置
                        pts[3].y = pts[2].y + ndL2.get('offsetHeight');
                        // 整个一级菜单容器底部

                        navTop = ndNav.getY();
                        y2 = navTop + ndNav.get('offsetHeight');
                        // 一级菜单底部到二级菜单底部的距离
                        diff = y2 - pts[3].y;
                        if (diff < 0) {
                        // 需要再往上调一点
                            // 二级菜单底部的上限
                            bottomLim = pts[2].y + nd.get('offsetHeight');
                            diff = y2 + err > bottomLim ? diff + err : diff;
                            if (pts[2].y + diff < navTop) {
                                diff = navTop - pts[2].y;
                            }
                            ndL2.setStyle('top', diff);
                            pts[2].y += diff;
                        } else {
                            ndL2.setStyle('top', 0);
                        }
                        ndL2.setStyle('visibility', 'visible');
                    }
                    // IE9+需要重绘，否则从丽人切到购物时二级菜单显示上内容不会发生变化
                    if (isIE) $Util.reflow(ndNav);
                }

                function hide(nd) {
                    var ndL2 = nd.one(Y_CLS_LEVEL2);
                    nd.removeClass(CLASS_ACTIVE);
                    if (ndL2) {
                        ndL2.hide();
                    }
                }

            }

            // 隐藏整个菜单
            function hideNav() {
                if (isIE === 6) {
                    ndNav.all(Y_CLS_LEVEL2).hide();
                    ndNav.all('.nav-level2-indication').hide();
                }
                ndTrigger.removeClass(NAV_ACTIVE);
                ndNav.hide();
                deCates();
                isNavActive = false;
            }

            // 展开一级菜单
            function showNav() {
                ndTrigger.addClass(NAV_ACTIVE);
                ndNav.show();
                initCates();
                isNavActive = true;
            }

            function deCates() {
                if (navHoverTimer) {
                    navHoverTimer.detach();
                    navHoverTimer = null;
                }
                if (navItemHoverTimer) {
                    navItemHoverTimer.detach();
                    navItemHoverTimer = null;
                }
            }

        }
    });
}, '1.0.0', {
    requires: ['mt-base']
});
;/*jshint white:true, eqeqeq:false, laxbreak:true */
/*global M:true*/

/**
 * an Angular ported parser
 * @module mt-ngparser
 */
M.add('mt-ngparser', function (Y) {

    Y.namespace('mt.ngparser');

    // 两处改动：#a, #b

    var NODE_TYPE_ELEMENT = 1;
    var NODE_TYPE_TEXT = 3;
    var NODE_TYPE_COMMENT = 8;
    var NODE_TYPE_DOCUMENT = 9;
    var NODE_TYPE_DOCUMENT_FRAGMENT = 11;

    function noop(){}
    var lowercase = function(string) {return isString(string) ? string.toLowerCase() : string;};
    var hasOwnProperty = Object.prototype.hasOwnProperty;
    var toString = Object.prototype.toString;

    /**
     * @ngdoc function
     * @name angular.uppercase
     * @module ng
     * @kind function
     *
     * @description Converts the specified string to uppercase.
     * @param {string} string String to be converted to uppercase.
     * @returns {string} Uppercased string.
     */
    var uppercase = function(string) {return isString(string) ? string.toUpperCase() : string;};


    var manualLowercase = function(s) {
        /* jshint bitwise: false */
        return isString(s)
            ? s.replace(/[A-Z]/g, function(ch) {return String.fromCharCode(ch.charCodeAt(0) | 32);})
            : s;
    };
    var manualUppercase = function(s) {
        /* jshint bitwise: false */
        return isString(s)
            ? s.replace(/[a-z]/g, function(ch) {return String.fromCharCode(ch.charCodeAt(0) & ~32);})
            : s;
    };
    function toJson(obj, pretty) {
        if (typeof obj === 'undefined') return undefined;
        return JSON.stringify(obj, toJsonReplacer, pretty ? '  ' : null);
    }

    function toJsonReplacer(key, value) {
        var val = value;

        if (typeof key === 'string' && key.charAt(0) === '$' && key.charAt(1) === '$') {
            val = undefined;
        } else if (isWindow(value)) {
            val = '$WINDOW';
        } else if (value &&  document === value) {
            val = '$DOCUMENT';
        } else if (isScope(value)) {
            val = '$SCOPE';
        }

        return val;
    }


// String#toLowerCase and String#toUpperCase don't produce correct results in browsers with Turkish
// locale, for this reason we need to detect this case and redefine lowercase/uppercase methods
// with correct but slower alternatives.
    if ('i' !== 'I'.toLowerCase()) {
        lowercase = manualLowercase;
        uppercase = manualUppercase;
    }

    function minErr(module, ErrorConstructor) {
        ErrorConstructor = ErrorConstructor || Error;
        return function() {
            var code = arguments[0],
                prefix = '[' + (module ? module + ':' : '') + code + '] ',
                template = arguments[1],
                templateArgs = arguments,
                message, i;

            message = prefix + template.replace(/\{\d+\}/g, function(match) {
                var index = +match.slice(1, -1), arg;

                if (index + 2 < templateArgs.length) {
                    arg = templateArgs[index + 2];
                    if (typeof arg === 'function') {
                        return arg.toString().replace(/ ?\{[\s\S]*$/, '');
                    } else if (typeof arg === 'undefined') {
                        return 'undefined';
                    } else if (typeof arg !== 'string') {
                        return toJson(arg);
                    }
                    return arg;
                }
                return match;
            });

            return new ErrorConstructor(message);
        };
    }
    var $parseMinErr = minErr('$parse');

    function extend(dst) {
        //var h = dst.$$hashKey;

        for (var i = 1, ii = arguments.length; i < ii; i++) {
            var obj = arguments[i];
            if (obj) {
                var keys = Object.keys(obj);
                for (var j = 0, jj = keys.length; j < jj; j++) {
                    var key = keys[j];
                    dst[key] = obj[key];
                }
            }
        }

        //setHashKey(dst, h);
        return dst;
    }

    /**
     * @ngdoc function
     * @name angular.forEach
     * @module ng
     * @kind function
     *
     * @description
     * Invokes the `iterator` function once for each item in `obj` collection, which can be either an
     * object or an array. The `iterator` function is invoked with `iterator(value, key, obj)`, where `value`
     * is the value of an object property or an array element, `key` is the object property key or
     * array element index and obj is the `obj` itself. Specifying a `context` for the function is optional.
     *
     * It is worth noting that `.forEach` does not iterate over inherited properties because it filters
     * using the `hasOwnProperty` method.
     *
     * Unlike ES262's
     * [Array.prototype.forEach](http://www.ecma-international.org/ecma-262/5.1/#sec-15.4.4.18),
     * Providing 'undefined' or 'null' values for `obj` will not throw a TypeError, but rather just
     * return the value provided.
     *
     ```js
     var values = {name: 'misko', gender: 'male'};
     var log = [];
     angular.forEach(values, function(value, key) {
       this.push(key + ': ' + value);
     }, log);
     expect(log).toEqual(['name: misko', 'gender: male']);
     ```
     *
     * @param {Object|Array} obj Object to iterate over.
     * @param {Function} iterator Iterator function.
     * @param {Object=} context Object to become context (`this`) for the iterator function.
     * @returns {Object|Array} Reference to `obj`.
     */

    function forEach(obj, iterator, context) {
        var key, length;
        if (obj) {
            if (isFunction(obj)) {
                for (key in obj) {
                    // Need to check if hasOwnProperty exists,
                    // as on IE8 the result of querySelectorAll is an object without a hasOwnProperty function
                    if (key != 'prototype' && key != 'length' && key != 'name' && (!obj.hasOwnProperty || obj.hasOwnProperty(key))) {
                        iterator.call(context, obj[key], key, obj);
                    }
                }
            } else if (isArray(obj) || isArrayLike(obj)) {
                var isPrimitive = typeof obj !== 'object';
                for (key = 0, length = obj.length; key < length; key++) {
                    if (isPrimitive || key in obj) {
                        iterator.call(context, obj[key], key, obj);
                    }
                }
            } else if (obj.forEach && obj.forEach !== forEach) {
                obj.forEach(iterator, context, obj);
            } else {
                for (key in obj) {
                    if (obj.hasOwnProperty(key)) {
                        iterator.call(context, obj[key], key, obj);
                    }
                }
            }
        }
        return obj;
    }

    /** Type utitlity **/
    function valueFn(value) {return function() {return value;};}

    /**
     * @ngdoc function
     * @name angular.isUndefined
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is undefined.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is undefined.
     */
    function isUndefined(value) {return typeof value === 'undefined';}


    /**
     * @ngdoc function
     * @name angular.isDefined
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is defined.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is defined.
     */
    function isDefined(value) {return typeof value !== 'undefined';}


    /**
     * @ngdoc function
     * @name angular.isObject
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is an `Object`. Unlike `typeof` in JavaScript, `null`s are not
     * considered to be objects. Note that JavaScript arrays are objects.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is an `Object` but not `null`.
     */
    function isObject(value) {
        // http://jsperf.com/isobject4
        return value !== null && typeof value === 'object';
    }


    /**
     * @ngdoc function
     * @name angular.isString
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is a `String`.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is a `String`.
     */
    function isString(value) {return typeof value === 'string';}


    /**
     * @ngdoc function
     * @name angular.isNumber
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is a `Number`.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is a `Number`.
     */
    function isNumber(value) {return typeof value === 'number';}


    /**
     * @ngdoc function
     * @name angular.isDate
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a value is a date.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is a `Date`.
     */
    function isDate(value) {
        return toString.call(value) === '[object Date]';
    }


    /**
     * @ngdoc function
     * @name angular.isArray
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is an `Array`.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is an `Array`.
     */
    var isArray = Array.isArray;

    /**
     * @ngdoc function
     * @name angular.isFunction
     * @module ng
     * @kind function
     *
     * @description
     * Determines if a reference is a `Function`.
     *
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is a `Function`.
     */
    function isFunction(value) {return typeof value === 'function';}


    /**
     * Determines if a value is a regular expression object.
     *
     * @private
     * @param {*} value Reference to check.
     * @returns {boolean} True if `value` is a `RegExp`.
     */
    function isRegExp(value) {
        return toString.call(value) === '[object RegExp]';
    }


    /**
     * Checks if `obj` is a window object.
     *
     * @private
     * @param {*} obj Object to check
     * @returns {boolean} True if `obj` is a window obj.
     */
    function isWindow(obj) {
        return obj && obj.window === obj;
    }


    function isScope(obj) {
        return obj && obj.$evalAsync && obj.$watch;
    }


    function isFile(obj) {
        return toString.call(obj) === '[object File]';
    }


    function isBlob(obj) {
        return toString.call(obj) === '[object Blob]';
    }


    function isBoolean(value) {
        return typeof value === 'boolean';
    }


    function isPromiseLike(obj) {
        return obj && isFunction(obj.then);
    }
    /**
     * @private
     * @param {*} obj
     * @return {boolean} Returns true if `obj` is an array or array-like object (NodeList, Arguments,
     *                   String ...)
     */
    function isArrayLike(obj) {
        if (obj == null || isWindow(obj)) {
            return false;
        }

        var length = obj.length;

        if (obj.nodeType === NODE_TYPE_ELEMENT && length) {
            return true;
        }

        return isString(obj) || isArray(obj) || length === 0 ||
            typeof length === 'number' && length > 0 && (length - 1) in obj;
    }


    /**
     * Creates a new object without a prototype. This object is useful for lookup without having to
     * guard against prototypically inherited properties via hasOwnProperty.
     *
     * Related micro-benchmarks:
     * - http://jsperf.com/object-create2
     * - http://jsperf.com/proto-map-lookup/2
     * - http://jsperf.com/for-in-vs-object-keys2
     *
     * @returns {Object}
     */
    function createMap() {
        return {}; //Object.create(null);
    }
// Sandboxing Angular Expressions
// ------------------------------
// Angular expressions are generally considered safe because these expressions only have direct
// access to $scope and locals. However, one can obtain the ability to execute arbitrary JS code by
// obtaining a reference to native JS functions such as the Function constructor.
//
// As an example, consider the following Angular expression:
//
//   {}.toString.constructor('alert("evil JS code")')
//
// This sandboxing technique is not perfect and doesn't aim to be. The goal is to prevent exploits
// against the expression language, but not to prevent exploits that were enabled by exposing
// sensitive JavaScript or browser apis on Scope. Exposing such objects on a Scope is never a good
// practice and therefore we are not even trying to protect against interaction with an object
// explicitly exposed in this way.
//
// In general, it is not possible to access a Window object from an angular expression unless a
// window or some DOM object that has a reference to window is published onto a Scope.
// Similarly we prevent invocations of function known to be dangerous, as well as assignments to
// native objects.


    function ensureSafeMemberName(name, fullExpression) {
        if (name === "__defineGetter__" || name === "__defineSetter__"
            || name === "__lookupGetter__" || name === "__lookupSetter__"
            || name === "__proto__") {
            throw $parseMinErr('isecfld',
                    'Attempting to access a disallowed field in Angular expressions! '
                    +'Expression: {0}', fullExpression);
        }
        return name;
    }

    function ensureSafeObject(obj, fullExpression) {
        // nifty check if obj is Function that is fast and works across iframes and other contexts
        if (obj) {
            if (obj.constructor === obj) {
                throw $parseMinErr('isecfn',
                    'Referencing Function in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            } else if (// isWindow(obj)
                obj.window === obj) {
                throw $parseMinErr('isecwindow',
                    'Referencing the Window in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            } else if (// isElement(obj)
                obj.children && (obj.nodeName || (obj.prop && obj.attr && obj.find))) {
                throw $parseMinErr('isecdom',
                    'Referencing DOM nodes in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            } else if (// block Object so that we can't get hold of dangerous Object.* methods
                obj === Object) {
                throw $parseMinErr('isecobj',
                    'Referencing Object in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            }
        }
        return obj;
    }

    var CALL = Function.prototype.call;
    var APPLY = Function.prototype.apply;
    var BIND = Function.prototype.bind;

    function ensureSafeFunction(obj, fullExpression) {
        if (obj) {
            if (obj.constructor === obj) {
                throw $parseMinErr('isecfn',
                    'Referencing Function in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            } else if (obj === CALL || obj === APPLY || obj === BIND) {
                throw $parseMinErr('isecff',
                    'Referencing call, apply or bind in Angular expressions is disallowed! Expression: {0}',
                    fullExpression);
            }
        }
    }

//Keyword constants
    var CONSTANTS = createMap();
    forEach({
        'null': function() { return null; },
        'true': function() { return true; },
        'false': function() { return false; },
        'undefined': function() {}
    }, function(constantGetter, name) {
        constantGetter.constant = constantGetter.literal = constantGetter.sharedGetter = true;
        CONSTANTS[name] = constantGetter;
    });

//Not quite a constant, but can be lex/parsed the same
    CONSTANTS['this'] = function(self) { return self; };
    CONSTANTS['this'].sharedGetter = true;


//Operators - will be wrapped by binaryFn/unaryFn/assignment/filter
    var OPERATORS = extend(createMap(), {
        '+':function(self, locals, a, b) {
            a=a(self, locals); b=b(self, locals);
            if (isDefined(a)) {
                if (isDefined(b)) {
                    return a + b;
                }
                return a;
            }
            return isDefined(b)?b:undefined;},
        '-':function(self, locals, a, b) {
            a=a(self, locals); b=b(self, locals);
            return (isDefined(a)?a:0)-(isDefined(b)?b:0);
        },
        '*':function(self, locals, a, b) {return a(self, locals)*b(self, locals);},
        '/':function(self, locals, a, b) {return a(self, locals)/b(self, locals);},
        '%':function(self, locals, a, b) {return a(self, locals)%b(self, locals);},
        '===':function(self, locals, a, b) {return a(self, locals)===b(self, locals);},
        '!==':function(self, locals, a, b) {return a(self, locals)!==b(self, locals);},
        '==':function(self, locals, a, b) {return a(self, locals)==b(self, locals);},
        '!=':function(self, locals, a, b) {return a(self, locals)!=b(self, locals);},
        '<':function(self, locals, a, b) {return a(self, locals)<b(self, locals);},
        '>':function(self, locals, a, b) {return a(self, locals)>b(self, locals);},
        '<=':function(self, locals, a, b) {return a(self, locals)<=b(self, locals);},
        '>=':function(self, locals, a, b) {return a(self, locals)>=b(self, locals);},
        '&&':function(self, locals, a, b) {return a(self, locals)&&b(self, locals);},
        '||':function(self, locals, a, b) {return a(self, locals)||b(self, locals);},
        '!':function(self, locals, a) {return !a(self, locals);},

        //Tokenized as operators but parsed as assignment/filters
        '=':true,
        '|':true
    });
    var ESCAPE = {"n":"\n", "f":"\f", "r":"\r", "t":"\t", "v":"\v", "'":"'", '"':'"'};


/////////////////////////////////////////

    /**
     * @constructor
     */
    var Lexer = function(options) {
        this.options = options;
    };

    Lexer.prototype = {
        constructor: Lexer,

        lex: function(text) {
            this.text = text;
            this.index = 0;
            this.ch = undefined;
            this.tokens = [];

            while (this.index < this.text.length) {
                this.ch = this.text.charAt(this.index);
                if (this.is('"\'')) {
                    this.readString(this.ch);
                } else if (this.isNumber(this.ch) || this.is('.') && this.isNumber(this.peek())) {
                    this.readNumber();
                } else if (this.isIdent(this.ch)) {
                    this.readIdent();
                } else if (this.is('(){}[].,;:?')) {
                    this.tokens.push({
                        index: this.index,
                        text: this.ch
                    });
                    this.index++;
                } else if (this.isWhitespace(this.ch)) {
                    this.index++;
                } else {
                    var ch2 = this.ch + this.peek();
                    var ch3 = ch2 + this.peek(2);
                    var fn = OPERATORS[this.ch];
                    var fn2 = OPERATORS[ch2];
                    var fn3 = OPERATORS[ch3];
                    if (fn3) {
                        this.tokens.push({index: this.index, text: ch3, fn: fn3});
                        this.index += 3;
                    } else if (fn2) {
                        this.tokens.push({index: this.index, text: ch2, fn: fn2});
                        this.index += 2;
                    } else if (fn) {
                        this.tokens.push({
                            index: this.index,
                            text: this.ch,
                            fn: fn
                        });
                        this.index += 1;
                    } else {
                        this.throwError('Unexpected next character ', this.index, this.index + 1);
                    }
                }
            }
            return this.tokens;
        },

        is: function(chars) {
            return chars.indexOf(this.ch) !== -1;
        },

        peek: function(i) {
            var num = i || 1;
            return (this.index + num < this.text.length) ? this.text.charAt(this.index + num) : false;
        },

        isNumber: function(ch) {
            return ('0' <= ch && ch <= '9');
        },

        isWhitespace: function(ch) {
            // IE treats non-breaking space as \u00A0
            return (ch === ' ' || ch === '\r' || ch === '\t' ||
                ch === '\n' || ch === '\v' || ch === '\u00A0');
        },

        isIdent: function(ch) {
            return ('a' <= ch && ch <= 'z' ||
                'A' <= ch && ch <= 'Z' ||
                '_' === ch || ch === '$');
        },

        isExpOperator: function(ch) {
            return (ch === '-' || ch === '+' || this.isNumber(ch));
        },

        throwError: function(error, start, end) {
            end = end || this.index;
            var colStr = (isDefined(start)
                ? 's ' + start +  '-' + this.index + ' [' + this.text.substring(start, end) + ']'
                : ' ' + end);
            throw $parseMinErr('lexerr', 'Lexer Error: {0} at column{1} in expression [{2}].',
                error, colStr, this.text);
        },

        readNumber: function() {
            var number = '';
            var start = this.index;
            while (this.index < this.text.length) {
                var ch = lowercase(this.text.charAt(this.index));
                if (ch == '.' || this.isNumber(ch)) {
                    number += ch;
                } else {
                    var peekCh = this.peek();
                    if (ch == 'e' && this.isExpOperator(peekCh)) {
                        number += ch;
                    } else if (this.isExpOperator(ch) &&
                        peekCh && this.isNumber(peekCh) &&
                        number.charAt(number.length - 1) == 'e') {
                        number += ch;
                    } else if (this.isExpOperator(ch) &&
                        (!peekCh || !this.isNumber(peekCh)) &&
                        number.charAt(number.length - 1) == 'e') {
                        this.throwError('Invalid exponent');
                    } else {
                        break;
                    }
                }
                this.index++;
            }
            number = 1 * number;
            this.tokens.push({
                index: start,
                text: number,
                constant: true,
                fn: function() { return number; }
            });
        },

        readIdent: function() {
            var expression = this.text;

            var ident = '';
            var start = this.index;

            var lastDot, peekIndex, methodName, ch;

            while (this.index < this.text.length) {
                ch = this.text.charAt(this.index);
                if (ch === '.' || this.isIdent(ch) || this.isNumber(ch)) {
                    if (ch === '.') lastDot = this.index;
                    ident += ch;
                } else {
                    break;
                }
                this.index++;
            }

            //check if the identifier ends with . and if so move back one char
            if (lastDot && ident[ident.length - 1] === '.') {
                this.index--;
                ident = ident.slice(0, -1);
                lastDot = ident.lastIndexOf('.');
                if (lastDot === -1) {
                    lastDot = undefined;
                }
            }

            //check if this is not a method invocation and if it is back out to last dot
            if (lastDot) {
                peekIndex = this.index;
                while (peekIndex < this.text.length) {
                    ch = this.text.charAt(peekIndex);
                    if (ch === '(') {
                        methodName = ident.substr(lastDot - start + 1);
                        ident = ident.substr(0, lastDot - start);
                        this.index = peekIndex;
                        break;
                    }
                    if (this.isWhitespace(ch)) {
                        peekIndex++;
                    } else {
                        break;
                    }
                }
            }

            this.tokens.push({
                index: start,
                text: ident,
                fn: CONSTANTS[ident] || getterFn(ident, this.options, expression)
            });

            if (methodName) {
                this.tokens.push({
                    index: lastDot,
                    text: '.'
                });
                this.tokens.push({
                    index: lastDot + 1,
                    text: methodName
                });
            }
        },

        readString: function(quote) {
            var start = this.index;
            this.index++;
            var string = '';
            var rawString = quote;
            var escape = false;
            while (this.index < this.text.length) {
                var ch = this.text.charAt(this.index);
                rawString += ch;
                if (escape) {
                    if (ch === 'u') {
                        var hex = this.text.substring(this.index + 1, this.index + 5);
                        if (!hex.match(/[\da-f]{4}/i))
                            this.throwError('Invalid unicode escape [\\u' + hex + ']');
                        this.index += 4;
                        string += String.fromCharCode(parseInt(hex, 16));
                    } else {
                        var rep = ESCAPE[ch];
                        string = string + (rep || ch);
                    }
                    escape = false;
                } else if (ch === '\\') {
                    escape = true;
                } else if (ch === quote) {
                    this.index++;
                    this.tokens.push({
                        index: start,
                        text: rawString,
                        string: string,
                        constant: true,
                        fn: function() { return string; }
                    });
                    return;
                } else {
                    string += ch;
                }
                this.index++;
            }
            this.throwError('Unterminated quote', start);
        }
    };


    function isConstant(exp) {
        return exp.constant;
    }

    /*----------------------------------*/
    /**
     * @constructor
     */
    var Parser = function(lexer, $filter, options) {
        this.lexer = lexer;
        this.$filter = $filter;
        this.options = options;
    };

    Parser.ZERO = extend(function() {
        return 0;
    }, {
        sharedGetter: true,
        constant: true
    });

    Parser.prototype = {
        constructor: Parser,

        parse: function(text) {
            this.text = text;
            this.tokens = this.lexer.lex(text);

            var value = this.statements();

            if (this.tokens.length !== 0) {
                this.throwError('is an unexpected token', this.tokens[0]);
            }

            value.literal = !!value.literal;
            value.constant = !!value.constant;

            return value;
        },

        primary: function() {
            var primary;
            if (this.expect('(')) {
                primary = this.filterChain();
                this.consume(')');
            } else if (this.expect('[')) {
                primary = this.arrayDeclaration();
            } else if (this.expect('{')) {
                primary = this.object();
            } else {
                var token = this.expect();
                primary = token.fn;
                if (!primary) {
                    this.throwError('not a primary expression', token);
                }
                if (token.constant) {
                    primary.constant = true;
                    primary.literal = true;
                }
            }

            var next, context;
            while ((next = this.expect('(', '[', '.'))) {
                if (next.text === '(') {
                    primary = this.functionCall(primary, context);
                    context = null;
                } else if (next.text === '[') {
                    context = primary;
                    primary = this.objectIndex(primary);
                } else if (next.text === '.') {
                    context = primary;
                    primary = this.fieldAccess(primary);
                } else {
                    this.throwError('IMPOSSIBLE');
                }
            }
            return primary;
        },

        throwError: function(msg, token) {
            throw $parseMinErr('syntax',
                'Syntax Error: Token \'{0}\' {1} at column {2} of the expression [{3}] starting at [{4}].',
                token.text, msg, (token.index + 1), this.text, this.text.substring(token.index));
        },

        peekToken: function() {
            if (this.tokens.length === 0)
                throw $parseMinErr('ueoe', 'Unexpected end of expression: {0}', this.text);
            return this.tokens[0];
        },

        peek: function(e1, e2, e3, e4) {
            if (this.tokens.length > 0) {
                var token = this.tokens[0];
                var t = token.text;
                if (t === e1 || t === e2 || t === e3 || t === e4 ||
                    (!e1 && !e2 && !e3 && !e4)) {
                    return token;
                }
            }
            return false;
        },

        expect: function(e1, e2, e3, e4) {
            var token = this.peek(e1, e2, e3, e4);
            if (token) {
                this.tokens.shift();
                return token;
            }
            return false;
        },

        consume: function(e1) {
            if (!this.expect(e1)) {
                this.throwError('is unexpected, expecting [' + e1 + ']', this.peek());
            }
        },

        unaryFn: function(fn, right) {
            return extend(function $parseUnaryFn(self, locals) {
                return fn(self, locals, right);
            }, {
                constant:right.constant,
                inputs: [right]
            });
        },

        binaryFn: function(left, fn, right, isBranching) {
            return extend(function $parseBinaryFn(self, locals) {
                return fn(self, locals, left, right);
            }, {
                constant: left.constant && right.constant,
                inputs: !isBranching && [left, right]
            });
        },

        statements: function() {
            var statements = [];
            while (true) {
                if (this.tokens.length > 0 && !this.peek('}', ')', ';', ']'))
                    statements.push(this.filterChain());
                if (!this.expect(';')) {
                    // optimize for the common case where there is only one statement.
                    // TODO(size): maybe we should not support multiple statements?
                    return (statements.length === 1)
                        ? statements[0]
                        : function $parseStatements(self, locals) {
                        var value;
                        for (var i = 0, ii = statements.length; i < ii; i++) {
                            value = statements[i](self, locals);
                        }
                        return value;
                    };
                }
            }
        },

        filterChain: function() {
            var left = this.expression();
            var token;
            while ((token = this.expect('|'))) {
                left = this.filter(left);
            }
            return left;
        },

        filter: function(inputFn) {
            var token = this.expect();
            var fn = this.$filter(token.text);
            var argsFn;
            var args;

            if (this.peek(':')) {
                argsFn = [];
                args = []; // we can safely reuse the array
                while (this.expect(':')) {
                    argsFn.push(this.expression());
                }
            }

            var inputs = [inputFn].concat(argsFn || []);

            return extend(function $parseFilter(self, locals) {
                var input = inputFn(self, locals);
                if (args) {
                    args[0] = input;

                    var i = argsFn.length;
                    while (i--) {
                        args[i + 1] = argsFn[i](self, locals);
                    }

                    return fn.apply(undefined, args);
                }

                return fn(input);
            }, {
                constant: !fn.$stateful && inputs.every(isConstant),
                inputs: !fn.$stateful && inputs
            });
        },

        expression: function() {
            return this.assignment();
        },

        assignment: function() {
            var left = this.ternary();
            var right;
            var token;
            if ((token = this.expect('='))) {
                if (!left.assign) {
                    this.throwError('implies assignment but [' +
                        this.text.substring(0, token.index) + '] can not be assigned to', token);
                }
                right = this.ternary();
                return extend(function $parseAssignment(scope, locals) {
                    return left.assign(scope, right(scope, locals), locals);
                }, {
                    inputs: [left, right]
                });
            }
            return left;
        },

        ternary: function() {
            var left = this.logicalOR();
            var middle;
            var token;
            if ((token = this.expect('?'))) {
                middle = this.assignment();
                if ((token = this.expect(':'))) {
                    var right = this.assignment();

                    return extend(function $parseTernary(self, locals) {
                        return left(self, locals) ? middle(self, locals) : right(self, locals);
                    }, {
                        constant: left.constant && middle.constant && right.constant
                    });

                } else {
                    this.throwError('expected :', token);
                }
            }

            return left;
        },

        logicalOR: function() {
            var left = this.logicalAND();
            var token;
            while ((token = this.expect('||'))) {
                left = this.binaryFn(left, token.fn, this.logicalAND(), true);
            }
            return left;
        },

        logicalAND: function() {
            var left = this.equality();
            var token;
            if ((token = this.expect('&&'))) {
                left = this.binaryFn(left, token.fn, this.logicalAND(), true);
            }
            return left;
        },

        equality: function() {
            var left = this.relational();
            var token;
            if ((token = this.expect('==','!=','===','!=='))) {
                left = this.binaryFn(left, token.fn, this.equality());
            }
            return left;
        },

        relational: function() {
            var left = this.additive();
            var token;
            if ((token = this.expect('<', '>', '<=', '>='))) {
                left = this.binaryFn(left, token.fn, this.relational());
            }
            return left;
        },

        additive: function() {
            var left = this.multiplicative();
            var token;
            while ((token = this.expect('+','-'))) {
                left = this.binaryFn(left, token.fn, this.multiplicative());
            }
            return left;
        },

        multiplicative: function() {
            var left = this.unary();
            var token;
            while ((token = this.expect('*','/','%'))) {
                left = this.binaryFn(left, token.fn, this.unary());
            }
            return left;
        },

        unary: function() {
            var token;
            if (this.expect('+')) {
                return this.primary();
            } else if ((token = this.expect('-'))) {
                return this.binaryFn(Parser.ZERO, token.fn, this.unary());
            } else if ((token = this.expect('!'))) {
                return this.unaryFn(token.fn, this.unary());
            } else {
                return this.primary();
            }
        },

        fieldAccess: function(object) {
            var expression = this.text;
            var field = this.expect().text;
            var getter = getterFn(field, this.options, expression);

            return extend(function $parseFieldAccess(scope, locals, self) {
                return getter(self || object(scope, locals));
            }, {
                assign: function(scope, value, locals) {
                    var o = object(scope, locals);
                    if (!o) object.assign(scope, o = {});
                    return setter(o, field, value, expression);
                }
            });
        },

        objectIndex: function(obj) {
            var expression = this.text;

            var indexFn = this.expression();
            this.consume(']');

            return extend(function $parseObjectIndex(self, locals) {
                var o = obj(self, locals),
                    i = indexFn(self, locals),
                    v;

                ensureSafeMemberName(i, expression);
                if (!o) return undefined;
                v = ensureSafeObject(o[i], expression);
                return v;
            }, {
                assign: function(self, value, locals) {
                    var key = ensureSafeMemberName(indexFn(self, locals), expression);
                    // prevent overwriting of Function.constructor which would break ensureSafeObject check
                    var o = ensureSafeObject(obj(self, locals), expression);
                    if (!o) obj.assign(self, o = {});
                    /* jshint ignore:start */
                    return o[key] = value;
                    /* jshint ignore:end */
                }
            });
        },

        functionCall: function(fnGetter, contextGetter) {
            var argsFn = [];
            if (this.peekToken().text !== ')') {
                do {
                    argsFn.push(this.expression());
                } while (this.expect(','));
            }
            this.consume(')');

            var expressionText = this.text;
            // we can safely reuse the array across invocations
            // #b. IE的apply不能接受null
            var args = []; //argsFn.length ? [] : null;

            return function $parseFunctionCall(scope, locals) {
                var context = contextGetter ? contextGetter(scope, locals) : scope;
                var fn = fnGetter(scope, locals, context) || noop;

                if (args) {
                    var i = argsFn.length;
                    while (i--) {
                        args[i] = ensureSafeObject(argsFn[i](scope, locals), expressionText);
                    }
                }

                ensureSafeObject(context, expressionText);
                ensureSafeFunction(fn, expressionText);

                // IE stupidity! (IE doesn't have apply for some native functions)
                var v = fn.apply
                    ? fn.apply(context, args)
                    : fn(args[0], args[1], args[2], args[3], args[4]);

                return ensureSafeObject(v, expressionText);
            };
        },

        // This is used with json array declaration
        arrayDeclaration: function() {
            var elementFns = [];
            if (this.peekToken().text !== ']') {
                do {
                    if (this.peek(']')) {
                        // Support trailing commas per ES5.1.
                        break;
                    }
                    var elementFn = this.expression();
                    elementFns.push(elementFn);
                } while (this.expect(','));
            }
            this.consume(']');

            return extend(function $parseArrayLiteral(self, locals) {
                var array = [];
                for (var i = 0, ii = elementFns.length; i < ii; i++) {
                    array.push(elementFns[i](self, locals));
                }
                return array;
            }, {
                literal: true,
                constant: elementFns.every(isConstant),
                inputs: elementFns
            });
        },

        object: function() {
            var keys = [], valueFns = [];
            if (this.peekToken().text !== '}') {
                do {
                    if (this.peek('}')) {
                        // Support trailing commas per ES5.1.
                        break;
                    }
                    var token = this.expect();
                    keys.push(token.string || token.text);
                    this.consume(':');
                    var value = this.expression();
                    valueFns.push(value);
                } while (this.expect(','));
            }
            this.consume('}');

            return extend(function $parseObjectLiteral(self, locals) {
                var object = {};
                for (var i = 0, ii = valueFns.length; i < ii; i++) {
                    object[keys[i]] = valueFns[i](self, locals);
                }
                return object;
            }, {
                literal: true,
                constant: valueFns.every(isConstant),
                inputs: valueFns
            });
        }
    };

//////////////////////////////////////////////////
// Parser helper functions
//////////////////////////////////////////////////

    function setter(obj, path, setValue, fullExp) {
        ensureSafeObject(obj, fullExp);

        var element = path.split('.'), key;
        for (var i = 0; element.length > 1; i++) {
            key = ensureSafeMemberName(element.shift(), fullExp);
            var propertyObj = ensureSafeObject(obj[key], fullExp);
            if (!propertyObj) {
                propertyObj = {};
                obj[key] = propertyObj;
            }
            obj = propertyObj;
        }
        key = ensureSafeMemberName(element.shift(), fullExp);
        ensureSafeObject(obj[key], fullExp);
        obj[key] = setValue;
        return setValue;
    }

    var getterFnCacheDefault = createMap();
    var getterFnCacheExpensive = createMap();

    function isPossiblyDangerousMemberName(name) {
        return name == 'constructor';
    }
    function identity($) {return $;}

    /**
     * Implementation of the "Black Hole" variant from:
     * - http://jsperf.com/angularjs-parse-getter/4
     * - http://jsperf.com/path-evaluation-simplified/7
     */
    function cspSafeGetterFn(key0, key1, key2, key3, key4, fullExp, expensiveChecks) {
        ensureSafeMemberName(key0, fullExp);
        ensureSafeMemberName(key1, fullExp);
        ensureSafeMemberName(key2, fullExp);
        ensureSafeMemberName(key3, fullExp);
        ensureSafeMemberName(key4, fullExp);
        var eso = function(o) {
            return ensureSafeObject(o, fullExp);
        };
        var eso0 = (expensiveChecks || isPossiblyDangerousMemberName(key0)) ? eso : identity;
        var eso1 = (expensiveChecks || isPossiblyDangerousMemberName(key1)) ? eso : identity;
        var eso2 = (expensiveChecks || isPossiblyDangerousMemberName(key2)) ? eso : identity;
        var eso3 = (expensiveChecks || isPossiblyDangerousMemberName(key3)) ? eso : identity;
        var eso4 = (expensiveChecks || isPossiblyDangerousMemberName(key4)) ? eso : identity;

        return function cspSafeGetter(scope, locals) {
            var pathVal = (locals && locals.hasOwnProperty(key0)) ? locals : scope;

            if (pathVal == null) return pathVal;
            pathVal = eso0(pathVal[key0]);

            if (!key1) return pathVal;
            if (pathVal == null) return undefined;
            pathVal = eso1(pathVal[key1]);

            if (!key2) return pathVal;
            if (pathVal == null) return undefined;
            pathVal = eso2(pathVal[key2]);

            if (!key3) return pathVal;
            if (pathVal == null) return undefined;
            pathVal = eso3(pathVal[key3]);

            if (!key4) return pathVal;
            if (pathVal == null) return undefined;
            pathVal = eso4(pathVal[key4]);

            return pathVal;
        };
    }

    function getterFnWithEnsureSafeObject(fn, fullExpression) {
        return function(s, l) {
            return fn(s, l, ensureSafeObject, fullExpression);
        };
    }

    function getterFn(path, options, fullExp) {
        var expensiveChecks = options.expensiveChecks;
        var getterFnCache = (expensiveChecks ? getterFnCacheExpensive : getterFnCacheDefault);
        var fn = getterFnCache[path];
        if (fn) return fn;


        var pathKeys = path.split('.'),
            pathKeysLength = pathKeys.length;

        // http://jsperf.com/angularjs-parse-getter/6
        if (options.csp) {
            if (pathKeysLength < 6) {
                fn = cspSafeGetterFn(pathKeys[0], pathKeys[1], pathKeys[2], pathKeys[3], pathKeys[4], fullExp, expensiveChecks);
            } else {
                fn = function cspSafeGetter(scope, locals) {
                    var i = 0, val;
                    do {
                        val = cspSafeGetterFn(pathKeys[i++], pathKeys[i++], pathKeys[i++], pathKeys[i++],
                            pathKeys[i++], fullExp, expensiveChecks)(scope, locals);

                        locals = undefined; // clear after first iteration
                        scope = val;
                    } while (i < pathKeysLength);
                    return val;
                };
            }
        } else {
            var code = '';
            if (expensiveChecks) {
                code += 's = eso(s, fe);\nl = eso(l, fe);\n';
            }
            var needsEnsureSafeObject = expensiveChecks;
            forEach(pathKeys, function(key, index) {
                ensureSafeMemberName(key, fullExp);
                var lookupJs = (index
                    // we simply dereference 's' on any .dot notation
                    ? 's'
                    // but if we are first then we check locals first, and if so read it first
                    // #a. IE不接受”点号保留字“,故取一切为['$key']
                    : '((l&&l.hasOwnProperty("' + key + '"))?l:s)') + ('["' + key +'"]');
                if (expensiveChecks || isPossiblyDangerousMemberName(key)) {
                    lookupJs = 'eso(' + lookupJs + ', fe)';
                    needsEnsureSafeObject = true;
                }
                code += 'if(s == null) return undefined;\n' +
                    's=' + lookupJs + ';\n';
            });
            code += 'return s;';

            /* jshint -W054 */
            var evaledFnGetter = new Function('s', 'l', 'eso', 'fe', code); // s=scope, l=locals, eso=ensureSafeObject
            /* jshint +W054 */
            evaledFnGetter.toString = valueFn(code);
            if (needsEnsureSafeObject) {
                evaledFnGetter = getterFnWithEnsureSafeObject(evaledFnGetter, fullExp);
            }
            fn = evaledFnGetter;
        }

        fn.sharedGetter = true;
        fn.assign = function(self, value) {
            return setter(self, path, value, path);
        };
        getterFnCache[path] = fn;
        return fn;
    }

    var objectValueOf = Object.prototype.valueOf;

    function getValueOf(value) {
        return isFunction(value.valueOf) ? value.valueOf() : objectValueOf.call(value);
    }

///////////////////////////////////


    Y.mt.ngparser = new Parser(new Lexer({}), {}, {});

}, '1.0.0', {});

;/*jshint white:true*/
/*global M:true*/

/**
 * 数据scope
 * @module mt-scope
 */
M.add('mt-scope', function (Y) {

    Y.namespace('mt.scope');

    var each = Y.each;
    var win = typeof window !== 'undefined' ? window : {};
    var console = win.console || {};
    var parser = Y.mt.ngparser;
    var noop = function () {
    };

    var isArray = Y.Lang.isArray;
    var isDate = Y.Lang.isDate;
    var isRegExp = Y.Lang.isRegExp;
    var isFunction = Y.Lang.isFunction;

    var ENABLE_LOG = false;
    var instanceCount = 0;
    var watchId = 1;

    var parserCache = {};
    function parse(string) {
        if (!parserCache[string]) {
            parserCache[string] = parser.parse(string);
        }
        return parserCache[string];
    }
    function checkLog() {
        if (ENABLE_LOG || win.ENABLE_SCOPE_LOG) {
            return true;
        }
        return false;
    }

    function log() {
        if (checkLog() && console.log) {
            console.log.apply(console, arguments);
        }
    }

    function dir() {
        if (checkLog() && console.dir) {
            console.dir.apply(console, arguments);
        }
    }

    function isWindow (obj) {
        return obj && obj.document && obj.location && obj.alert && obj.setInterval;
    }

    // deep equal, copied from: https://github.com/angular/angular.js/blob/8d4e3fdd31eabadd87db38aa0590253e14791956/src/Angular.js#L812
    function equals(o1, o2) {
        /* jshint ignore:start */
        if (o1 === o2) return true;
        if (o1 === null || o2 === null) return false;
        if (o1 !== o1 && o2 !== o2) return true; // NaN === NaN
        var t1 = typeof o1, t2 = typeof o2, length, key, keySet;
        if (t1 == t2) {
            if (t1 == 'object') {
                if (isArray(o1)) {
                    if (!isArray(o2)) return false;
                    if ((length = o1.length) == o2.length) {
                        for(key=0; key<length; key++) {
                            if (!equals(o1[key], o2[key])) return false;
                        }
                        return true;
                    }
                } else if (isDate(o1)) {
                    return isDate(o2) && o1.getTime() == o2.getTime();
                } else if (isRegExp(o1) && isRegExp(o2)) {
                    return o1.toString() == o2.toString();
                } else {
                    if (/*isScope(o1) || isScope(o2) ||*/ isWindow(o1) || isWindow(o2) || isArray(o2)) return false;
                    keySet = {};
                    for(key in o1) {
                        if (key.charAt(0) === '$' || isFunction(o1[key])) continue;
                        if (!equals(o1[key], o2[key])) return false;
                        keySet[key] = true;
                    }
                    for(key in o2) {
                        if (!keySet.hasOwnProperty(key) &&
                            key.charAt(0) !== '$' &&
                            o2[key] !== undefined &&
                            !isFunction(o2[key])) return false;
                    }
                    return true;
                }
            }
        }
        /* jshint ignore:end */
        return false;
    }

    function Scope() {
        this._$$watched = [];
        this._$$children = [];
        this._$id = ++instanceCount;
        Scope._instances[this._$id] = this;
    }

    Scope.TTL = 10;
    Scope.DELAY = 10;
    Scope._instances = {};

    var rootScope;
    Scope.getRootInstance  = function() {
        if (rootScope && !rootScope._$destoried) {
            return rootScope;
        }
        rootScope = new Scope();
        return rootScope;
    };

    Scope.getIsolateInstance = function () {
        return new Scope();
    };

    Scope.getInheritInstance = function (parent) {
        function ChildScope() {}
        ChildScope.prototype = parent;
        var scope = new ChildScope();
        Scope.apply(scope);
        parent._$$children.push(scope);
        scope.$parent = parent;
        return scope;
    };

    Scope.getInstance = function () {
        return this.getRootInstance(); // 暂时兼容线上
        //return parent ? this.getInheritInstance(parent) : this.getIsolateInstance();
    };

    Scope.digest = function (force) {
        Scope.getRootInstance()[force ? '$digest' : '$digestIfPending']();
    };

    Scope.prototype = {
        constructor: Scope,
        _$digestAsync: function () {
            var me = this;
            if (Scope._handler) return;
            Scope._handler = setTimeout(function () {
              me.$digest();
            }, Scope.DELAY);
        },
        _clearHanlder: function () {
            clearTimeout(Scope._handler);
            Scope._handler = null;
        },
        /**
         * 收集所有scope的watchers
         * @returns {Array}
         * @private
         */
        _$collectWatchers: function () {
            var watchers = [];
            Y.each(Scope._instances, function(instance){
                watchers = watchers.concat(instance._$$watched);
            });
            return watchers;
        },
        /**
         * 为避免数据死循环，一次性消化所有的scope。
         */
        $digest: function () {
            log("-----[digesting]------");
            var count = 0;

            while (true) {
                // clear handler on every round
                this._clearHanlder();
                var watchers = this._$collectWatchers(); // 可能在watchers中会生成新的scope,故每次loop都要重新收集一遍

                count++;
                if (count > Scope.TTL) {
                    log('[digesting] digest max loop [' + Scope.TTL + ']reached, give up');
                    break;
                }

                var invokeList = [];
                var dirty = false;

                log("[digesting] round #" + count);
                each(watchers, function (watcher) {
                    var oldValue = watcher.value;
                    var newValue = watcher.func();
                    if (!equals(oldValue, newValue)) {
                        watcher.value = Y.clone(newValue, true);
                        dirty = true;
                        invokeList.push({
                            func: watcher.callback,
                            args: [watcher.value, oldValue]
                        });
                    }
                });

                log('[digesting] ' + invokeList.length + " callbacks in queue.");
                if (invokeList.length) {
                    log('they are:');
                }

                each(invokeList, function (invoker) {
                    dir(invoker);
                    try {
                        invoker.func.apply(null, invoker.args || []);
                    } catch (e) {
                        log('error when invoke:');
                        dir(invoker);
                    }
                });

                if (dirty) {
                    log('[digesting] data is dirty, start next digest');
                } else {
                    log('[digesting] data is stable, stop digesting');
                    // clear handler before exit!
                    this._clearHanlder();
                    break;
                }
            }
        },
        /**
         * 测试专用函数。
         * 用于同步消化数据。
         */
        $digestIfPending: function() {
            if (Scope._handler) {
                this.$digest();
            }
        },
        $destory: function () {
            delete Scope._instances[this._$id];
            this._$$watched = [];
            this._$destoried = true;
        },
        $set: function (name, value) {
            var me = this;
            if (name instanceof Object) {
                return each(name, function (val, key) {
                    me[key] = val;
                });
            }

            if (!name) {
                return;
            }

            me[name] = value;
            this._$digestAsync();
        },
        $apply: function () {
            this._$digestAsync();
        },
        $eval: function (expression) {
            if (isFunction(expression)) {
                return expression.call(this);
            }
            return parse(expression)(this);
        },
        $default: function (hash) {
            var scope = this;
            Object.keys(hash).forEach(function(key){
                scope[key] = scope.hasOwnProperty(key) ? scope[key] : hash[key];
            });
        },
        /**
         * 监控一个表达式，如果表达式得到的值有变化，则会响应回调函数。
         * sync为true，则同步表达式的初始值
         * 返回一个函数，用于解除监控。
         * @param expression
         * @param callback
         * @param sync
         * @returns {Function}
         */
        $watch: function (expression, callback, sync) {
            var func = expression;
            var me = this;

            // fast return
            if (!expression) {
                return;
            }

            if (Array.isArray(expression)) {
                return function(cb){
                    expression.forEach(function(expr) {me.$watch(expr, cb);});
                }(callback);
            }

            if (typeof expression === 'string') {
                func = function () {
                   return this.$eval(expression);
                };
            }

            var wid = ++watchId;
            this._$$watched.push({
                watchId: wid,
                scopeId: this._$id,
                value: sync ? this.$eval(expression) : null,
                expression: expression + '', // convert into string
                func: function () {
                    return func.call(me);
                },
                callback: callback || noop
            });
            this._$digestAsync();

            return function() {
                me._$$watched = me._$$watched.filter(function(w) {
                    return w.watchId !== wid;
                });
            };
        }
    };

    Y.mt.scope = Scope;

}, '1.0.0', {requires: ['mt-base', 'mt-ngparser']});

;/*jshint white:true*/
/*global M:true*/

/**
 * 数据validator
 * @module mt-validator
 */
M.add('mt-validator', function (Y) {

    Y.namespace('mt.validator');

    function Validator() {
        this._validations = [];
        this._status = {};
    }

    Validator.prototype = {
        constructor: Validator,
        /**
         * 注册validations.
         *
         * @param {Object|String} group  可选, validation的标识符。
         * @param {Function} fn 对应的回调函数。
         * @return {Function}  返回注销函数。
         */
        register: function (group, fn) {
            if (typeof group === "function") {
                fn = group;
            }

            this._validations.push({
                component: group, // @deprecated
                group: group,
                validateFn: fn
            });

            var me = this;
            return function() {
                me.unregister(group, fn);
            };
        },
        /**
         * 注销。
         * @param group
         * @param fn
         */
        unregister: function (group, fn) {
            this._validations = this._validations.filter(function(tuple) {
               return !(tuple.group === group &&  tuple.validateFn === fn);
            });
        },
        validate: function (fn) {
            var me = this;
            return this._validations.filter(fn || Boolean).every(function(tuple) {
                var res = tuple.validateFn();
                res = (typeof res === 'object') ? res : {value: res};
                me._status = {
                    group: tuple.group,
                    value: res.value,
                    msg: res.msg
                };
                return me._status.value;
            });
        },
        getStatus: function () {
            return this._status;
        }
    };

    /**
     * ArrayOf({
     *   node: HTMLElement|String,
     *   instance: instanceOf(Validator)
     * })
     * @type {Array}
     */
    var instances = [];

    Validator.getInstance = function(form) {
        var node = form._node || form;

        if (!node) {
            throw new Error("please pass a [form instance] or [string] to validator.getInstance..");
        }

        // find if exist
        var filtered = instances.filter(function(tuple) {
            return tuple.node === node;
        });

        if (filtered[0]) {
            return filtered[0].instance;
        }

        var instance = new Validator();
        instances.push({
            node: node,
            instance: instance
        });

        return instance;
    };

    Validator._instances = instances;

    Y.mt.validator = Validator;

}, '1.0.0', {requires: ['mt-base']});

;/*global M*/
/**
 * 通用函数的自动初始化
 * http://wiki.sankuai.com/pages/viewpage.action?pageId=86631029
 *
 * 使用方法：
 * 1. 应该在使用页面顶部声明 M._autoinit = M._autoinit || [];
 * 2. 函数注册 任意时刻 M._autoinit.push(demoFunction);
 * 3. 元素注册，在新增元素的容器内插入 <img src="data:image/gif," onerror="M._autoinit.push(this.parentNode);this.parentNode.removeChild(this);" />
 *    或在对应 JS 中调用 M._autoinit(ndContainer)
 *
 * 如果使用遇到问题，可以查看 wiki 中的文章和评论，或者联系 liujingwei02@meituan.com
 * @module mt-autoinit
 */
M.add('mt-autoinit', function(Y) {
    // 处理 M._autoinit
    (function (autoinit, root) {
        /*jshint curly:false, white:false*/
        "use strict";

        var queue = root[autoinit] || [];

        var isFunction = Y.Lang.isFunction;

        /**
         * if o is an element, passed it as parameter to all workers
         * if o is a function, run it on all current jobs, one job at a time
        **/
        function consume(o) {
            /*jshint validthis:true*/
            var _jobs, _workers;

            o = o || this.jobs;

            if (isFunction(o)) {
                _jobs = this.jobs;
                _workers = [].concat(o);
            }
            else {
                _jobs = [].concat(o);
                _workers = this.workers;
            }

            Y.Array.each(_jobs, function(job) {
                Y.Array.each(_workers, function(worker) {
                    setTimeout(Y.bind(worker, null, job), 0);
                });
            });
        }

        function AutoInitQueue() {
            // elements to be processed
            this.jobs = [];

            // function to run everytime an element is passed in
            this.workers = [];

            // empty the queue
            Y.Array.each(queue, function(o) {
                if (isFunction(o)) this.workers.push(o);
                else this.jobs.push(o);
                // ?? maybe discard elements at this stage
                // because at this time, all elements could be handled
                // by their corresponding JS module
                // so we are not pushing elements into jobs
            }, this);

            queue = null;
            consume.call(this);
        }

        AutoInitQueue.prototype.push = function(o) {
            if (isFunction(o)) this.workers.push(o);
            else this.jobs.push(o);

            consume.call(this, o);
        };

        root[autoinit] = new AutoInitQueue();
    }("_autoinit", M));
}, '1.0.0', {
    requires: [
        'yui-base'
    ]
});
 

;/*jshint white:true*/
/*global M:true*/

/**
 * 简易数据单向绑定机制.
 * @module mt-form-factory
 */
M.add('mt-bind', function (Y) {

    Y.namespace('mt.bind');

    var SCOPE_ATTR = "mt-scope";
    var eachId = 0;
    var parser = Y.mt.ngparser;
    var mCache = {};
    /**
     * 自动找出以$$开头的变量
     * @param expr
     * @returns {string[]}
     */
    function getMagicVars(expr) {
        if (!expr) return [];
        if (mCache[expr]) return mCache[expr];

        var vars = [];
        (expr + '').replace(/\$\$(\w+)/g, function(match, p1) {
            vars.push(p1);
        });
        return vars;
    }

    /**
     * 取obj上的自有属性，防止查找原型链，向未来兼容。
     * 如obj.watch在其它浏览器下是undefined，但FF下会是一个函数。
     * @param obj
     * @param prop
     * @param defaultVal
     * @returns {*}
     */
    function getOwn(obj, prop, defaultVal) {
        if (!obj) return defaultVal;
        if (!obj.hasOwnProperty(prop)) return defaultVal;
        return obj[prop];
    }

    /**
     * 查找表达式里是否有$${name}，如果有，则自动执行$${name} = node.get({name})
     * 例如：将有 scope.$$value = node.get('value')
     *
     * @param expr
     * @param scope
     * @param node
     */
    function handleMagicVars(expr, scope, node) {
        getMagicVars(expr).forEach(function(attName){
            scope['$$' + attName] = node.get(attName);
        });
    }

    /**
     * Example:
     * 只有在scope.selected == 这个option的value时，才是selected.
     * <option value="1" mt-bind-selected="$this.get('value') == selected" />
     * <option value="1" mt-bind-selected="$$value == selected" />
     */
    function wrapExpr(expr, node) {
        var ret = function () {
            this.$this = node;
            handleMagicVars(expr, this, node);
            var r = this.$eval(expr);
            this.$this = null;
            return r;
        };
        ret.toString = function() {return expr;}; // for debug reason
        return ret;
    }

    /**
     * 不适合在页面显示出来的变量。
     * undefined, NaN, null
     */
    function isBadSymbol(val) {
        return (val === void 0 || val !== val || val === null);
    }

    // TODO: remove 'mt-bind-' prefix and use 'mb-' instead.
    var binders = {
        'mt-bind-init': function (node, scope, expr) {
            scope.$eval(expr);
        },
        'mt-bind-html': function (node, scope, expr) {
            scope.$watch(expr, function (val) {
                var v = isBadSymbol(val) ? "" : val;
                node.setHTML(Y.mt.lang.htmlEscape(v + ''));
            });
        },
        'mt-bind-unsafe-html': function (node, scope, expr) {
            scope.$watch(expr, function (val) {
                node.setHTML(val);
            });
        },
        'mt-bind-show': function (node, scope, expr) {
            scope.$watch(expr, function (val) {
                node[val ? 'show' : 'hide']();
            });
        },
        'mt-bind-disabled': function (node, scope, expr) {
            scope.$watch(expr, function(val){
                node.set('disabled', !!val);
            });
        },
        'mt-bind-class': function (node, scope, expr) {
            scope.$watch(expr, function (val) {
                node.set('className',
                    Object
                        .keys(val || {})
                        .filter(function (key) {
                            return val[key];
                        })
                        .join(' ')
                );
            });
        },
        /**
         * mt-bind-value和mt-bind-checked需要特殊关照。
         * 因为用户的输入，点击会改变node.get('value')、
         * node.get('checked')，所以需要在每次$digest
         * 同步value/checked到节点上，保证数据与节点同步。
         */
        'mt-bind-value': function (node, scope, expr) {
            scope.$watch(function() {
                var val = scope.$eval(expr);
                if (String(node.get('value')) !== String(val)) {
                    node.set('value', isBadSymbol(val) ? '' : val);
                }
                // Special logic for select under IE6,7,8,9,10
                if (Y.UA.ie > 0 && node.get('tagName') === "SELECT") {
                    var selectElem = node.getDOMNode();
                    var options = selectElem.options;
                    for (var i = 0; i < options.length; i++) {
                        if (String(options[i].value) === String(val)) {
                            selectElem.selectedIndex = i;
                        }
                    }
                }
            });
        },
        'mt-bind-checked': function (node, scope, expr) {
            scope.$watch(function(){
                var val = scope.$eval(expr);
                if (node.get('checked') !== val) {
                    node.set('checked', val);
                }
            });
        },
        /**
         * mt-bind-defaultvalue
         * 仅仅在初始时赋值。
         */
        'mt-bind-defaultvalue': function(node, scope, expr) {
            var unwatch = scope.$watch(expr, function(val){
                if (!isBadSymbol(val)) {
                    node.set('value', val);
                    unwatch();
                }
            });
        },
        'mt-bind-attr': function (node, scope, expr) {
            scope.$watch(expr, function(val) {
                Y.each(val, function(val, key) {
                   node.setAttribute(key, val);
                });
            });
        },
        'mt-bind-repeat': function (node, scope) {
            function strip(str) {
                return str.trim().replace(/^\(|\)$/, '').trim();
            }
            // parse
            var expression, keyName, valName;
            var origExp = node.getAttribute('mt-bind-repeat') || node.getAttribute('mb-repeat');
            var match = origExp.match(/(.*) in (.*) (track by (.*))/) || origExp.match(/(.*) in (.*)/);
            var trackExp;
            if (match) {
                var pair = match[1].split(',');
                expression = match[2];
                keyName = strip(pair.length > 1 ? pair[0] : "__");
                valName = strip(pair.length > 1 ? pair[1] : pair[0]);
                trackExp = match[4] || '$index';
            } else {
                throw new Error(origExp + ' is not a valid mt-bind-repeat statement. please follow [val in attr]/ [(key, val) in attr] convention.');
            }

            // prepare template
            var id = ++eachId;
            var tmplId = 'data-mt-bind-template-' + id;

            node.removeAttribute('mt-bind-repeat');
            node.removeAttribute('mb-repeat');

            node.setAttribute('data-mt-bind-id', id);
            var html = node.get('outerHTML');

            var templateNode = Y.Node.create(
                    '<script type="text/x-mt-bind-repeat-template" id="$id">/* expression: $expr */\n$html</script>'
                        .replace('$id', tmplId)
                        .replace('$html', html)
                        .replace('$expr', origExp)
            );
            node.insert(templateNode, "after");
            node.remove();

            var idMap = {
                // [trackId] : {scope: Scope, node: Y.Node}
            };
            // track id helper function
            function TempLocal() {}
            TempLocal.prototype = scope;
            var trackFn = parser.parse(trackExp);
            var getId = function(val, index) {
                var local = new TempLocal();
                local[valName] = val;
                local['$index'] = index;
                return trackFn(local);
            };

            // watch changes
            scope.$watch(expression, function(array) {
                // 1. 判断idMap中的元素是否在新的array中存在，如不存在，删除之.
                var toBeDeleteIds = [];
                var newMap = {};
                Y.each(array, function(val, index) {
                    newMap[getId(val, index)] = val;
                });
                Y.each(idMap, function(val, key) {
                    if (!newMap.hasOwnProperty(key)) {
                        toBeDeleteIds[key] = true;
                    }
                });
                Y.each(toBeDeleteIds, function(val, key){
                    idMap[key].scope.$destory();
                    idMap[key].node.remove(true);
                    delete idMap[key];
                });

                // 2. 遍历新的array
                var count = 0;
                var len = Array.isArray(array) ? array.length : Object.keys(array || {}).length;
                var baseNode = templateNode;
                Y.each(array, function(val, key) {
                    var trackId = getId(val, key);
                    var childScope, node;
                    if (idMap[trackId]) {
                        childScope = idMap[trackId].scope;
                        node = idMap[trackId].node;
                    } else {
                        childScope = Y.mt.scope.getInheritInstance(scope);
                        node = Y.Node.create(html);
                    }
                    idMap[trackId] = {scope: childScope, node: node};
                    childScope[valName] = val;
                    childScope[keyName] = key;
                    // magic vars
                    childScope.$set({
                        '$index': key,
                        '$first': count === 0,
                        '$last' : count === len - 1,
                        '$middle': (count > 0 && count < len - 1),
                        '$even' : count % 2 === 0,
                        '$odd'  : count % 2 !== 0
                    });
                    Y.mt.bind.init(node, childScope);
                    if (baseNode.next() !== node) {
                        baseNode.insert(node, 'after');
                    }
                    M._autoinit.push(node); // auto boot the uix-widgets..
                    baseNode = node;

                    count++;
                });
            });

            return false; // 终止其它helper
        },
        /**
         * Example:
         *
         * <input
         *    mt-bind-validate="{
         *               events: 'change|blur',
         *               validation: 'checkLength($this)'
         *               }" />
         *
         *  scope.checkLength = function (node, min, max) {
         *     var value = node.get('value');
         *
         *     var valid = value.length >= min && value.length <= max;
         *     // 设置可设置其它东西
         *     // do something here...
         *
         *     // 遵循mt-validator规范
         *     // 1. 返回true/false, true代表通过
         *     // 2. 返回 {value: boolean, msg: string}，value的值代表是否通过,msg为成功|错误信息
         *     return valid;
         *  }
         *
         *
         * @param node
         * @param scope
         * @param expr
         */
        'mt-bind-validate': function (node, scope, expr) {
            var config = scope.$eval(expr);
            var validation = config.validation;
            if (!validation) {
                throw new Error("The mt-bind-validate config should contain validation field");
            }
            var validate = function () {
                // 如果这个节点不在DOM里，则跳过对其验证
                if (!Y.one(document.body).contains(node)) return true;
                // 如果这个节点为disabled状态，则跳过对其验证
                if (node.get('disabled')) return true;

                scope.$this = node;
                var valid = node.scopeEval(validation);
                scope.$this = null;
                node.removeClass('mt-validate-valid');
                node.removeClass('mt-validate-invalid');
                node.addClass((valid === true || (valid && valid.value === true)) ? "mt-validate-valid" : "mt-validate-invalid");
                scope.$apply();
                return valid;
            };

            scope.$watch(getOwn(config, 'watch'), validate, true);

            // bind to validator
            var ndForm = node.ancestor('form');
            if (ndForm) {
                var validator = Y.mt.validator.getInstance(ndForm);
                validator.register(config.group, validate);
            }

            // bind to events
            (getOwn(config, 'event') || getOwn(config, 'events') || '').split('|').map(function(eventName) {
                return eventName.trim().replace(/^on/, '');
            }).forEach(function(eventName) {
                bindEvent(eventName, node, validate);
            });
        },
        'mt-bind-model': function (node, scope, modelName) {
            function equal(a, b) {
                return String(a) === String(b);
            }

            function contains(arr, val) {
                return !!arr.filter(function (v) {
                    return equal(v, val);
                }).length;
            }

            var strategies = {
                "input#radio": function (node, val) {
                    node.set('checked', equal(node.get('value'), val));
                },
                "input#checkbox": function (node, val) {
                    node.set(
                        'checked',
                        Array.isArray(val) ? contains(val, node.get('value')) : equal(val, node.get('value'))
                    );
                },
                // https://developer.mozilla.org/en-US/docs/Web/API/HTMLSelectElement/type
                "select#select-one": function (node, val) {
                    node.set('value', val);
                },
                "select#select-multiple": function (node, val) {
                    var options = node.get('options');
                    Y.all(options).each(function (ndOption) {
                        ndOption.set(
                            'selected',
                            contains(val, ndOption.get('value'), val)
                        );
                    });
                },
                'default': function (node, val) {
                    if (!equal(node.get('value'), val)) {
                        node.set('value', val);
                    }
                }
            };

            scope.$watch(modelName, function (val) {
                var strategy = strategies[(node.get('tagName') + "#" + node.get('type')).toLowerCase()] || strategies['default'];
                strategy(node, val);
            });
        }
    };

    // use mb- as shortcut..
    Y.each(binders, function (val, key) {
        binders[key.replace('mt-bind-', 'mb-')] = val;
    });

    var priorites = {
        'mt-bind-repeat': 999,
        'mb-repeat': 999,
        '*': 100
    };

    // http://yuilibrary.com/yui/docs/event/#adding-to-the-dom-event-whitelist
    Y.mix(Y.Node.DOM_EVENTS, {
        propertychange: true
    });

    var eventNormalizer = {
        'change': function (node, callback) {
            var eventname = "change";

            // http://blog.csdn.net/huli870715/article/details/7887818
            if (/checkbox|radio/.test(node.get('type')) && (Y.UA.ie > 0 && Y.UA.ie < 9)) {
                node.on('propertychange', function(e) {
                    if (e._event.propertyName === 'checked') {
                        callback(e);
                    }
                });
                return;
            }
            // http://yuilibrary.com/yui/docs/event/valuechange.html
            if (node.get('type') === 'text' || node.get('type') === 'password' || node.get('tagName') === 'TEXTAREA') {
                eventname = 'valuechange';
            }
            node.on(eventname, callback);
        }
    };



    /**
     * General event expression bind helper function
     * @param eventName
     * @param node
     * @param expr
     */
    function bindEvent(eventName, node, expr) {
        var eventCallback = function (e) {
            var scope = node.getScope();
            scope.$event = e;
            scope.$eval(expr);
            scope.$event = null;
            scope.$apply();
        };

        if (eventNormalizer[eventName]) {
            eventNormalizer[eventName](node, eventCallback);
        } else {
            node.on(eventName, eventCallback);
        }
    }

    /**
     * attributes是不区分大小写的，还是在这里指明好了，统一强制转成小写。
     * @param name
     * @returns {*}
     */
    function getBinder(name) {
        return binders[name.toLowerCase()];
    }

    Y.mt.bind = {
        init: function (ele, scope) {
            var root = Y.one(ele);
            if (!root) return;

            if (scope) {
                root._$scope = scope;
            }

            var PAT_BIND_EVENT_ATTR1 = /^mt-bind-on\w+/;
            var PAT_BIND_EVENT_ATTR2 = /^mb-on\w+/;

            traverse(root, function (node) {
                if (node.hasAttribute(SCOPE_ATTR)) {
                    createScopeOnElement(node);
                }

                // 防止多次绑定
                if (node._$binded) return;
                node._$binded = true;

                var attributes = node.get('attributes');
                if (!attributes) return; // 一些情况下attributes为null.

                // 找到需要bind的属性
                var bindAttrs = [];
                attributes.each(function (attr) {
                    var attrName = attr.get('name');
                    if (getBinder(attrName)) {
                        bindAttrs.push({
                            name: attrName,
                            priority: priorites[attrName] || priorites['*']
                        });
                    } else if (PAT_BIND_EVENT_ATTR1.test(attrName) || PAT_BIND_EVENT_ATTR2.test(attrName)) {
                        bindAttrs.push({
                            name: attrName,
                            priority: priorites['*']
                        });
                    }
                });

                // 按优先级排序, 从大到小
                bindAttrs.sort(function(a, b) {
                   return b.priority - a.priority;
                });

                // 绑定，如果其中有一个binder返回了false, 则终止所有剩下的binder
                var stop = false;
                bindAttrs.forEach(function(attr) {
                    if (stop) return;

                    var attrName = attr.name;
                    var expr = wrapExpr(node.getAttribute(attrName), node);
                    // 普通binder
                    if (getBinder(attrName)) {
                        stop = (getBinder(attrName))(node, node.getScope(), expr) === false;
                        return;
                    }

                    // 事件binder
                    var eventName = attrName.indexOf('mt-bind-on') > -1 ? attrName.slice('mt-bind-on'.length) : attrName.slice('mb-on'.length);
                    bindEvent(eventName, node, expr);
                });
            });
        },
        /**
         * register a custom binder.
         * @param name
         * @param callback
         * @param config
         */
        register: function (name, callback, config) {
            name = name.toLowerCase();
            binders[name] = callback;
            config = config || {};
            priorites[name] = config.priority || priorites['*'];
        }
    };

    // init root scope for document.body
    Y.one(document.body)._$scope = Y.mt.scope.getRootInstance();

    function getScope() {
        var node = this;
        while (node) {
            if (node.hasAttribute(SCOPE_ATTR) && !node._$scope) {
                createScopeOnElement(node);
            }
            if (node._$scope) return node._$scope;
            node = node.ancestor();
        }
        return Y.mt.scope.getRootInstance();
    }

    // augment YUI node
    Y.Node.addMethod('getScope', getScope);
    Y.Node.addMethod('scope', getScope);
    // for debug reason
    Y.Node.addMethod('scopeEval', function (element, expr) {
        var node = Y.one(element);
        var scope = node.getScope();
        return scope.$eval(wrapExpr(expr, node));
    });

    /**
     * <div mt-scope></div> ==> 建立一个与父级拥有继承关系的scope
     * <div mt-scope="{}"></div> ==> 建立一个与外界完全独立的scope
     * <div mt-scope="{name: 'nickname'}"></div> ==> 建立一个scope.name 恒等于 outerScope.name的scope
     *
     * @param ele
     */
    function createScopeOnElement(ele) {
        var node = Y.one(ele);
        if (!node || node._$scope) return;
        var isIsolate = node.getAttribute(SCOPE_ATTR) || false;
        var outerScope = node.ancestor() && node.ancestor().getScope();
        node._$scope = isIsolate ?
            Y.mt.scope.getIsolateInstance() :
            Y.mt.scope.getInheritInstance(outerScope);
        if (isIsolate) {
            bindScopeAttributes(node, outerScope);
        }
    }

    /**
     * 内外部单向scope传递数据。
     * 如：
     * <div mt-scope='["name=profile.name","age=profile.age"]'> ... </div>
     * 只要outerScope.profile.name变成'abc'，那么scope.name则会立刻变成'abc'.
     *
     * @param ele
     */
    function bindScopeAttributes(node, outerScope) {
        var scope = node.getScope();
        var scopeConfig = Y.mt.util.decodeJSON(node.getAttribute(SCOPE_ATTR));

        if (!outerScope || !Array.isArray(scopeConfig)) return;

        scopeConfig.forEach(function (val) {
            if (!('' + val).trim()) return;

            // "attr1=attr2" 表示父attr2的改变会同步到子的attr1上
            // "attr1" 表示父attr1的改变会同步到子attr1上
            var pair = [val, val];

            if (val.indexOf('=') > 0) {
                pair = val.split('=');
            }

            var attr1 = pair[0];
            var attr2 = pair[1];

            scope[attr1] = outerScope[attr2];
            outerScope.$watch(attr2, function (value) {
                scope.$set(attr1, value);
            });
        });
    }

    // 遍历节点
    function traverse(root, callback) {
        function dfs(node) {
            if (callback(node) === false) return;

            node.get('children').each(function (ndChild) {
                dfs(ndChild);
            });
        }

        dfs(root);
    }

}, '1.0.0', {requires: ['mt-base', 'mt-scope', 'mt-validator', 'mt-ngparser', 'mt-autoinit']});

;